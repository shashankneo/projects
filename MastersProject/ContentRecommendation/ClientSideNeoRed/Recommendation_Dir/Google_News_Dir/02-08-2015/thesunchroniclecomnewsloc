        //try to get width and height again just in case utl didn't pull in a width and height for the image
        $.each($('#instantGallery .instant-gallery .frame img'),function(){
            //console.log("checking image dimensions");
            //console.log(this);
            
            var $t = $(this);
            var $w = $t.width();
            var $h = $t.height();

            if($w > $h){
                $t.removeClass('neutral').addClass('horizontal');
            }else if($w < $h){
                $t.removeClass('neutral').addClass('vertical');
            };
        });

        
            // assign manual iterations
            $('#instantGallery .instant-gallery .frame').each(function(frameIndex){
                $(this).attr('rel', frameIndex);
            });

            // contruct gallery
            var controls = '#instantGallery .preview-slide-navigator';
            $('#instantGallery .instant-gallery').addClass(uniqueID);
            $('#instantGallery .instant-gallery').cycle({
                fxFn:function(currSlideElement, nextSlideElement, options, afterCalback, forwardFlag){
                   //console.info('transitioning');
                },
                fx:      'fade',
                speed:      0,
                timeout:  0, // ex: set this to 2000 for 2 seconds
                before:onBefore,
                after:onAfter,
                prev:'#instantGallery .gallery-play-controls .prev',
                next:'#instantGallery .gallery-play-controls .next',
                pager:    controls,

                pagerAnchorBuilder: function(idx, slide) {

                    var slideImage = $(slide).find("img").attr('src');
                    var slideOrientation = $(slide).find("img").attr('class');
                    // markup for thumb previews

                    if((typeof(slideImage)!="undefined")){
                        return '<a href="javascript:void()" class="' + slideOrientation + ' preview' + '"><span class="crowd-control"><img src="' + slideImage + '" class="' + slideOrientation + '" align="left" /></span></a>';
                    }
                }
            });

            // stuff to do before the slide transition
            function onBefore(curr, next, opts) {
                // if force reload
                if($('#instantGallery').attr('rel') == 'init' && forceReload){
                    $('#instantGallery .gallery-play-controls .prev,#instantGallery .gallery-play-controls .next').remove(); //kill the slideshow buttons
                    $('#instantGallery .instant-gallery').cycle('destroy'); //kill the slideshow functionality
                    $('#instantGallery .instant-gallery').remove();

                    //refresh page to new url
                    currentURL = String(window.location.href);
                    nextURL = currentURL.replace(/photo\=[0-9]+/gi,'photo=' + $(next).attr('rel') );
                    if(nextURL.indexOf('photo=') == -1){
                        if(nextURL.indexOf('?') == -1){
                           nextURL =  currentURL + '?photo='+ $(next).attr('rel');
                        } else {
                           nextURL =  currentURL + '&photo='+ $(next).attr('rel');
                        }
                    }
                    window.location.href = nextURL;
                }
                // show image
                $('#instantGallery .frame').eq(opts.currSlide).show();

                $('#instantGallery').attr('rel','init');

                var currentSlideHeight = $('#instantGallery .frame').eq(opts.currSlide).children('img').attr('rel');
                $('#instantGallery .instant-gallery').height(currentSlideHeight);
                
                
                //console.log("checking image dimensions");
                //console.log(this);
                
                var $t = $(this);
                var $w = $t.width();
                var $h = $t.height();
                
    
                if($w > $h){
                    $t.removeClass('neutral').addClass('horizontal');
                }else if($w < $h){
                    $t.removeClass('neutral').addClass('vertical');
                };
                
                if(($w!=0) && ($h!=0)){
                    $t.parent().width($w).height($h);
                }
            };

            // stuff to do after the slide transition
            function onAfter(curr, next, opts) {
                $('#instantGallery .gallery-play-controls .prev,#instantGallery .gallery-play-controls .next').show();

                // shows and hides previous and next buttons appropriately
                currentSlideNum = $(next).attr('rel');
                if(currentSlideNum == 0){
                    $('#instantGallery .gallery-play-controls .prev').addClass('inactive');
                } else {
                    $('#instantGallery .gallery-play-controls .prev').removeClass('inactive');
                }

                if(currentSlideNum == opts.slideCount-1){
                    $('#instantGallery .gallery-play-controls .next').removeClass('inactive');
                } else {
                    $('#instantGallery .gallery-play-controls .next').addClass('inactive');
                }
            };

            // cancel default a tag functionality on prev next buttons
            $('.gallery-play-controls .prev','.gallery-play-controls .next').click(function(){
                return false;
            });

            // prev
            $('.gallery-play-controls .prev').click(function(){
            });

            // next
            $('.gallery-play-controls .next').click(function(){
            });

            $('.preview-slide-navigator').append('<div class="clear"/>');
            $('.instant-gallery-app .frame img').each(function(i){

            });
            
        

        $('#instantGallery .blox-loading').remove();
        $('.ribbon-icon').css('top', $('#instantGallery .instant-gallery').position().top - $('#instantGallery .instant-gallery .gallery-play-controls').height() - 1);
        $('.preview-slide-navigator a').click(function() {
                $(this).googlePageviews('http://www.thesunchronicle.com/news/local_news/going-the-distance-for-the-pan-mass-challenge/article_7efd42b2-f299-5b0f-8668-e0be1b69e06f.html');
        });
        
        if($("#stock_photos_gallery").length){
            $("#stock_photos_gallery").find(".crowd-control").each(function(){
                
                
                
            });
        }
    });
    
    
