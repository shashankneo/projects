		var iablength = iabterms.length,
			inflength = informterms.length,
			iabstring = "",
			informstring = "";
		
		
		
		for(i=0; i<iablength; i++)
		{
			iabterms[i] = iabterms[i].replace(/[\&]/g,"and");
			iabterms[i] = iabterms[i].replace(/[^A-Za-z0-9\s]/g,"");
			iabstring += "ciab=" + iabterms[i] + ";";
		}

		for (j=0; j<inflength; j++)
