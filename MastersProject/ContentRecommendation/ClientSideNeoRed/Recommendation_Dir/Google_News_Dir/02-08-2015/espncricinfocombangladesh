    <div class="story-page-wrapper">

        <div class="site-container ad-banner-1280">
			<div class="pencil-ad">

        <div class="slot-usa-leaderboard">
<div class="espni-ad-slot ad-banner" data-slot-type="banner" data-kvpos="top"></div>
