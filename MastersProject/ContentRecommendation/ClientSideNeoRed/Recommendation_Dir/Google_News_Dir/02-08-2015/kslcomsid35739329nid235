<script type="text/ddm-template" id="commentAdTmpl">
    <div class="kslCommentAd">
        <div class="queue_ad" data-ksl-news-dfp-ad-id="commentBlock"></div>
    </div>
</script>

<script>
$(function(){
    var groups = {
        r: parseInt('', 10)
    };

            var kslComments = {
            props: {
                comment_ct: 0,
                html: '',
                baseclass: 'formDivider',
                page: 1,
                                size: 20,
                                prev_lvl: 0,
                loadingNode: false,
                MIN_COMMENTS_PER_AD: 10,
                commentAdClassName: 'kslCommentAd',
                data: {}
            },
            els: {
                commentsContainer: $('#cmt_mn'),
                commentsBubble: $('.comment_bubble'),
                commentsLoadBtn: $('#cmt_top'),
                commentsLoadMoreBtn: $('button#loadMore'),
                commentsContentContainer: $('#commentBoard'),
                tmplCommentAd: $('#commentAdTmpl')
            },
            url: {
                backend: '/resources/comments/comments_backend.php'
            },
            display: function(){
                if (this.props.page == 1) {
                    this.els.commentsContainer.find('#commentBoard').html('<li>Loading...</li>');
                    this.props.loadingNode = true;
                    this.loadMore();
                }

                this.els.commentsContainer.show();

                $('html, body').animate({
                    scrollTop: this.els.commentsContainer.offset().top
                }, 'slow');
            },
            onEachComment: function(index, cmt){
                this.props.comment_ct++;
                this.props.html = '';
                this.props.baseclass = 'formDivider';

                if(cmt.indentLevel > 0) {
                    this.props.baseclass += 'Cd';
                } else {
                    this.props.baseclass += 'Wd';
                }

                if(cmt.admin > 0) {
                    this.props.baseclass += 'a';
                }

                if(cmt.indentLevel == 2) {
                    this.props.baseclass += '2';
                }

                if (groups.r && cmt.deleted == 1) {
                    this.props.baseclass += ' deleted';
                }

                if(this.props.prev_lvl != cmt.indentLevel && cmt.indentLevel > 0) {
                    this.props.baseclass += ' first';
                }

                this.props.prev_lvl = cmt.indentLevel;

                this.props.html += '<a name="comment_' + cmt.id + '" id="comment_' + cmt.id + '"></a>';
                this.props.html += '<li id="comment_' + cmt.id + '" class="' + this.props.baseclass + '">';
                this.props.html += '<div id="cheader_' + cmt.id + '" class="commentHeader">';

                this.props.html += '<span class="userName"><a href="/public/member/home/' + cmt.memberId + '/account" target="_blank">' + cmt.author + '</a></span>';

                if(cmt.admin > 0) {
                    this.props.html += '<span class="admin">KSL Staff Post</span>';
                }

                this.props.html += '<span class="postTime">&nbsp;&middot;&nbsp;' + cmt.age + '</span>';

                if (groups.r){
                    this.props.html += '<span class="del" rel="' + cmt.id + '">X</span>';
                }

                this.props.html += '</div>';
                this.props.html += '<div class="commentV26" id="cmt_' + cmt.id + '"><span>' + cmt.message + '</span>';
                this.props.html += '';
                this.props.html += '<div class="cmt_meta" id="cmt_meta_' + cmt.id + '" style="clear: both; margin-top: 5px; font-size: .9em; color: #777; line-height: 20px;">';

                if(this.props.data['votes'] == null || this.props.data['votes'] == false || typeof(this.props.data['votes']['response']['v']) == 'undefined' || typeof(this.props.data['votes']['response']['v'][cmt.id]) == 'undefined') {
                    vote_ct = '';
                } else {
                    vote_ct = this.props.data['votes']['response']['v'][cmt.id]['like'];
                }

                this.props.html += '<span style="margin-right: 10px;" id="vote_ct_' + cmt.id + '">' + vote_ct + '</span>';
                var clicked = '';

                
                this.props.html +='<span title="vote" class="thumb' + clicked + '" alt="' + cmt.id + ':' + cmt.memberId + '">';
                this.props.html += '<svg width="20" height="20">';
                this.props.html += '<image class="imgthumb" xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/resources/comments/thumbsup'
                    + clicked
                    + '.svg" src="/resources/comments/thumbsup'
                    + clicked
                    + '.png" width="20" height="20" style="width: 20px; height: 20px;">'
                    + '</image>';

                this.props.html += '</svg></span>';

                
                this.props.html += '</div></div></li></a>';

                if (this.props.loadingNode) {
                    this.props.loadingNode = false;
                    this.els.commentsContentContainer.html(this.props.html);
                } else {
                    this.els.commentsContentContainer.append(this.props.html);
                }

                this.els.commentsContentContainer.fadeTo(100, 1.0);

                $('.pagesize').html(this.props.comment_ct);

                if(this.props.comment_ct >= parseInt('131', 10)) {
                    $('#loadMore').hide();
                    this.props.comment_ct = parseInt('131', 10);
                    $('.pagesize').html(this.props.comment_ct);
                }

                $('#bot_add_cmt').show();
            },
            onMoreLoaded: function(data){
                if(data['comments']['status']) {
                    this.props.data = data;
                    $.each(data['comments']['response'], $.proxy(this.onEachComment, this));
                    this.els.commentsLoadMoreBtn.removeAttr('disabled');
                    kslEventBus.emit(kslEventBus.eventTypes.commentsLoaded, {
                        comments: data.comments,
                        page: this.props.page - 1
                    });
                }
            },
            loadMore: function(){
                var args = {
                    sid: parseInt('35739329', 10),
                                        page: this.props.page,
                    size: this.props.size
                                    };

                if (!this.props.loadingNode) {
                    this.els.commentsContentContainer.fadeTo(100, 0.5);
                }

                this.props.page += 1;
                this.els.commentsLoadMoreBtn.attr('disabled', true);
                $.post(this.url.backend, args, $.proxy(this.onMoreLoaded, this));
            },
            loadCommentAd: function(data){
                // this.props.page is incremented before this function is called,
                // so after the first page is loaded, this.props.page === 2.
                var viewCount = this.props.page - 1;
                var $comments = this.els.commentsContentContainer.find('li');
                var insertPoint = this.props.MIN_COMMENTS_PER_AD * viewCount * 2 - this.props.MIN_COMMENTS_PER_AD;

                // insert in the middle of new page <=> |new page| >= this.props.MIN_COMMENTS_PER_AD
                if ($comments.length >= insertPoint) {
                    $comments.eq(insertPoint - 1)
                        .append(this.els.tmplCommentAd.html());
                }

                setTimeout(KSL.news.dfp.loadAds, 100);
            }
        };

        $(kslComments.els.commentsLoadMoreBtn).on('click', $.proxy(kslComments.loadMore, kslComments));
        $(kslComments.els.commentsBubble).on('click', $.proxy(kslComments.display, kslComments));
        $(kslComments.els.commentsLoadBtn).on('click', $.proxy(kslComments.display, kslComments));
        kslEventBus.on(kslEventBus.eventTypes.commentsLoaded, $.proxy(kslComments.loadCommentAd, kslComments));


        $('body').on('click', '.thumb', function() {
            var vals = $(this).attr('alt');
            vals = vals.split(':');
            var msgid = vals[0];
            var owner = vals[1];
                            if($("div[id^=emsg_]").length > 0)
                    $("div[id^=emsg_]").remove();

                newtag = '<div id="emsg_' + msgid + '" class="cerror">You must be logged in to cast your vote. <br /><a href="/public/member/signin?login_forward=%2F%3Fsid%3D35739329%26nid%3D235">login here</a></div>';
                $("#cmt_meta_"+ msgid).before(newtag);
                    });


                $('body').on('mouseenter', '.thumb', function() {
            $(this).find('.voteHoverBox').fadeIn('fast');
        }).on('mouseleave','.buttonBox', function() {
            $(this).find('.voteHoverBox').fadeOut('fast');
        });
    });
</script>

