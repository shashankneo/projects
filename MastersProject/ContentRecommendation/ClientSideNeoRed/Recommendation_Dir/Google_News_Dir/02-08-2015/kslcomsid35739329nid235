<script>
    (function(){
        // 0 = disable skipping, >0 = time before skipping is available. ParseInt dance so (JS <==> PHP) soup is less fragile
        var skipOffsetTime = parseInt('0', 10);

        var VIDEO_PLAYER_WIDTH = '100%';
        var VIDEO_PLAYER_RATIO = '16:9';
        var viewportWidth = $(window).width();

        var isJWActive = true;
        var isYouTubeLoaded = false;

        var $thumbnailAnchor = $('#anchor_pic');
        var $thumbnail = $thumbnailAnchor.find('picture');
        var $loader = $thumbnailAnchor.find('.loader');
        var $photoattr = $thumbnailAnchor.find('.photo-attribution');
        var $playerContainer = $('#jw-container');
        var $playerContainerYt = $('#yt-container');
        var navTop = $('.ddm-menu-container__inner').offset().top;

        var youTubePlayer = null;
        var queuedYouTubeVideoId = '';

        /**
         *
         * @param Object v
         * @returns Object         */
        function getJWConfig(v) {
            var useHTML5 = false;

            var config = {
                file: v.file,
                title: v.title,
                image: v.image,
                width: VIDEO_PLAYER_WIDTH,
                aspectratio: VIDEO_PLAYER_RATIO,
                skin: 'bekle',
                displaytitle: false,
                autostart: v.autoStart || true,
                preferFlash: v.preferFlash || true
            };

            if (v.autoStart === false) {
                config.autostart = false;
            }

                        if (KSL.news.util.isMobile(null)) {
                config.autostart = false;

//                v.showAds = false;
            }

            
            if (v.preferFlash === false) {
                config.preferFlash = false;
            }

            if (v.showWatermark === true) {
                config.logo = {
                    file: '//static.ksl.com/images/ksl_watermark.png',
                    link: 'http://www.ksl.com/',
                    position: 'top-right'
                };
            }

            config.tracks = [{
                file: v.captions,
                label: 'English'
            }];

            if (v.showAds) {
                config.advertising = {};
                config.advertising.client = v.adClient || 'vast';
                config.advertising.companiondiv = {
                    id: 'video-companion-2',
                    width: 300,
                    height: 250
                };

                config.advertising.skipoffset = skipOffsetTime;
                config.advertising.schedule = {};

                if (v.prerollAdTag) {
                    config.advertising.schedule.adbreak1 = {
                        offset: 'pre',
                        tag: v.prerollAdTag
                    };
                }

                if (v.postrollAdTag) {
                    config.advertising.schedule.adbreak2 = {
                        offset: 'post',
                        tag: v.postrollAdTag
                    };
                }
            }

            config.sitecatalyst = {
                mediaName: v.title
            };

            config.sharing = {
                code: v.shareCode,
                link: v.shareLink
            };

            if (v.preferFlash) {
                config.primary = "flash";
            }

            if (useHTML5) {
                config.primary = 'html5';
            }

            return config;
        }

        /**
         *
         * @param video
         * @param shouldPlay
         */
        function loadJWVideo(video, shouldPlay) {
            if (!isJWActive) {
                isJWActive = true;
            }

            $playerContainerYt.hide();
            $('html,body').animate({ scrollTop: navTop }, 'fast');
            // do we need to load the JW player?
            runJW(video, shouldPlay);
        }

        /**
         *
         * @param video
         * @param shouldPlay
         */
        function runJW(video, shouldPlay) {
            var config = getJWConfig(video);
            var isAdBlockEnabled = false;

            var showLoader = true;
            if (navigator.userAgent.indexOf('Firefox') != -1) {
                showLoader = false;
            }

                        var overrideAutoplay = KSL.news.util.isMobile(null);

            if (showLoader) {
                $thumbnail.fadeTo(0, 1.0).show().fadeTo(1000, 0.5);
                $loader.text('loading').show();
                $playerContainer.css({ position: 'absolute', top: -99999 }).show();
            } else {
                $thumbnail.hide();
                $photoattr.hide();
                $playerContainer.show();
            }

            var player = jwplayer('jw-player').setup(config);

            player.onSetupError(function(fallback, message){
                if (isAdBlockEnabled) {
                    $('#video-problems').show();
                }
            });

            player.onAdCompanions(function(event) {
                $('#video-companion-2').show();
            });

            player.onReady(function () {
                if (showLoader) {
                    $thumbnail.fadeTo(100, 0.01, function(){
                        $loader.hide().text('');
                        $playerContainer.css({ position: 'static', top: 0 }).show();
                        $thumbnail.hide();
                        $photoattr.hide();
                    });
                }
            });

            player.onError(function (message) {
            });

            player.onAdError(function (tag, message) {
            });

            player.onAdPlay(function (tag) {
            });

            player.onAdImpression(function (tag) {
            });

            player.onBeforePlay(function () {
            });

            if (!overrideAutoplay) {
                player.play(true);
            }

                    }

        /**
         *
         * @param video
         * @param shouldPlay
         */
        function loadYouTubeVideo(video, shouldPlay) {
            if (isJWActive) {
                isJWActive = false;
            }

            $thumbnail.fadeTo(0, 1.0).show().fadeTo(1000, 0.5);
            $loader.text('loading').show();
            $playerContainer.hide();
            $('html,body').animate({ scrollTop: navTop },'fast');

            if (isYouTubeLoaded) {
                $thumbnail.hide();
                $loader.hide().text('');
                $playerContainerYt.show();
                runYouTube(video.file);
            } else {
                var tag = document.createElement('script');
                var firstScriptTag = $('script')[0];
                tag.src = "https://www.youtube.com/iframe_api";
                firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

                isYouTubeLoaded = true;
                queuedYouTubeVideoId = video.file;
            }
        }

        /**
         *
         */
        function onYouTubeIframeAPIReady() {
            if (queuedYouTubeVideoId != '') {
                runYouTube(queuedYouTubeVideoId);
                queuedYouTubeVideoId = '';
            }
        }

        /**
         *
         * @param videoId
         */
        function runYouTube(videoId) {
            if (youTubePlayer == null) {
                youTubePlayer = new YT.Player('yt-player', {
                    height: VIDEO_PLAYER_HEIGHT,
                    width: VIDEO_PLAYER_WIDTH,
                    videoId: videoId
                });
            } else {
                youTubePlayer.loadVideoById({ videoId: videoId });
            }
        }

        window.KSL = window.KSL || {};
        KSL.news = KSL.news || {};
        KSL.news.jwPlayer = {
            loadYouTubeVideo: loadYouTubeVideo,
            loadJWVideo: loadJWVideo
        };
    }());
</script>

