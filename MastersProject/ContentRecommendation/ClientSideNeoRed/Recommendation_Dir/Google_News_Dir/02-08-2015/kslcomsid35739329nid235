    <script>
        var kslSocialShare = {
            openPopup: function (url, winName, width, height, x, y) {
                var url = url;
                var width = width || 575;
                var height = height || 255;
                var left = x || (($(window).width() - width) / 2);
                var top = y || (($(window).height() - height) / 3);
                var opts = 'status=1' +
                    ',menubar=no,toolbar=no,resizable=yes,scrollbars=yes' +
                    ',width=' + width +
                    ',height=' + height +
                    ',top=' + top +
                    ',left=' + left;
                if (!url) {
                    throw new Error('Invalid URL')
                }
                window.open(url, winName, opts);
            },
            facebook: function () {
                kslEventBus.emit(kslEventBus.eventTypes.socialShare, { type: 'facebook' });
                FB.ui({
                    method: 'share',
                    href: 'https://www.ksl.com/?sid=35739329'
                }, function (response) {
                });
            },
            twitter: function () {
                kslEventBus.emit(kslEventBus.eventTypes.socialShare, { type: 'twitter' });
                this.openPopup('//twitter.com/share', 'twitterShare');
            },
            googlePlus: function () {
                kslEventBus.emit(kslEventBus.eventTypes.socialShare, { type: 'google+' });
                this.openPopup('https://plus.google.com/share?url=https%3A%2F%2Fwww.ksl.com%2F%3Fsid%3D35739329', 'gpShare', 500);
            },
            /**
             * @param { bool= } sameWindow
             * @returns { bool }
             */
            email: function(sameWindow){
                var url = 'mailto:?' +
                'subject=' + encodeURIComponent('Zimbabwe: American lion killer\'s extradition being sought') +
                '&body=' + encodeURIComponent('https://www.ksl.com/?sid=35739329');

                kslEventBus.emit(kslEventBus.eventTypes.socialShare, { type: 'email' });

                if (sameWindow !== false) {
                    sameWindow = sameWindow || false;
                }

                if (sameWindow) {
                    window.location.href = url;
                } else {
                    this.openPopup(url, 'emailShare', parseInt($(window).width() * 0.75, 10), parseInt($(window).height() * 0.75, 10));
                    return false;
                }
            },
            print: function(){
                kslEventBus.emit(kslEventBus.eventTypes.socialShare, { type: 'print' });
                return true;
            }
        };
    </script>

