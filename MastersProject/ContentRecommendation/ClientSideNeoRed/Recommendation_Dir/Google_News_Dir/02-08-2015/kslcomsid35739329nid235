            var maxPerPage = parseInt('12', 10);
            var totalInGallery = kslPhotoGallery.length;
            var $photoContainer = $('#gallery');
            var $loadMoreBtn = $('#photoGalleryMore');
            var currPage = 0;

            var makeDom = function (data){
                return '' +
                    '<a href="' + data.img + '?filter=ksl/img614"  title="' + data.caption + '">' +
                        '<img data-src="' + data.img + '?filter=ksl/img143" border="0"  class="lazyload" />' +
                        '<!--[if lt IE 9]>' +
                            '<img src="' + data.img + '?filter=ksl/img143" />' +
                        '<![endif]-->' +
                    '</a>';
            };

            $loadMoreBtn.on('click', function(){
                var totalShown = $photoContainer.find('a').length;
                if (totalShown < totalInGallery) {
                    // batch all new images into a single DOM mod
                    var html = '';

                    $(kslPhotoGallery.slice(totalShown, totalShown + maxPerPage))
                        .each(function (index, photo) {
                            html += makeDom(photo);
                        }
                    );

                    $photoContainer.append(html);
                }

                if ($photoContainer.find('a').length >= totalInGallery) {
                    $(this).parent().hide();
                }

                if (viewportWidth >= KSL.news.ui.breakPoints.DESKTOP_WIDTH_MIN) {
                    updateGalleryModal();
                }

                kslEventBus.emit(kslEventBus.eventTypes.inArticlePhotoWall_loadMore, { page: ++currPage });
            });
        });

        // --- in-article ad ---
        $(function inlineAd(){
            /**
             * @type { KSL.EventBus } kslEventBus
             */
            kslEventBus.emit(kslEventBus.eventTypes.adMounted + ':articleWordCount');

            kslEventBus.on(kslEventBus.eventTypes.adMounted, function ($adEl){
                if($adEl.attr('data-ksl-news-dfp-ad-id') === 'articleWordCount') {
                    var $words = $('#kslMainArticle').find('p');
                    var WORD_COUNT = $words.text().split(/\s+/g).length;
                    var VIEWPORT_WIDTH = $(window).width();
                    var DESKTOP_WORD_COUNT_THRESHOLD = 350;
                    var DESKTOP_WORD_COUNT_INSERT = 150;

                    // Is phone
                    if (VIEWPORT_WIDTH <= KSL.news.ui.breakPoints.PHONE_WIDTH_MAX) {
                        $words.eq(2).before($adEl);
                    }

                    // Is desktop
                    else if (viewportWidth >= KSL.news.ui.breakPoints.DESKTOP_WIDTH_MIN) {
                        if (WORD_COUNT < DESKTOP_WORD_COUNT_THRESHOLD) {
                            $('#recommendationsContainer').before($adEl);
                        } else {
                            var totalCount = 0;
                            var lastParg = 0;

                            // Find last paragraph where word count up to that point >= min threshold
                            $words.each(function (index, word){
                                var currCount = $(word).text().split(/\w+/g).length;
                                totalCount += currCount;
                                lastParg = index;

                                // Break out of $.each
                                if (totalCount >= DESKTOP_WORD_COUNT_INSERT) {
                                    return false;
                                }
                            });

                            $($words[lastParg]).after($adEl);
                        }
                    }
                }
            });
        }());

        

        var makeEmbedVideosFluid = {
            go: function(selectors, fluidClassName){
                $(selectors)
                    .map(function (index, iframe) {
                        var $targetEl = $(iframe);
                        var src = $targetEl.attr('src');
                        var $wrapperEl = $targetEl.parent();
                        var totalChildren = $wrapperEl.children().length;

                        if (src.match(/youtube.com/)) {
                            if (totalChildren === 1) {
                                $wrapperEl.addClass(fluidClassName);
                                return true;
                            } else {
                                $targetEl.wrap('<div class="' + fluidClassName + '"></div>');
                                return true;
                            }
                        }
                    });

                $('.' + fluidClassName).fitVids();
            }
        };

        setTimeout(function(){
            makeEmbedVideosFluid.go('#kslMainArticle iframe', 'fluidEmbed');
        }, 0);
    });
</script>
