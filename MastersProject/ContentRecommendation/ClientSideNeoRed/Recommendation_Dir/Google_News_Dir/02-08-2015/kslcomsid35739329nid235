<!-- @font-face monitor: loads default font then adds class once web font available    -->

<script src="/beta/bower_components/fontfaceobserver/fontfaceobserver.js"></script>

<script>
    var fontA = new FontFaceObserver('raleway', {
        weight: 300
    });

    var fontB = new FontFaceObserver('raleway', {
        weight: 400
    });

    var fontC = new FontFaceObserver('raleway', {
        weight: 700
    });

    // check whether or not web font loads and set cookie
    Promise.all([
        fontA.check(),
        fontB.check(),
        fontC.check()
    ]).then(function() {
        document.documentElement.className += ' fonts-loaded';
        var now = new Date();
        var time = now.getTime();
        time += 3600 * 720000;
        now.setTime(time);
        document.cookie = 'fontsloaded=' + 'true' + '; expires=' + now.toUTCString() + '; path=/';
    }, function() {
        document.documentElement.className += ' fonts-unavailable';
    });
</script>

<script>
    $(function evarEvents(){
        /**
         * @typedef { Object } loadMoreStoriesEvent
         * loadMoreStoriesEvent => {
         *      totalPages: { number }
         * }
         */
        kslEventBus.on(kslEventBus.eventTypes.hpLoadMoreStories, function(event){
            s.eVar24 = 'HP Load More ' + event.totalPages;
            s.t();
        });

        /**
         * @typedef { Object } commentsLoadedEvent
         * commentsLoadedEvent => {
         *      page: { number }
         * }
         *
         * If data.page === 1, then the first page of comments was just displayed.
         * When "load more" is clicked for the first time, data.page => 2
         */
        kslEventBus.on(kslEventBus.eventTypes.commentsLoaded, function(event){
            if (event.page === 1) {
                s.eVar25 = 'comment board';
            } else {
                s.eVar25 = 'CB Load More ' + (event.page - 1);
            }

            s.t();
        });

        /**
         * @typedef { Object } inArticlePhotoWall_loadMoreEvent
         * inArticlePhotoWall_loadMoreEvent => {
         *      page: { number }
         * }
         */
        kslEventBus.on(kslEventBus.eventTypes.inArticlePhotoWall_loadMore, function(event){
            s.eVar44 = 'Wall Page ' + event.page;
            s.t();
        });

        /**
         * @typedef { Object } socialShareEvent
         * socialShareEvent => {
         *      type: [facebook|twitter|google+|email|print]
         * }
         */
        kslEventBus.on(kslEventBus.eventTypes.socialShare, function(event){
            var evar = 'eVar58';
            s.linkTrackVars = evar;
            s[evar] = event.type;
            s.tl(true, 'o', 'Social Media Tracking');
        });
    });

    (function() {
        $(function () {
            $('.ksl-header-menu').on('open.ddm-menu', function () {
                $.cookie('drawerOpen', 'open');
            });

            window.onpageshow = function(event) {
                var ua = navigator.userAgent;
                var match = ua.match(/iPhone|iPad|iPod/i) || '';

                if (match.length > 0 && event.persisted && $.cookie('drawerOpen') === 'open') {
                    $.removeCookie('drawerOpen');
                    window.location.reload();
                }
            };
        });
    }());
</script>

