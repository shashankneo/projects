            <div class="queue_ad nodeContent" data-ksl-news-dfp-ad-id="articleFixed"></div>
        </div>
        <script>
            $(function(){
                var $container = $('.nodeClosable');

                if ($(window).width() > KSL.news.ui.breakPoints.PHONE_WIDTH_MAX) {
                    setTimeout(onAdLoaded, 100);
                    $container.on('click', '.nodeClose', function(){
                        $container.remove();
                    });
                }

                if ($(window).width() >= KSL.news.ui.breakPoints.DESKTOP_WIDTH_MIN) {
                    $container.parents('.bottom_ad').addClass('leaderboard_ad').removeClass('bottom_ad');
                }

                function onAdLoaded(){
                    var isAdMounted = false;
                    try {
                        // Chrome will throw if iframe's contentDocument is on a different domain than host
                        if ($container.find('iframe').contents().find('body').children().length > 0) {
                            isAdMounted = true;
                        }
                    } catch (e){
                        // If ad request returned, but no ad available, container will collapse
                        var adDisplay = $container.find('iframe').parent().css('display');

                        if (adDisplay === 'block') {
                            isAdMounted = true;
                        } else if (adDisplay === 'none') {
                            return;
                        }
                    }

                    if (isAdMounted) {
                        if ($(window).width() < KSL.news.ui.breakPoints.DESKTOP_WIDTH_MIN) {
                            var $ad = $container.find('.nodeContent iframe');
                            var $btn = $container.find('.nodeClose');
                            var css = {
                                top: -4 - $btn.height(),
                                left: $ad.width() / 2 - $btn.outerWidth()
                            };
                            $btn.css(css);
                            $container.find('.nodeAction').show();
                        }
                    } else {
                        setTimeout(onAdLoaded, 500);
                    }
                }
            }());
        </script>
    </div>

