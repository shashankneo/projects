<!-- mouseflow begin -->
<script type="text/javascript">
    var $body = $('body');
    var resolutionStr = getResolutionRange($body.width(), $body.height());
    var _mfq = _mfq || [];
    var mouseflowPath = '/Responsive Article' + ' ' + resolutionStr;

    function getResolutionRange(width, height){
        var heightRange = [
            { min: 0, max: 649, label: '649' },
            { min: 650, max: 749, label: '749' },
            { min: 750, max: 999999, label: '750+' }
        ];

        var widthRange = [
            { min: 0, max: 550, label: '550' },
            { min: 551, max: 729, label: '729' },
            { min: 730, max: 850, label: '850' },
            { min: 851, max: 990, label: '990' },
            { min: 991, max: 1024, label: '1024' },
            { min: 1025, max: 999999, label: '1025+' }
        ];

        function rangeFilter(base, range){
            return range.min <= base && base <= range.max;
        }

        var w = $.grep(widthRange, $.proxy(rangeFilter, null, width));
        var h = $.grep(heightRange, $.proxy(rangeFilter, null, height));

        if (w.length !== 1) {
            w = [widthRange[0]];
        }

        if (h.length !== 1) {
            h = [heightRange[0]];
        }

        return w[0].label + 'x' + h[0].label;
    }

    (function () {
        var mf = document.createElement("script");
        mf.type = "text/javascript";
        mf.async = true;
        mf.src = "//cdn.mouseflow.com/projects/5b4de110-bc3f-40aa-8751-c3176bbf87d5.js";
        $("head")[0].appendChild(mf);
    })();
</script>
<!-- mouseflow end -->
