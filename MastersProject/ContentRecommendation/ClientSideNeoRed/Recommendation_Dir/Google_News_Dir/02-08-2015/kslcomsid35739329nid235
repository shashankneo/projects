<script charset="utf-8">
    $(function () {
        var viewportWidth = $(window).width();
        var galleryWithWrapUrl = '/index.php?nid=1298&sid=35739329&noHistory=1&pid=';
        var galleryWithWrapIframe = [
            '<p class="magnificIframeLoader">Loading image...</p>' +
                '<div class="magnificCloseButtonContainer invisible">' +
                    '<button title="Close (Esc)" type="button" class="mfp-close">&#215;</button>' +
                '</div>' +
            '<iframe onload="this.style.visibility = \'visible\'; $(\'.magnificIframeLoader\').remove(); $(\'.magnificCloseButtonContainer.invisible\').removeClass(\'invisible\');" src="',
                '" height="709" width="969" style="border:0;overflow: hidden; display: block; margin: 0 auto;visibility:hidden;" class="magnificIframe">' +
            '</iframe>'
        ];

        var photoGalleryIframeOpt = {
            type: 'inline',
            fixedContentPos: true,

            gallery: {
                enabled: false,
                overflowY: 'scroll'
            },
            showCloseBtn: true,
            items: {},
            callbacks: {
                open: function () {
                    // Focus gallery window so arrow keys can navigate through photos
                    setTimeout(function() {
                        $('iframe.magnificIframe')[0].contentWindow.focus();
                    }, 100);
                }
            }
        };

        /**
         * Bind new images to magnificPopup if it hasn't already
         */
        function updateGalleryModal(){
            var $photos = $('#gallery a');
            var dataKey = 'data-has-magnificentGal';

            $photos.map(function(index, photo){
                if (!$(photo).attr(dataKey)) {
                    var src = galleryWithWrapIframe[0] + (galleryWithWrapUrl + index) + galleryWithWrapIframe[1];
                    var opt = $.extend({},
                        photoGalleryIframeOpt, {
                            items: {
                                src: src,
                                type: 'inline'
                            }
                        }
                    );

                    if (window.IE_VERSION === 8) {
                        $(photo).click(function(event){
                            event.preventDefault();
                            kslSocialShare.openPopup(galleryWithWrapUrl + index, 'photoGallery', 990, 710);
                        });
                    } else {
                        $(photo).magnificPopup(opt);
                        $(photo).attr(dataKey, true);
                    }
                }
            });
        }

        if (viewportWidth < KSL.news.ui.breakPoints.DESKTOP_WIDTH_MIN) {
            var photoGalleryOpt = {
                delegate: 'a',
                type: 'image',
                tLoading: 'Loading image #%curr%...',
                mainClass: 'mfp-img-mobile',
                fixedContentPos: true,
                gallery: {
                    enabled: true,
                    navigateByImgClick: true,
                    overflowY: 'scroll',
                    preload: [0, 2] // Will preload 0 - before current, and 1 after the current image
                },
                image: {
                    tError: '<a href="%url%">The image #%curr%</a> could not be loaded.',
                    verticalFit: true,
                    titleSrc: function (item) {
                        return item.el.attr('title')
                            + ' &middot; <a class="image-source-link" href="'
                            + item.src.replace(/[\?&]filter=.+/, '')
                            + '" target="_blank">Open full size</a>';
                    }
                }
            };

            var photoGalleryBtnOpt = $.extend({},
                photoGalleryOpt,
                { items: $('.popup-gallery a').toArray() },
                { delegate: '' }
            );

            $('#gallery').magnificPopup(photoGalleryOpt);
        } else {
            updateGalleryModal();
            var photoGalleryBtnOpt = $.extend({},
                photoGalleryIframeOpt, {
                    items: {
                        src: galleryWithWrapIframe[0] + (galleryWithWrapUrl + 0) + galleryWithWrapIframe[1],
                        type: 'inline'
                    }
                }
            );
        }

        if (window.IE_VERSION === 8) {
            $('.photoButton').on('click', function(){
                kslSocialShare.openPopup(galleryWithWrapUrl, 'photoGallery', 990, 710);
            });
        } else {
            $('.photoButton').magnificPopup(photoGalleryBtnOpt);
        }

        $('.single_image').magnificPopup({
            type: 'image',
            closeOnContentClick: true,
            mainClass: 'mfp-img-mobile',
            fixedContentPos: true,
            image: {
                verticalFit: true
            }

