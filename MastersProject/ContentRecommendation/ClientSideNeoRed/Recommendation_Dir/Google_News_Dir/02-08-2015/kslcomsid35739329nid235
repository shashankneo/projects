<script type="text/javascript" language="javascript" src="/static/s_code_atm.js"></script>

<script>
    /**
     * @return bool     */
    function isHomepage(){
        var sid = parseInt('35739329', 10);
        var nid = parseInt('235', 10);
        var hpSids = [53583];
        var hpNids = [10, 210];

        if ($.inArray(sid, hpSids) >= 0 || $.inArray(nid, hpNids) >= 0) {
            return true;
        }

        return false;
    }

    /**
     *
     * @returns string     */
    function getHpViewType(){
        if (!isHomepage()) {
            return false;
        }

        var $win = $(window);
        var vpWidth = $win.width();
        var vpHeight = $win.height();
        var breakPoints = [
            { label: '550', minWidth: 0, maxWidth: 550, minHeight: 0, maxHeight: 999999 },
            { label: '729', minWidth: 551, maxWidth: 729, minHeight: 0, maxHeight: 999999 },
            { label: '850', minWidth: 730, maxWidth: 850, minHeight: 0, maxHeight: 999999 },
            { label: '990 x 0-749', minWidth: 851, maxWidth: 999999, minHeight: 0, maxHeight: 749 },
            { label: '990 x 750-849', minWidth: 851, maxWidth: 999999, minHeight: 750, maxHeight: 849 },
            { label: '990 x 850+', minWidth: 851, maxWidth: 999999, minHeight: 850, maxHeight: 999999 }
        ];

        var currSize = $.grep(breakPoints, function findViewportRange(view) {
            return (
                view.minWidth  <= vpWidth  && vpWidth  <= view.maxWidth &&
                view.minHeight <= vpHeight && vpHeight <= view.maxHeight
            );
        });

        if (currSize.length === 1) {
            return currSize[0].label;
        }

        return '';
    }
</script>


