// INFINITE ARTICLE SCROLL
// New articles should appear at the base of every article,
// the "base" being halfway down the related-articles div.

// if ( document.location.hash == '#infinite' ) {

var cachebuster = '?date=2014120913';
var inf_config = {
    property: 'mercurynews',
    tid: 'disqus_thread',
    reload_chartbeat: 1,// Future-proofing
    reload_outbrain: 0, // Future-proofing
    reload_comments: 0,// Future-proofing
    section_lookups: {}
};

// Load the infinite-article script.
// It's in a different location on dev.
var script = document.createElement('script');
script.type = 'text/javascript';
if ( document.location.hash == '#dev' )
{
    script.src = 'http://www.willitsnews.com/infinite.js' + cachebuster;
}
else
{
    script.src = 'http://extras.denverpost.com/app/infinite/infinite.js' + cachebuster;
}
if ( document.location.hash !== '#noscroll' ) $('head')[0].appendChild(script);

// &&& PROPERTY-SPECIFIC
var check_href = document.location.href;
var inf_section = 'all';
if ( /business|troy-wolverton|michelle-quinn|60-second-business-break/i.exec(check_href) ) inf_section = 'business';
else if ( /sports|athletics|sharks|raiders|warriors|earthquakes|tim-kawakami|mark-purdy/i.exec(check_href) ) inf_section = 'sports';
else if ( /entertainment|music|movies-dvds-headlines|movies-dvds|books/i.exec(check_href) ) inf_section = 'entertainment';
else if ( /bay-area-living|home-garden|eat-drink-play|family-relationships|health|travel-headlines|travel|hawaii|cruises|libations|san-francisco-bay-area-travel/i.exec(check_href) ) inf_section = 'lifestyles';
else if ( /49ers/i.exec(check_href) ) inf_section = '49ers';
else if ( /giants/i.exec(check_href) ) inf_section = 'giants';

var script = document.createElement('script');
script.type = 'text/javascript';
script.src = 'http://extras.denverpost.com/cache/article/' + inf_config['property'] + '-' + inf_section + '.js';
if ( document.location.hash !== '#noscroll' ) $('head')[0].appendChild(script);

window.onload = function()
{
    inf_config['articles'] = ar.shuffle();

    // Update inf with the values we've set in inf_config
    if ( typeof inf_config !== 'undefined' )
    {
        for ( var key in inf_config )
        {
            if ( inf_config.hasOwnProperty(key) )
            {
                inf[key] = inf_config[key];
            }
        }
    }

    if ( document.location.hash !== '#noscroll' )
    {
        inf.init();
        inf.checkpoints[0].bottom = inf.get_top();
        $(window).scroll(inf, function()
        {
            run_on_scroll_interval();
        });
        var did_scroll = 0;
        function run_on_scroll_interval() { did_scroll = 1; }
        setInterval(function()
        {
            if ( did_scroll == 1 )
            {
                did_scroll = 0;
                inf.check_scroll();
            }
        }, 500);
    }
}

