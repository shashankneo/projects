        /* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
        var disqus_shortname = 'dfm-mercurynews'; // required: replace example with your forum shortname

(function() {
    // Create some additional Disqus variables so that section anchors do not prevent discussions on the same article from being linked
    var matchesForDisqus = (location.href.match(/(ci_[0-9]+)\/*(.*)/) || []);
    if (matchesForDisqus[0] && matchesForDisqus[1]) {
        window.disqus_identifier = matchesForDisqus[1];
        window.disqus_url = 'http://' + window.location.host + '/' + matchesForDisqus[0];
    }
})(); 

/* * * DON'T EDIT BELOW THIS LINE * * */
   (function() {
      var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
      dsq.src = '//' + disqus_shortname + '.disqus.com/embed.js';
      (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
   })();
   document.write('<div id="disqusHiddenTemp"><a style="font-size:12px; color:#39547a; float:right;" href="#disqus_thread" data-disqus-identifier="'+disqus_identifier+'"># Comments</a></div>');
   $.getScript('http://' + disqus_shortname + '.disqus.com/count.js');
   $('#disqusHiddenTemp a').appendTo($('#articleDate'));
   $('#disqusHiddenTemp').remove();
</script>
<noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
