<script language="JavaScript"><!--

//Local Variables
var PaperBrand = getBrand2(s_account);
var StoryID=  "2774414";
var SectionName= "Opinion";
var ArticleTitle= "Op-ed: Pact moves Iran further from a bomb";
var ArticleAuthor= "By Noel de Nevers";
var PageName= "Opinion" +" - "+ ArticleTitle; 
var FriendlyName = "" + "Opinion" +"/"+ "Commentary" +"/"+ArticleTitle;
var domainName = getDomainName();
var wholeURL= location.href;
var firstHeadline = "Op-ed: Pact moves Iran further from a bomb"

/* Construct path variables from the URL */
var pathArray= wholeURL.split("/");
var arrNum=0;
while (arrNum < pathArray.length)
 {
     //find the deepest level of the path
     if (pathArray[arrNum].indexOf(StoryID)>=0) {
        var pathMax= arrNum;
        }
     //find the root level of the path
     if (pathArray[arrNum].indexOf(domainName)>=0) {
        var pathMin= arrNum+1;
        }
      arrNum+=1;
 }
 var pathCount=pathMin;
 var sectCount=0;
 subSectValue="";
 var subSection= new Array();
 subSection[0]="";
 subSection[1]="";
 subSection[2]="";
 subSection[3]="";
 subSection[4]="";
 while (pathCount < pathMax)
  {
      if(sectCount<1){subSection[sectCount]= pathArray[pathCount];}
      else{subSection[sectCount]= subSectValue+"/"+pathArray[pathCount];}
      subSectValue=subSection[sectCount];
      pathCount+=1;
      sectCount+=1;
  }

userObj = new omniObj();
userObj.load();
userObj.update();
userObj.save();

/* You may give each page an identifying name, server, and channel on the next lines. */

	s.pageName=FriendlyName 		// Page Name ONLY.
	s.channel="Opinion" // Site section
	s.pageType="" // Error pages ONLY	
	s.prop1="D=g" // Populating with URL using Dynamic Variables. Do not modify
	s.prop2=subSection[0] // Sub section 1
	s.prop3=subSection[1] // Sub section 2
	s.prop4=subSection[2] // Sub section 3
	s.prop5=subSection[3] // Sub section 4
	s.prop6="" // Global - Section
	s.prop7="" // Global - Sub section 1
	s.prop8="" // Global - Sub section 2
	s.prop9=getCiQueryString("SOURCE") // Source of request, i.e. RSS, flash, etc...
	s.prop11= ArticleTitle +"/"+ StoryID // Title / ID
	s.prop12= ArticleAuthor // Byline
	s.prop13="Active" // Reserved for article
	s.prop18=""	// Sort Type on Search Results
	s.prop19=""	// Search
	s.prop20="" // Reserved for Search
	s.prop28="" // Related Topics, if applicable
	s.prop30="" // Form Analysis Plugin
	s.prop33=PaperBrand + " / " + PageName + getWithSlash(getCiQueryString("SOURCE"))  // Paper Brand + Page Name + Source
	s.prop34=PaperBrand + " /  " + FriendlyName         // Paper Brand + Section + Page Name
	s.prop35=getUserType() + " / " + PaperBrand + " / " + PageName          // User Type + Paper Brand + Page Name
	s.prop36=isCampaign(getCampaignValue("EADID")+getCampaignValue("CREF"), PaperBrand, PageName)    // Campaign Tracking Code  + Paper Brand + Page Name
	s.prop37=isCampaign(getCampaignValue("IADID")+getCampaignValue("SOURCE"), PaperBrand, PageName)    // Affiliate ID + Paper Brand + Page Name
	s.prop38=isCampaign(getCampaignValue("PARTNERID"), PaperBrand, PageName)      // Internal Referral ID + Paper Brand + Page Name
	s.prop40=PaperBrand				     // Paper Brand
	s.prop42="" // Internal Search Refinements
	s.prop43=ArticleTitle=="null"?"":SectionName		    // Section Name
	s.prop44= PaperBrand +"/"+ SectionName // Paper Brand + Article Section + Article ID
	s.prop45= PaperBrand +"/"+ SectionName // Paper Brand + Article Section + Writer
	s.prop46= firstHeadline // ArticleID + Special Page Name + Article Title
	s.prop47="" // Internal Search Query
	s.prop48="" // News.com
	s.prop49="" // News.com
	s.prop50="" // News.com
	
	/* E-commerce Variables */
	s.campaign=getCiQueryString("EADID")+getCiQueryString("CREF")          //External Campaign - ?EADID=id
	s.events="event1"
	s.products="[populated by Server. See DOC for instructions]"
	s.eVar1=getCiQueryString("PARTNERID")		          // Internal Campaign - ?PID=id
	s.eVar2=getCiQueryString("IADID")+getCiQueryString("SOURCE")           // Affiliate ID - ?IADID=id
	s.eVar4="D=pageName"; // Same as s.pageName. Populating with Dynamic Variables. Do not modify.
	s.eVar14=userObj.fPage|userObj.conPage|userObj.loginConPage?userObj.vType:'' //Visitory Type
	s.eVar15=userObj.fPage|userObj.userIdChange?userObj.userId:''	         // User ID
	s.eVar16=userObj.conPage?userObj.rType:''
	s.eVar18=getUserType()

	s.eVar19=userObj.fPage|userObj.conPage|userObj.aaPage?userObj.regStatus:'' //Registration Status
	s.eVar20="D=c47" // Same as s.prop47. Populating with Dynamic Variables. Do not modify
	s.eVar23="D=c42" // Same as s.prop42. Populating with Dynamic Variables. Do not modify.
	s.eVar24="D=c43" // Same as s.prop43. Populating with Dynamic Variables. Do not modify.
	s.eVar26= StoryID //Article ID or Identifier.
	s.eVar50=getCiQueryString("AADID")


