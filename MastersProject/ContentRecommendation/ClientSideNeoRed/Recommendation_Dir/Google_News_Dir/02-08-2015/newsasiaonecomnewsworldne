<source srcset="http://news.asiaone.com/sites/default/files/styles/w641/public/original_images/Aug2015/20150801-taliban.jpg?itok=S1CJbQ0d 1x" media="(min-width: 1024px)" />
<source srcset="http://news.asiaone.com/sites/default/files/styles/w848/public/original_images/Aug2015/20150801-taliban.jpg?itok=6E9xQXbM 1x" media="(min-width: 768px) and (max-width: 1024px)" />
<source srcset="http://news.asiaone.com/sites/default/files/styles/w641/public/original_images/Aug2015/20150801-taliban.jpg?itok=S1CJbQ0d 1x" media="(min-width: 480px) and (max-width: 767px)" />
<source srcset="http://news.asiaone.com/sites/default/files/styles/w400/public/original_images/Aug2015/20150801-taliban.jpg?itok=nm-8exd8 1x" media="(max-width: 479px)" />
