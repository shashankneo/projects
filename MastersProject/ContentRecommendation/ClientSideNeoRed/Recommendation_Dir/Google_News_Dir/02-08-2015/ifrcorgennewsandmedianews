<div class="cw_boxfix">
	<div>
		<div>
			<div class="cw_boxtitle"><h3>Map</h3></div>
			
			<div class="cw_boxcontent">
				<div class="cw_boxelement">
					<div id="map" style="width: 200px; height: 300px"></div>
				</div>
			</div>
			
		</div>
	</div>
</div>
<script type="text/javascript">
    //<![CDATA[
    google.load("maps", "2");
    var map = null;
    var geocoder = null;
	var latlngbounds;
	var zoomlevel;
    var locations = new Array('23.684994,90.356331');
    var locationNames = new Array('bangladesh');
	var countries = new Array();
	var countryCenterLatLng;
	var myIcon; 
	var markerOptions;


    function initialize() {
        map = new google.maps.Map2(document.getElementById("map"));
        geocoder = new GClientGeocoder();
        myIcon = new GIcon(G_DEFAULT_ICON);
		myIcon.image = "/Templates/CommonContent/images/icons/icon_news_03_public.png";
		myIcon.iconSize = new GSize(28, 28);
		markerOptions = { icon: myIcon };
        zoomlevel = 5;
		
		if(countCountries(locationNames) <= 1){
			 geocoder.getLocations(getCountryOutOfLocation(locationNames[0]), function(response){ countryCallback(response)});
		}

		for(index = 0; index < locations.length; index++) {
            var locationName = locationNames[index];
            geocoder.getLocations(locationName, function(response){ makeGMCallback(response)});
		}
    }

    google.setOnLoadCallback(initialize);

	function countryCallback(response){
		countryCenterLatLng = new google.maps.LatLng(response.Placemark[0].Point.coordinates[0], response.Placemark[0].Point.coordinates[1]);
		setZoomOneCountry(response.Placemark[0].ExtendedData.LatLonBox);
	}

    function makeGMCallback(response) {
		var latLng = new google.maps.LatLng(response.Placemark[0].Point.coordinates[1], response.Placemark[0].Point.coordinates[0]); 

		if(countries.length > 1){
			setZoom(latLng);
		}
		addMarker(latLng);

    } 
   
	function setZoom(latLng){
		if(latlngbounds == null){
			latlngbounds = new GLatLngBounds(latLng, latLng);
		}else{
			latlngbounds.extend(latLng);
		}
		centerAndZoom();
	} 

    function setZoomOneCountry(box) {
		var sw=new GLatLng(box.south,box.west);
		var ne=new GLatLng(box.north,box.east);
		latlngbounds = new GLatLngBounds(sw,ne);
		centerAndZoom();
    }

	function centerAndZoom(){
		var boundsZoomLevel = map.getBoundsZoomLevel(latlngbounds);
        if(boundsZoomLevel < zoomlevel){
			zoomlevel = boundsZoomLevel;
        }
		map.setCenter(latlngbounds.getCenter(), zoomlevel);
		map.panTo(latlngbounds.getCenter());
	}

   

    function addMarker(latLng) {
        var marker = new GMarker(latLng, markerOptions);
        map.addOverlay(marker);
    }



	/* Countryhelpers... */
	function countCountries(locations){
        var nrOfCountries = 0;
		var country;
		for(var i = 0; i < locations.length; i++) {
			if(countryNotExists(getCountryOutOfLocation(locations[i]))){
				nrOfCountries++;
			}
		}
		return nrOfCountries;
	}

	function countryNotExists(country){
		var doNotExist = true;
		for(var j = 0; j < countries.length; j++) {
			if(countries[j] == country){
				doNotExist = false;
				break;
			}
		}
		if(doNotExist){
			countries.push(country);
		}
		return doNotExist;
	}

	function getCountryOutOfLocation(location){
		var country;
		if (location.indexOf(',') != -1) {
			country = location.substr(location.lastIndexOf(', ')+2, location.length-1)   
		}else{
			country = location;
		}
		return country;
	}

	 /*function centerAndZoomOnBounds() {
		var center = bounds.getCenter();
		var newZoom = map.getBoundsZoomLevel(bounds);
		if (map.getZoom() != newZoom) {
			map.setCenter(center, newZoom);
		}else{
			map.panTo(center);
		}
    }*/

	//]]>
</script>
