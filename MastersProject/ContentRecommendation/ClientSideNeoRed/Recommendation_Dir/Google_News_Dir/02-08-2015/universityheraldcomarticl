		url = "/home/news/services/send_tip.php?ar_id=21688&ar_url=http://www.universityherald.com/articles/21688/20150730/plants-send-animal-like-signals-when-stressed.htm&ar_title=Plants+Send+Animal-Like+Signals+When+Stressed";
	} else if($(this).hasClass('pv')) {
		url = "/home/news/services/send_pv.php?ar_id=21688&ar_url=http://www.universityherald.com/articles/21688/20150730/plants-send-animal-like-signals-when-stressed.htm&ar_title=Plants+Send+Animal-Like+Signals+When+Stressed";
		h = 520;
	} else {
		url = "/home/news/services/send_cr.php?ar_id=21688&ar_url=http://www.universityherald.com/articles/21688/20150730/plants-send-animal-like-signals-when-stressed.htm&ar_title=Plants+Send+Animal-Like+Signals+When+Stressed";
	}
	$(this).colorbox({href: url, iframe:true, width: 560, height: h, onLoad: function(){$('#cboxClose').remove();}});
});
