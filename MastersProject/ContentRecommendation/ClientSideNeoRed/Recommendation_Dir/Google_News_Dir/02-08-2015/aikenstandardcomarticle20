<script>
  function resizeText(multiplier) {
    currentSize = parseInt($('p').css('font-size'));
    remSize = currentSize / 16;
    console.log(remSize);
    newSize = remSize + (multiplier * 0.2) + "rem";
    console.log(newSize);
    $('p').css('font-size', newSize);
  }
</script>
