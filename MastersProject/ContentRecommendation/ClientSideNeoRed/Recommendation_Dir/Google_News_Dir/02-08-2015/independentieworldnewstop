head.ready(function() {
function addHandler(player) { if(player.getRootUrl() !== "http://independent.bbvms.com") return; var played = false; player.on('play', function() { if (!played) { played = true; $(document.body).append('<img style="display:none" src="http://r.independent.ie/track.gif?referrer=' + encodeURIComponent(player.getDeeplink()) + '&vid=' + encodeURIComponent(player._id) + '">') } }) }
var count=5; function doAdd() {if(bluebillywig.players.length === 0 && count > 0){count--; setTimeout(doAdd, 500)} else {for(var i=0; i<bluebillywig.players.length; i++) addHandler(bluebillywig.players[i]);} }
