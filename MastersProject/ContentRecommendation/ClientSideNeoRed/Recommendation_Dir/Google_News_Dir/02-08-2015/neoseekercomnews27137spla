if (enableStickyAd) {
	function unstickAd(adContainerId) {
		var scrollTop = $(window).scrollTop();
		var $adContainer = $('#'+adContainerId);
		if (adContainerId == 'leaderboardTop' && $adContainer.length) {
			if ($adContainer.closest('.fixed').hasClass('unstick')) {
				return false;
			}
			var h = $('#header').height() + parseInt($adContainer.offset().top);
			if (scrollTop > h) {
				$('#' + adContainerId).closest('.fixed').addClass('unstick').animate({
					marginTop: "-100%"
				}, 1000, function () {
					$(this).removeClass('fixed').css('marginTop', '');
				});
			} else {
				$('#' + adContainerId).closest('.fixed').addClass('unstick').removeClass('fixed');
			}
			if ($('#column_right_ads_top').hasClass('fixed')) { // unstick top sidebar
				unstickAd('sidebarAdTop-header');
			}
		} else if (adContainerId == 'sidebarAdTop-header') {
			$('#column_right_ads_top').animate({
				top: '0'
			}, 1000);
		} else if (adContainerId == 'column_right_ads_top' && $adContainer.length) {
			if ($('#column_right_ads_top').hasClass('unstick')) {
				return false;
			}
			var h = $('#column_right_ads_top').offset().top;
			if (scrollTop > h) {
				$('#sidebarAdTopPlaceholder').remove();
				$('#column_right_ads_top:not(.unstick)').addClass('unstick').animate({
					marginTop: '-100%'
				}, 1000, function () {
					$(this).removeClass('fixed').css('marginTop', '');
				});
			} else {
				$('#column_right_ads_top').addClass('unstick').removeClass('fixed');
				$('#sidebarAdTopPlaceholder').remove();
			}
		}
	}
	if (window.location.hash) {
		var hash = window.location.hash.replace(/#/, '');
		if ($('*[id="'+hash+'"]').length) {
			$.scrollTo($('*[id="' + hash + '"]'), 500, { offset: { top: -105 } });
		} else {
			$.scrollTo($('a[name="' + hash + '"]'), 500, { offset: { top: -105 } });
		}
	}
	unstickAd_by_timeout = function() {
		console.log('in unstickAd_by_timeout');
//		if ($('#ads_page_top').hasClass('fixed') || $('#column_right_ads_top').hasClass('fixed')) {
			console.log("Unstick by timeout");
			unstickAd('leaderboardTop');
			unstickAd('column_right_ads_top');
//		}
		if ($('#ads_page_top').hasClass('fixed') || $('#column_right_ads_top').hasClass('fixed')) {
			setTimeout(unstickAd_by_timeout, 1000);
		}
	};
	setTimeout(function() {
		console.log('Timeout fired');
		unstickAd_by_timeout();
	}, 10000);
	window.ZDVOptions = {
		onViewable: function(ad) {
			var id = $('#'+ad.containerId).closest('.section-ad').attr('id');
			unstickAd(id);
			console.log("Unstick by viewable: "+ad.containerId+"; id: "+id);
		}
	};
	var $sidebarAdTop = $('#column_right_ads_top');
	if ($sidebarAdTop.length) {
		var sidebarAdTopPos = $sidebarAdTop.offset();
		reposition_ads = function() {
			var scrollTop = $(window).scrollTop();
			var sidebarAdTopH = parseInt(sidebarAdTopPos.top)-parseInt($('#ads_page_top.fixed').outerHeight() || 0);
			if (scrollTop >= sidebarAdTopH && !$sidebarAdTop.hasClass('unstick')) {
				if (!$('#sidebarAdTopPlaceholder').length) {
					$sidebarAdTop.addClass('fixed').before('<div id="sidebarAdTopPlaceholder"></div>');
					$('#sidebarAdTopPlaceholder').css({
						width: $sidebarAdTop.outerWidth(),
						height: $sidebarAdTop.outerHeight(true)
					});
					if ($('#ads_page_top').hasClass('fixed')) {
						$sidebarAdTop.css('top', $('#ads_page_top').outerHeight(true) + 'px');
					}
				}
			} else {
				$sidebarAdTop.removeClass('fixed');
				$('#sidebarAdTopPlaceholder').remove();
			}
		};
		reposition_ads();
		$(window).scroll(reposition_ads);
	}
	$.getScript("//cdn.ziffstatic.com/jst/zdvtools.min.js?v=1");
}
