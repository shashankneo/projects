<script type="text/javascript">
$(function() {
	$('a', '#related_products_navigation').click(function() {
		if ($(this).hasClass('hide')) {
			return false;
		} else {
			var related_product_id = $(this).attr('rel');
			$('.entity:not(.hide)', '#attached_products').addClass('hide');
			$('#related_'+related_product_id).removeClass('hide');

			$('.active', '#related_products_navigation').removeClass('active');
			$(this).addClass('active');
			return false;
		}
	});
});
