rse_data'].city && window._config['golfcourse_data'].city!="")	{	golf_address =golf_address+', '+window._config['golfcourse_data'].city;	}	if(window._config['golfcourse_data'].postalCode && window._config['golfcourse_data'].postalCode!="")	{	golf_address = golf_address+', '+window._config['golfcourse_data'].postalCode;	}	document.getElementById('golf-title').innerHTML =golf_address+', '+window._config['golfcourse_data'].phone1;	}	if(typeof window._config['campground_data'] != 'undefined' && typeof window._config['campground_data'].address != 'undefined')	{	var campground_address;	campground_address = window._config['campground_data'].address;	if(window._config['campground_data'].city && window._config['campground_data'].city!="")	campground_address += ', ' + window._config['campground_data'].city;	if(window._config['campground_data'].postalCode && window._config['campground_data'].postalCode!="")	campground_address += ', ' + window._config['campground_data'].postalCode;	document.getElementById('location-address-title').innerHTML = campground_address + ', ' + window._config['campground_data'].phone;	var el = document.getElementById('masthead');	if(el.className != ''){	el.className += ' has-location-address';	} else {	el.className = 'has-location-address';	}	}	if(typeof window._config['beach_info_data'] != 'undefined')	{	var beach_info_data = window._config['beach_info_data'],	beaches_address = [],	address = [];	if(beach_info_data.streetNumber && beach_info_data.streetNumber != "")	address.push(beach_info_data.streetNumber);	if(beach_info_data.streetName && beach_info_data.streetName != "")	address.push(beach_info_data.streetName);	if(beach_info_data.streetType && beach_info_data.streetType != "" && isNaN(beach_info_data.streetType))	address.push(beach_info_data.streetType);	if(beach_info_data.streetDirection && beach_info_data.streetDirection != "" && isNaN(beach_info_data.streetDirection))	address.push(beach_info_data.streetDirection);	if(address.length != 0)	beaches_address.push(address.join(' '));	if(beach_info_data.city && beach_info_data.city != "")	beaches_address.push(beach_info_data.city);	if(beach_info_data.postal && beach_info_data.postal != "")	beaches_address.push(beach_info_data.postal);	document.getElementById('location-address-title').innerHTML = beaches_address.join(', ');	var el = document.getElementById('masthead');	if(el.className != ''){	el.className += ' has-location-address';	} else {	el.className = 'has-location-address';	}	}	if(typeof window._config['skiresort_address'] != 'undefined') {	document.getElementById('location-address-title').innerHTML = window._config['skiresort_address'];	var el = document.getElementById('masthead');	if(el.className != ''){	el.className += ' has-location-address';	} else {	el.className = 'has-location-address';	}	}	</script>	<div class="navsearchform">
