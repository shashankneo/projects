}
</script>
<script>
//Story Share Tools
$(document).ready(function() {
var stickyTools = function(){
var stickyShareTools = $('.share-tools-wrapper').offset().top;
var commentingContainer = $('.commenting-container').offset().top;
var scrollTop = $(window).scrollTop();
var storyBodyTop = $('#story-body-items').offset().top;
if (scrollTop > stickyShareTools) { $('.share-tools-wrapper').addClass('sticky-tools');
}
if (scrollTop > commentingContainer || scrollTop < storyBodyTop) {
$('.share-tools-wrapper').removeClass('sticky-tools'); }
};
stickyTools();
$(window).scroll(function() {
stickyTools();
});
});
//Story Numberlines
$( '.highline-number p').each(function() {
$(this).wrapInner('<span></span>');
var numberlineSpan = $(this).find('span');
$(this).find('.ng_number_leadin').detach().insertBefore(numberlineSpan);
});
</script>
