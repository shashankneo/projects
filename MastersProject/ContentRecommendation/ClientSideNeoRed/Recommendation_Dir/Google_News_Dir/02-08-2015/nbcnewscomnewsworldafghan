    if (window.location.href.indexOf('stageads=true') > -1 || window.location.host.indexOf('sys') === 0 || window.location.host.indexOf('local.nbcnews.com:9001') === 0  || window.location.host.indexOf('nbcnews.com') === -1){return true;}
