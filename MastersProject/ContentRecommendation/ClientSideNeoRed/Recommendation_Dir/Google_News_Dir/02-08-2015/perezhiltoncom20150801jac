for (index = 0; index < phrt_config.length; index++){
    // check cookie
    if (!$.cookie(phrt_config[index].cookie_name)){
        if (phrt_config[index].geo.indexOf(ph_geoc) != -1){
    	    if (phrt_config[index].page_types.indexOf(ph_page_type) != -1){
	        if ($('.post.' + phrt_config[index].post_category).length){
    	    	    // if not extists then store
    	    	    $.cookie(phrt_config[index].cookie_name,phrt_config[index].cookie_value,{
            	        expires: phrt_config[index].cookie_exp,
              	        path: '/',
            	        domain: 'perezhilton.com'
        	    });
		    $.cookie(phrt_config[index].cookie_value,1,{
            	        expires: 0.5,
            	        path: '/',
            	        domain: 'perezhilton.com'
        	    });
    	    	    // and trace it
    	    	    perez_statLog({
		        app: 'perez_ads',
		        category: phrt_config[index].tracer_category + '-' + phrt_config[index].cookie_value
    	    	    });
                    perez_statLog({
                        app: 'perez_ads',
                        category: phrt_config[index].tracer_category + '-total'
                    });
	        }
    	    }
        }
    } else {
        // active user tracking
        phrt_config[index].cookie_value = $.cookie(phrt_config[index].cookie_name);
        // use UID as 12 hours cookie
        if (!$.cookie(phrt_config[index].cookie_value)){
	    $.cookie(phrt_config[index].cookie_value,1,{
                expires: 0.5,
                path: '/',
                domain: 'perezhilton.com'
            });
	    perez_statLog({
                app: 'perez_ads',
                category: phrt_config[index].tracer_category + '-' + phrt_config[index].cookie_value + '-' + ph_date_number,
	        message: ph_date_number
            });
        }
    }
}
