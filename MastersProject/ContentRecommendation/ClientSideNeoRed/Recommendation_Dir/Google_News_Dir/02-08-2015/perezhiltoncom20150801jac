    <script type='text/javascript'>
    var _sf_async_config={};
        /** CONFIGURATION START **/
        _sf_async_config.uid = 57067;
        _sf_async_config.domain = 'perezhilton.com';
        _sf_async_config.useCanonical = true;

                _sf_async_config.sections = 'Post, Politik, Donald Trump, Hillary Clinton, Jack Osbourne, John McCain, Funny, Controversy';
