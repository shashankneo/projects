<!-- StatDB -->
<script>
    function statLog(params) {
        var app = params.app || 'perezhilton';
        var options = 'm';
        var category = params.category || 'site';
        var message = params.message || '';
        var target = 'http://statdb.pressflex.com/log.gif?';
        target += 'a=' + escape(app);
        target += '&o=' + options;
        target += '&c=' + escape(category);
        target += '&m=' + escape(message);
        target += '&rand=' + Math.floor(Math.random() * 99999999);
        var targetImage = new Image();
        targetImage.src = target;
    }
    statLog({});
    
            statLog({category: 'site_desktop'});
    
    function perez_countVisits() {
        var cookieName = 'perez_visit_counter';
        if (!$.cookie(cookieName)) {
            statLog({category: 'visit'});
        }
        var expires = new Date();
        expires.setTime(expires.getTime() + (30 * 60 * 1000)); // 30 minutes
        $.cookie(cookieName, '1', {
            expires: expires,
            path: '/',
            domain: 'perezhilton.com'
        });
    }
    perez_countVisits();

    // Referer statistics
        if (typeof(document.referrer)=='string' && document.referrer!='') {
        var _ref = document.referrer.toLowerCase(),_d;
        if (_ref.indexOf("://") > -1) {
            _d = _ref.split('/')[2];
        } else {
            _d = _ref.split('/')[0];
        }
        _d = _d.split(':')[0];
        if (_d.indexOf('perezhilton')==-1) {
            var _dd = _d.split('.'),_ll = _dd.length;
            if (_d.indexOf('google')!==-1)
                _d = 'google.com';
            else if (_ll == 4 && /[0-9]{1-3}\.[0-9]{1-3}\.[0-9]{1-3}\.[0-9]{1-3}/.test(_d))
                _d = '';
            else if (_ll > 2 && (_dd[_ll-2]=='com' || _dd[_ll-2]=='co'))
                _d = _dd[_ll-3] + '.' + _dd[_ll-2] + '.' + _dd[_ll-1];
            else if (_ll > 2)
                _d = _dd[_ll-2] + '.' + _dd[_ll-1];
            if (_d != '')
                statLog({category: 'referrer_data-fixed-' + _d});
        }
    }
    </script>
<!-- /StatDB -->

