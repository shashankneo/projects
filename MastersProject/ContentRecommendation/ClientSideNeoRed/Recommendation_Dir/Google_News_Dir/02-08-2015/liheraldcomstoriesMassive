		<script>
		function lih_nav(in_item){
			$('#navitem_'+in_item).addClass('subnavitem_on');
			if( in_item == 11 || in_item == 12 || in_item == 13 || in_item == 212 || in_item == 16 || in_item == 119 || in_item == 216 || in_item == 88 ){
				$('#subnav_'+in_item).show();
				$('.subitem'+in_item).show();
			}		
		}
		function lih_nav_out(in_item){
			$('#navitem_'+in_item).removeClass('subnavitem_on');
			if( in_item == 11 || in_item == 12 || in_item == 13 || in_item == 212 || in_item == 16 || in_item == 119 || in_item == 216 || in_item == 88 ){
				$('#subnav_'+in_item).hide();
			}
		}
		</script>
		
