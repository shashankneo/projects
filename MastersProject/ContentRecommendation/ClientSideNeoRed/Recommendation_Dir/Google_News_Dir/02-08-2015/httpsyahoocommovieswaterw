,"ts_update":1438488282,"suppress_articles":false,"display_name":"Lists","alias_type":"NONE","query_tag":"lists","name":"Lists","slug":"lists","count":200},{"name":"Trivia","slug":"trivia","count":2}],"images":[],"url_slug":"25-raucous-road-trip-facts-about-the-original-125456242337","makelive_date":1438354645419,"ts_update":1438364262,"ccm_id":"ef48050a-2b23-3173-90a2-c9ff86c86822","page_date":1438354645418,"attribution":{"display_name":"Adam K. Raymond","url":{"href":"https:\u002F\u002Ftwitter.com\u002Fadamkraymond","rel":"alternate","type":"text\u002Fhtml"}},"image_sizes":[{"type":"ORIG","sizes":[{"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002FFIT_TO_WIDTH-w500\u002F6359d5ced27dfbd7cc73f0917c3f3e0585d6cbfc.jpg","type":"image\u002Fjpeg","width":500,"height":334,"size_type":"500","formats":{}},{"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002FFIT_TO_WIDTH-w540\u002F6359d5ced27dfbd7cc73f0917c3f3e0585d6cbfc.jpg","type":"image\u002Fjpeg","width":540,"height":361,"size_type":"original","formats":{}}],"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002Foriginal\u002FVZEuzOgvYFKszSSyiF0mfi4ndew","source_url":"http:\u002F\u002Fmagazines.zenfs.com\u002Fresizer\u002F2.0\u002Foriginal\u002FVZEuzOgvYFKszSSyiF0mfi4ndew"},{"type":"ORIG","sizes":[{"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002FFIT_TO_WIDTH-w500\u002F762705e3093d3fdf4ac1f2908fb22a222b3118b3.jpg","type":"image\u002Fjpeg","width":500,"height":360,"size_type":"500","formats":{}},{"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002FFIT_TO_WIDTH-w540\u002F762705e3093d3fdf4ac1f2908fb22a222b3118b3.jpg","type":"image\u002Fjpeg","width":540,"height":389,"size_type":"original","formats":{}}],"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002Foriginal\u002FU5dR7AzsrT8jM1O9vPKJAeEtuhA","source_url":"http:\u002F\u002Fmagazines.zenfs.com\u002Fresizer\u002F2.0\u002Foriginal\u002FU5dR7AzsrT8jM1O9vPKJAeE