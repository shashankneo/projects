            </div>
        
    </div>
    <script type="text/javascript">
        USN.load('jquery.slides.min');
    </script>
    <script type="text/javascript">
        $(function(){
            $('.carousel-slidesjs').each(function () {
                var $img = $(this).find('img').first(),
                        height = $img.attr('height'),
                        width = $img.attr('width');

                $(this).slidesjs({
                    height: '270',
                    width: '300',
                    play: {
                        auto: true
                    }
                });
            });
        });
    </script>
</div>







