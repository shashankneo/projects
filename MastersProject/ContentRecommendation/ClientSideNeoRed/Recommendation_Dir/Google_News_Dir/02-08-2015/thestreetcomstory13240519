<!-- the promo -->
        <!-- the logic extracts the ticker from querystring/pathcomponent depending on the source of the request -->
<!-- and populates the story promo. In absence of the ticker, default promo is displayed -->

<script language="javascript" type="text/javascript">
    function getParameterByName (url,name)  {
        name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
        var regexS = "[\\?&]" + name + "=([^&#]*)";
        var regex = new RegExp(regexS);
        var results = regex.exec(url);
        if(results == null)
            return "";
        else
            return decodeURIComponent(results[1].replace(/\+/g, " "));
    }
    if(jQuery){
        $(function(){
            function get_hostname(url) {
                if (url != null && url != '') {
                    partDomain = url.split('/');
                    var domainName = partDomain[0];
                    var hostName = domainName.split('.').reverse()[1];
                    if (hostName !== undefined) {
                        return hostName;
                    }
                }
            };

            var partners = {
                "promoDisplay":{
                    
                    "freeReport":{
                        "text":"Get <i>TheStreet Quant Ratings'</i> exclusive 5-page report for (<span>RCL</span>) - FREE.",
                        "cssClass":"FreeReport"
                    }
                },
                "noReferrer": {
                    "roid":"028241",
                    "useQueryString": false
                },
                "thestreet": {
                    "roid":"028241",
                    "useQueryString": false,
                    "pathComponent": "2"
                },
                "yahoo": {
                    "roid":"028235",
                    "useQueryString": true,
                    "queryStringParamName": "s"
                },
                "cnn": {
                    "roid":"028236",
                    "useQueryString": true,
                    "queryStringParamName": "symb"
                },
                "marketwatch":{
                    "roid":"028237",
                    "useQueryString": false,
                    "pathComponent": "3"
                },
                "nasdaq": {
                    "roid":"028238",
                    "useQueryString": false,
                    "pathComponent": "2"
                },
                "dailyfinance": {
                    "roid":"028239",
                    "useQueryString": false,
                    "pathComponent": "4"
                },
                "finviz": {
                    "roid":"028240",
                    "useQueryString": true,
                    "queryStringParamName": "t"
                }
            };



            function hasRatingsReport(partnerInfo, ticker, callback){
                ticker = ticker.toUpperCase();
                var url = 'https://secure2.thestreet.com/cap/prm.do?OID=028241&ticker=' + ticker;
                var oid = '028241';
                
                $.getJSON("/util/ratings-screener.jsp?wt=json&omitHeader=true&rows=0&indent=on&q=type:equity+AND+ticker:" + ticker + "&fl=ticker").success(
                        function(data){
                            var cssClass = partnerInfo.promoDisplay.freeReport.cssClass;
                            var promoText =  partnerInfo.promoDisplay.freeReport.text;

                            if(data && data.response && data.response.numFound > 0){
                                url = 'https://secure2.thestreet.com/cap/prm.do?OID=' + partnerInfo.roid + '&ticker=' + ticker;
                                oid = partnerInfo.roid;
                            } else if(data && data.response && data.response.numFound == 0){
                                return false;
                            }
                            if(callback && typeof callback == 'function' ){
                                callback(ticker, url, oid, promoText, cssClass);
                            }
                        }
                );

            }

            function configurePromotion( ticker, url, id, text, cssClass){
                var targetClass = 'textPromoAd' + cssClass;
                ticker = ticker.toUpperCase();
                var outerDiv = $('.textPromoAdFreeRep');
                var link = outerDiv.find("A.findoutFreeRep");
                link.attr('href',url);
                link.attr('data-oid', id);
                if (text !== '') {
                    link.attr('data-link-type','product-signup');
                    link.attr('data-product-name', 'TheStreet Quant Ratings');
                } else {
                    link.attr('data-link-type','co-registration');
                }
                link.attr('data-link-name','above transcript article content');
                link.attr('data-link-order','1');
                link.html(text);
                link.attr('class', 'findout' + cssClass);
                outerDiv.find("span").html(ticker);
                outerDiv.attr('class', targetClass);
            }

       /*      function populateEtfAlternativesWidget(ticker){
                $.ajax({
                    url:"/quote/etfWidget.ajaxaction",
                    type:"GET",
                    cache: false,
                    data:{
                        ticker:ticker,
                        defaultEtfs:"VTI,SPY,IWM,QQQ,DIA,EEM"
                    }
                }).done(function (data) {
                    $('#etfAlternativeWidget').html(data);
                })
            } */

            function findPartnerPromotion(partners){
                var matches = document.referrer.match(/http:\/\/(.*)(\?(.*))?/i);
                var hostname = "";
                var uri = "";
               /*  var etfWidgetTicker = ""; */

                if(matches != null && matches.length > 1){
                    hostname = matches[1];
                } else {
                    hostname = 'xxx.noReferrer.xxx/xxx';
                }

                for (p in partners) {
                    var partnerCode = p;
                    var partnerInfo = $.extend({promoDisplay: partners.promoDisplay}, partners[p]);
                    if (get_hostname(hostname) === partnerCode) {
                        var ticker = "";
                        if (partnerInfo.useQueryString) {
                            ticker = getParameterByName(document.referrer, partnerInfo.queryStringParamName);
                        }
                        else {
                            var pathComponents = hostname.split('/');
                            if (partnerCode == "thestreet") {
                                if (pathComponents[1] == 'quote') {
                                    var pathComponents = hostname.split('.');
                                    ticker = pathComponents[partnerInfo.pathComponent];
                                    ticker = decodeURIComponent(ticker.split('/')[2].toUpperCase());
                                } else {
                                    ticker = 'RCL';
                                }
                            } else if (partnerCode == "noReferrer") {
                                ticker = 'RCL';
                            } else {
                                ticker = pathComponents[partnerInfo.pathComponent].toUpperCase();
                            }
                        }

                        if (ticker != null && ticker != '' && ticker.substring(0, 1) != '^') {
                            hasRatingsReport(partnerInfo, ticker, configurePromotion);
                           /*  populateEtfAlternativesWidget(ticker);
                            etfWidgetTicker = ticker; */
                        }
                        break;
                    }
                }
                /* if(etfWidgetTicker != null && etfWidgetTicker != '' && etfWidgetTicker.substring(0, 1) != '^'){
                } else {
//                	var str = partnerInfo.promoDisplay.default.text;
                    var str = partnerInfo.promoDisplay.freeReport.text;
                    var tempTicker = str.substring(str.indexOf("<span>")+6, str.indexOf("</span>"));
                    populateEtfAlternativesWidget(tempTicker);
                } */
            }

            findPartnerPromotion(partners);
        });
    }
</script>

