function addToTargeting(key, value) {
    try {
        if (typeof value == "string") {
            value = value.toLowerCase();
            value = clean(value);
        }


        if (targetingValues.hasOwnProperty(key)) {
            targetingValues[key].push(value);
        }
        else {
            targetingValues[key] = new Array();
            targetingValues[key][0] = value;
        }
    } catch (e) {
    }
}

function clean(str) {
    //   s = s.toLowerCase();
    var s = str.toString();
    s = s.toLowerCase();
    s = s.replace(/ /g, "-");
    s = s.replace(/\|\.|\[|\]|\*|\!|\@|#|$|\%|\^|\&|\(|\)|\:|\'|\"|\<|\>|\?|\\|\/|\~|\`/g, "");
    return s;
}


function resizeIframe(newHeight, newWidth, id) {

    var ifrm = document.getElementById(id);
    ifrm.height = parseInt(newHeight, 10);
    ifrm.width = parseInt(newWidth, 10)


}

function animateSlider(data) {
    $(document).ready(function () {
        var w = window.innerWidth;
        var shiftLeft = w/2 - 150;
        try {
            $('body').append($('<div/>', {
                id:'swslider',
                style:'position: fixed;top: 200px;left:-400px'

            }));
            $("#swslider").html(data);
            setTimeout(function () {
                $("#swslider").animate({"left":shiftLeft}, 1900);
            }, 6000);

            setTimeout(function () {
                $("#swslider").hide();
            }, 26000);
        } catch (e) {
        }
    });

}

function bottomOverlay(data) {
    $(document).ready(function () {
        try {
            var now = new Date();
            var cookieName = "tstbottomoverlay_" + now.getMonth() + "" + now.getDate() + "" + now.getYear();
            if (document.cookie.indexOf(cookieName) == -1) {
                var expires = new Date();
                expires.setHours(23);
                expires.setMinutes(59);
                expires.setSeconds(59);
                expires = expires.toGMTString();
                document.cookie = cookieName + "=" + now + ";expires=" + expires + ";path=/;domain=.thestreet.com";
            }
            else {
                return false;
            }
            $('body').append($('<div/>', {
                id:'btmoverlay',
                style:'position: fixed;bottom:-120px;left:820px'
            }));
            $("#btmoverlay").html(data);
            setTimeout(function () {
                $("#btmoverlay").animate({"bottom":"0px"}, 1200);
            }, 15000);

            setTimeout(function () {
                $("#btmoverlay").hide();
            }, 30000);
        } catch (e) {
        }
    });

}


    var initAds = function(preload) {

        if (typeof googletag !== 'undefined'){
        var gptAdSlots = {};
        var forwardSlash = '/';
        var adsToRender=[];
        var adPlaceholders=[];
        var slotsToRefresh=[];
        googletag.cmd.push(function() {

            $('.googleAd').each(function() {

                var id = this.id;
                var adSizesArray;

                $adDiv= $( "#" + id );
                try {
                    var slot = "";
                    adSizesArray = getAdSizeFromId(id);
                    if (preload==true && $adDiv.hasClass("preload") || preload==false &&  !$adDiv.hasClass("preload")) {

                        if ($adDiv.hasClass("placeholder")) {
                            //dont
                        }

                        else {

                            if (adSizesArray.targeting.hasOwnProperty('desktopOnly') && !device.desktop()) {
                                // NO-OP

                            }
                            else if (adSizesArray.targeting.hasOwnProperty('tabletOnly') && !device.tablet()) {
                                // NO-OP

                            }
                            else {

                                if (adSizesArray.oop==true) {
                                    slot = googletag.defineOutOfPageSlot(adSizesArray.zone, id);
                                }
                                else {
                                    slot = googletag.defineSlot(adSizesArray.zone, adSizesArray.sizes, id);
                                }

                                if (adSizesArray.targeting.hasOwnProperty('key') && adSizesArray.targeting.hasOwnProperty('value')) {
                                    slot.setTargeting(adSizesArray.targeting.key, adSizesArray.targeting.value);
                                }
                                if (adSizesArray.targeting.hasOwnProperty('noCollapse')) {
                                    slot.setCollapseEmptyDiv(false);
                                }
                                if (adSizesArray.targeting.hasOwnProperty('collapse')) {
                                    slot.setCollapseEmptyDiv(true,true);
                                }
                                if (slot) {
                                    slot.addService(googletag.pubads())
                                    gptAdSlots[id] = slot;
                                    adsToRender.push(id);
                                    slotsToRefresh.push(gptAdSlots[id]);
                                }
                            }
                        }
                    }
                }catch(e){console.log("ad error" + e + " " + id)}
            });


            var rsi_segs = [];
            var segs_beg = document.cookie.indexOf('rsi_segs=');
            if (segs_beg >= 0) {
                segs_beg = document.cookie.indexOf('=', segs_beg) + 1;
                if (segs_beg > 0) {
                    var segs_end = document.cookie.indexOf(';', segs_beg);
                    if (segs_end == -1) segs_end = document.cookie.length;
                    rsi_segs = document.cookie.substring(segs_beg, segs_end)
                            .split('|');
                }
            }

            googletag.pubads().setTargeting("rsi", rsi_segs.slice(0, 20));
            targetingValues = {};
            try {
                addToTargeting("kval", $.url().param('kval'));
            } catch (e) {
            }
            
            addToTargeting("tkr", "RCL");
            
            addToTargeting("kw", "Earnings,Suppress PS_Text promo,Transcript");
            
            addToTargeting("storytype", "earningstranscript");
            
            addToTargeting("authors", "TheStreet Transcripts ");
            
            addToTargeting("kval", "tsbottomoverlay,earningstranscript");
            

            for (var key in targetingValues) {
                googletag.pubads().setTargeting(key, targetingValues[key]);
            }


                googletag.pubads().collapseEmptyDivs();
                googletag.pubads().enableSingleRequest();
                googletag.pubads().disableInitialLoad();
                googletag.enableServices();



            for (var adsToRenderId in adsToRender){

                googletag.display(adsToRender[adsToRenderId]);

            }
            if  (slotsToRefresh.length>0) {
                googletag.cmd.push(function() {
                    googletag.pubads().refresh(slotsToRefresh);
                })
            }

        });

        }

    };

    //$(document).ready(function () {
    initAds(true);
    //})
    tstOnreadyDefer.done(function( ) {
        initAds(false);
    })
</script></body>
</html>