<script type="text/javascript">

    if($.cookie('hoverfoot') === null  ) {
        $('#hoverfoot').fadeIn(3000);
    //          $('#toggleSpan').removeClass();
        var date = new Date();
        var minutes = 1;
        var seconds = 15;
        //date.setTime(date.getTime() + (minutes * 60 * 1000));
        date.setTime(date.getTime() + (seconds * 1000));
        $.cookie("hoverfoot", "on", { expires: date });

         // settimeout
        setTimeout(function(){$('#hoverfoot').fadeOut(3000); //*$('#toggleSpan').removeClass(); $('#toggleSpan').addClass('toShow');
        }, 15000);

    }

</script><!--start omniture-->

