            </div></div></a></script><script type="text/javascript">
    (function($) {
        var weather_widget_path = '/weather/currentconditions/';
        var site_zip = '33401';
        try {
            var user_cookie_data_raw = $.cookie('_cmg_weather_location'),
                user_cookie_data = JSON.parse(user_cookie_data_raw),
                user_zip = user_cookie_data.zip_code;
        } catch (err) {
            var user_zip = "";
        }

        cmg.cmgMainNav = $('.cm-navbar-container').cmgMainNav();
        cmg.cmgMainNav.init();

        if (user_zip) {
            cmg.cmgMainNav.initWeather(weather_widget_path, user_zip);
        } else if (site_zip !== "") {
            cmg.cmgMainNav.initWeather(weather_widget_path, site_zip);
        }
    }(cmg.query));
