		    if(typeof colombia == 'undefined') {
    			var colombia = colombia || {};
    			colombia.fns = colombia.fns || [];
    			(function() {
    				var cads = document.createElement("script");
    				cads.async = true;
    				cads.type = "text/javascript";
    				cads.src = "http://static.columbia.timesinternet.in/ad/commons/js/colombia.js";
    				var node = document.getElementsByTagName("script")[0];
    				node.parentNode.insertBefore(cads, node);
    			})();
			}
			function adwidget(data,container) {
				if(data!=null) {
					var conObj = document.getElementById(container),
					conText = '<a target="_blank" href="' + data.paidAds[0].clk[0] + '" title="' + data.paidAds[0].mainimage + '"><img class="lazy" width="110" height="80" src="' + data.paidAds[0].mainimage + '" alt="' + data.paidAds[0].mainimage + '"></a><a target="_blank" href="' + data.paidAds[0].clk[0] + '">' + data.paidAds[0].title + '</a><p class="sponsored">Sponsored by ' + data.paidAds[0].brandtext + '<img class="flr" src="' + data.paidAds[0].colombiaLogo + '" width="14" height="14"></p>';
					conObj.innerHTML = conText;
					conObj.style.display = '';
				}
			}
			lazyLoadColombia = {
			    threshold :200,
			    init:function () {
			        // console.log('init');
			        window.onscroll=function () {
			            lazyLoadColombia.onScroll();
			        }
			        
			        lazyLoadColombia.onScroll(1);
			        
			    },
			    onScroll :function (init) {
			        try {
			            if(typeof document.querySelectorAll != 'undefined') {
			                var arr = document.querySelectorAll('.list.colombiaAd');
			            } else {
			                document.getElementsByClassName = function(cl) {
                                 var retnode = [];
                                 var elem = this.getElementsByTagName('*');
                                 for (var i = 0; i < elem.length; i++) {
                                   if((' ' + elem[i].className + ' ').indexOf(' ' + cl + ' ') > -1) retnode.push(elem[i]);
                                 }
                                 return retnode;
                            };
    			            var arr = document.getElementsByClassName('list colombiaAd');
			            }
			            for(i=0; i<arr.length; i++) {
			                var cur = arr[i], curTop = cur.offsetTop, curHeight = cur.offsetHeight, winTop = (window.scrollY ? window.scrollY : document.documentElement.scrollTop), winHeight = (window.innerHeight ? window.innerHeight : document.documentElement.clientHeight), curId = cur.id, ind = curId.substring(curId.lastIndexOf("-") + 1, curId.length), th = typeof init == 'undefined' ? lazyLoadColombia.threshold : 0,
			                reqDone = (" " + cur.className + " ").replace(/[\n\t]/g, " ").indexOf(" reqDone ") > -1;
			                if(reqDone == 0 && cur.innerHTML == '' && ((curTop + curHeight - th) < (winTop + winHeight))) {
			                    cur.className = cur.className + " reqDone";
			                    lazyLoadColombia.load(curId, ind);
			                }
			            }
		            } catch (e) {
		                console.log('Error : lazyLoadColombia', e);
		            }
			    },
			    load:function (curId, ind) {
    			    // console.log('curId', curId, ind);
    			    colombia.fns.push(function(){
            			colombia.registerSlot('129117',curId,ind,0,adwidget);
        				colombia.load();
        				colombia.display(curId);
    				});
			    }
			}
			
			if(document.addEventListener) {
			    document.addEventListener('DOMContentLoaded', function() {
			        lazyLoadColombia.init();
                });
			} else if (document.attachEvent){
			    document.attachEvent('onreadystatechange', function() {
			        if(document.readyState == 'complete') {
			            lazyLoadColombia.init();
			        }
                });
			} else {
			    document.onreadystatechange = function() {
			        lazyLoadColombia.init();
                };
			}
			
			
			
