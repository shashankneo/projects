<script type="text/javascript">

YUI(_CBC_LOADER).use('cbc_core', 'cbc_config', 'node', 'gigya', 'event', function(Y, result) {

  window.printSocialCounts = function printSocialCounts(response) {
    if (!response) return;

    if (typeof response.errorCode !== "undefined" && response.errorCode != 0) {
      if (typeof response.errorMessage !== "undefined") console.log('Error :' + response.errorMessage);
        return;
      }

    if (typeof response.shareCounts === "undefined") {
      console.log('Error : No shareCounts returned.');
      return;
    }

    var total = 0;

    for (provider in response.shareCounts) {
      if (!response.shareCounts.hasOwnProperty(provider)) {
        continue;
      }
      total += response.shareCounts[provider];
    }

    Y.one(response.context).one("#totalshares").setHTML(total + (total == 1 ? " share" : " shares"));
  }

  window.onGigyaLoad = function gigyaLoadHandler(e) {
    // TODO: Raygun.
    // Add node that will hold count.
    var container = "#" + e.sourceContainerID;
    Y.one(container).one(".gig-bar-container").one("table").one("tbody").prepend("<tr><td><div id='totalshares' class='gig-button-container'></div></td></tr>");
    gigya.socialize.getProviderShareCounts({
      callback:window.printSocialCounts,
      context: container
    });
  };

  window.onSendDone = function (event) {
    if(event.providers) {
      var providers = event.providers.split(",");
      for(i = 0; i < providers.length; i++) {
        var provider = providers[i];
        var action = provider + ": Share Published";
      }
    }
  }

  if (!result.success) return;

  var conf = {
    APIKey: CBC.SETTINGS.getVal('gigyaApiKey'),
    "shortURLs": "never"
  };

  var ua = new gigya.services.socialize.UserAction();
  ua.setUserMessage("Share this CBC content");


  ua.setLinkBack("http://www.cbc.ca/1.3176326");



  // Set a proper default for the title of the page:
  // use the OG tag, or twitter tag, if available,
  // else document.title. TODO: Better in the rerelease.
  var titleNode = Y.one("meta[property='og:title']") || Y.one("meta[name='twitter:title']");
  window.socialPageTitle = titleNode && titleNode.get("content") || document.title;


ua.setTitle(window.socialPageTitle);

var cid = '';


// TODO: get customizations based on the storyCategory here
// (eg, custom Twitter recommendations).

// Include the appropriate configuration file based on the selected
// theme (as determined by the 'theme' query parameter value).

  var verticalShareButtons = [
    {
        provider: "facebook",
        iconImgUp: "/i/img/theme/default/facebook-up.png",
        iconImgOver: "/i/img/theme/default/facebook-over.png"
    },
    {
        provider: "twitter",
        iconImgUp: "/i/img/theme/default/twitter-up.png",
        iconImgOver: "/i/img/theme/default/twitter-over.png"
    },
    {
        provider: "reddit",
        iconImgUp: "/i/img/theme/default/reddit-up.png",
        iconImgOver: "/i/img/theme/default/reddit-over.png"
    },
    {
        provider: "Googleplus",
        annotation: "bubble",
        iconImgUp: "/i/img/theme/default/plus-up.png",
        iconImgOver: "/i/img/theme/default/plus-over.png",
        showCounts: "right",
        size: "tall"
    },
    {
        provider: "share",
        iconImgUp: "/i/img/theme/default/share-up.png",
        iconImgOver: "/i/img/theme/default/share-over.png"
    },
    {
        provider: "email",
        iconImgUp: "/i/img/theme/default/email-up.png",
        iconImgOver: "/i/img/theme/default/email-over.png"
    }
];



var horizontalShareButtons = verticalShareButtons;

var params = {
    userAction: ua,
    cid: cid,
    onSendDone: window.onSendDone,
    onLoad: window.onGigyaLoad,
    iconsOnly: false,
    showCounts:'right',
    noButtonBorders: true
};

// TODO: Better test for mobile?
if( /Android|webOS|iPhone|iPod|BlackBerry/i.test(navigator.userAgent) ) {
    params.deviceType = "mobile";
}

if(Y.one("#sharecount"))
{
    params.layout = "vertical";
    params.shareButtons = verticalShareButtons;
    params.showCounts = "none";
    params.containerID = "sharecount";

    gigya.services.socialize.showShareBarUI(conf, params);
}

if(Y.one("#sharecounttwo"))
{
    params.layout = "vertical";
    params.shareButtons = verticalShareButtons;
    params.showCounts = "none";
    params.containerID = "sharecounttwo";

    gigya.services.socialize.showShareBarUI(conf, params);
}

if (Y.one("#sharetools"))
{
    params.layout = "horizontal";
    params.shareButtons = horizontalShareButtons;
    params.showCounts = "none";
    params.iconsOnly = true;
    params.onLoad = null;
    params.containerID = "sharetools";

    gigya.services.socialize.showShareBarUI(conf, params);
}

if (Y.one("#sharetoolstwo"))
{
    params.layout = "horizontal";
    params.shareButtons = horizontalShareButtons;
    params.showCounts = "none";
    params.iconsOnly = true;
    params.onLoad = null;
    params.containerID = "sharetoolstwo";

    gigya.services.socialize.showShareBarUI(conf, params);
}

});
</script>

