var currentCP2540612 = 0,
    maxCP2540612;

function sizeCarousel2540612() {
	var h = 0;
	$("li", "#videoCarousel-2540612").each(function() {
		if(this.offsetHeight > h) {
			h = this.offsetHeight;
		}	
	});
	if(h) {	
		$("#videoCarousel-2540612").css("height", (h + 20) + "px");
	}
};
function showCarousel2540612() {
	$("#videoCarousel-2540612").animate({'bottom' : 0}, {queue:false, duration:500});	
};
function hideCarousel2540612() {
	$("#videoCarousel-2540612").animate({'bottom' : -($("#videoCarousel-2540612").height() + 20)}, {queue:false, duration:500});	
	return false;
};	
function slideCarousel2540612(e) {
	var inc = e.target.id == "carouselPP-2540612" ? -1 : 1;
	
	currentCP2540612 += inc;
	
	if(currentCP2540612 < 0) {
		currentCP2540612 = maxCP2540612 - 1;
	} else if(currentCP2540612 >= maxCP2540612) {
		currentCP2540612 = 0;
	}
	
	$("ul", "#videoCarousel-2540612").animate({'left' : -(currentCP2540612 * $("#en-video-2540612-wrap").width())}, {queue:false, duration:500});
	
	return false; 
};    
$(document).ready(function() {
	$("#videoCarousel-2540612").css({"display":"block"});
	maxCP2540612 = Math.ceil($("li", "#videoCarousel-2540612").length / 4);
	if(maxCP2540612 > 1) {
		$("#videoCarousel-2540612").append('<a href="#" class="carouselPP" id="carouselPP-2540612">Previous</a><a href="#" class="carouselNP" id="carouselNP-2540612">Next</a><a href="#" class="carouselCL" id="carouselCL-2540612"><img src="http://static.euronews.com/projects/embed/close-icon.png" width="20" height="20" alt="close carousel" /></a>');
		$("#carouselPP-2540612, #carouselNP-2540612").click(slideCarousel2540612);		
		$("#carouselCL-2540612").click(hideCarousel2540612);
	};
	hideCarousel2540612();
});	  
</script>
