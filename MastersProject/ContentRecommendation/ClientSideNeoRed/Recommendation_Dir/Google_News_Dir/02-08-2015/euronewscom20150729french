    function callAdvert(position, showMention)
    {
    showMention = typeof showMention !== 'undefined' ? showMention : true;
    if("size" in dblClick && "host" in dblClick && "zone" in dblClick)
    {
    if(position!=undefined && position in dblClick.size)
    {
    googletag.cmd.push(function() {
    googletag.defineSlot(dblClick.host+dblClick.zone+'_'+position, dblClick.size[position], 'adzone-'+position)
    .addService(googletag.pubads())
    .setTargeting("lng",dblClick.lang)
    .setTargeting("kw",dblClick.keyw)
    .setTargeting("tags",dblClick.tagsV2)
    .setTargeting("!c",dblClick.exct);

    googletag.pubads().collapseEmptyDivs();                                     
    googletag.enableServices();
    googletag.display('adzone-'+position);                                                                
    });
    if(position != undefined && position in dblClick.mention && showMention == true)
    {
    document.write(dblClick.mention[position]);

    }
    }
    };
    };
    



    function callNA(position, sponsor)
    {
    showMention = typeof showMention !== 'undefined' ? showMention : true;
    if("size" in dblClick && "host" in dblClick && "zone" in dblClick)
    {
    if(position!=undefined && position in dblClick.size)
    {
    googletag.cmd.push(function() {
    googletag.defineSlot(dblClick.host + position, dblClick.size[position], 'adzone-'+position)
    .addService(googletag.pubads())
    .setTargeting("lng",dblClick.lang)
    .setTargeting("sponsor",sponsor);
    

    googletag.pubads().addEventListener('slotRenderEnded', function(event) {
        if(!event.isEmpty && event.slot.i.indexOf(position) != -1) {
        
            $('#'+position+'_save').css('display','none'); 
        }
    });

    googletag.pubads().collapseEmptyDivs();   

    googletag.enableServices();

    
    googletag.display('adzone-'+position);                                                                
    });
    if(position != undefined && position in dblClick.mention && showMention == true)
    {
    document.write(dblClick.mention[position]);

    }

    }
    };
    };
    

    
    
