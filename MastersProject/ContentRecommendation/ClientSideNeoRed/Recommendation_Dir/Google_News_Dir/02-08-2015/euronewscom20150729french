                            try {
                                (function() {
                                    var documentBody = document.getElementsByTagName("body")[0],
                                            sel = document.getElementById("en_page");
                                    if (documentBody) {
                                        documentBody.className += " js-active";
                                    }
                                    ;
                                    if (sel) {
                                        sel.onchange = function(e) {
                                            var langCode = this.options[this.selectedIndex].lang,
                                                    v = this.value;
                                            if (langCode)
                                                EN.cookie.createCookie("EN_lang", langCode, 365);
                                            EN.weather.updateWeatherBlockCache(false);
                                            setTimeout(function() {
                                                window.location = v;
                                            }, 100);
                                        };
                                    };
                                })();

                                $(document).ready(function() {
                                    $("#searchRow form").submit(function() {
                                        if ($("#searchRow #google-inp").val() == $("#searchRow #google-sub").val() || $("#searchRow #google-inp").val() == "") {
                                            $("#searchRow #google-inp").focus();
                                            return false;
                                        } else {
                                            $("#header #searchRow .searchField .searchButton").addClass("loading");

                                        }
                                    });
                                    $("#searchRow #google-inp").attr("value", $("#searchRow #google-sub").val());
                                    $("#searchRow #google-inp").removeAttr("placeholder");
                                    $("#searchRow #google-inp").focus(function() {
                                        $("#header #searchRow .searchField").addClass("focus");
                                        if ($(this).val() == $("#searchRow #google-sub").val()) {
                                            $(this).val("");
                                        }
                                    });
                                    $("#searchRow #google-inp").blur(function() {
                                        $("#header #searchRow .searchField").removeClass("focus");
                                        if ($(this).val() == "") {
                                            $(this).val($("#searchRow #google-sub").val());
                                        }
                                    });

                                });
                            } catch (err) {
                            };
                        </script>
