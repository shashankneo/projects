    <script  type="text/javascript">
        function startHighlight(node, replacements) {
            for (var i = 0, rep; rep = replacements[i]; i++) {
                highlightWord(node, rep);
            }
            ;
        }
        ;
        function highlightWord(node, rep) {

            if (highlightWord[rep[0]] == 1) {
                return;
            }
            ;

            if (node.hasChildNodes) {
                for (var hi_cn = 0; hi_cn < node.childNodes.length; hi_cn++) {
                    highlightWord(node.childNodes[hi_cn], rep)
                }
                ;
            }
            ;

            if (node.nodeType == 3) {
                var tempNodeVal = node.nodeValue.toLowerCase(),
                        tempWordVal = String(rep[0]).toLowerCase();

                if (tempNodeVal.indexOf(tempWordVal) != -1) {
                    var pn = node.parentNode;
                    if (pn.className != "searchword" && pn.tagName != "NOSCRIPT" && pn.tagName != "A") {
                        var nv = node.nodeValue,
                                ni = tempNodeVal.indexOf(tempWordVal),
                                before = document.createTextNode(nv.substr(0, ni)),
                                docWordVal = nv.substr(ni, tempWordVal.length),
                                after = document.createTextNode(nv.substr(ni + tempWordVal.length)),
                                hiwordtext = document.createTextNode(docWordVal),
                                hiword = document.createElement("a"),
                                bT = nv.substr(0, ni),
                                eT = nv.substr(ni + tempWordVal.length);

                        if ((bT.length && bT.charAt(bT.length - 1) != " ") || (eT.length && eT.charAt(0).search(/[\s,\.!\?]/) == -1)) {
                            return;
                        }
                        ;

                        hiword.appendChild(hiwordtext);
                        hiword.title = rep[2];
                        hiword.href = rep[1];
                        pn.insertBefore(before, node);
                        pn.insertBefore(hiword, node);
                        pn.insertBefore(after, node);
                        pn.removeChild(node);
                        highlightWord[rep[0]] = 1;

                    }
                }
            }
        }
        ;
        
        startHighlight(document.getElementById('articleTranscript'),[["European Union","\/tag\/european-union\/","More about: European Union"],["Foreign policy","\/tag\/foreign-policy\/","More about: Foreign policy"],["France","\/tag\/france\/","More about: France"],["Iran","\/tag\/iran\/","More about: Iran"],["Nuclear Energy","\/tag\/nuclear-energy\/","More about: Nuclear Energy"],["Tehran","\/tag\/tehran\/","More about: Tehran"]]);
        $("#articleTranscript a").attr("target","_blank");
    </script>

  
  
