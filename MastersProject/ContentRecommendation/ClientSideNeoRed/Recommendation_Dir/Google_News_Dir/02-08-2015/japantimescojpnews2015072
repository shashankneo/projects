								<li>
									<a class="icon-printer" href="javascript:window.print()"><span>PRINT</span></a>
								</li>
								<li>
									<!-- AddThis Button BEGIN -->
									<a class="addthis_button" href="http://www.addthis.com/bookmark.php?v=300&amp;pubid=jtimes">
										<span class="icon-share"></span>SHARE
									</a>
									
									<script type="text/javascript">var addthis_config = {"data_track_addressbar":true};</script>
									<script type="text/javascript" src="http://s7.addthis.com/js/300/addthis_widget.js#pubid=jtimes"></script>
									<!-- AddThis Button END -->
								</li>
								<li class="post_details">
																	</li>
