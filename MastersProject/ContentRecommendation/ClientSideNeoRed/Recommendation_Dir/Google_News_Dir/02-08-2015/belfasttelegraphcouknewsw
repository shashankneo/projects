    <p class="lead">
                            Dozens of topless women and men staged a Bare With Us rally in Canada meant to educate the public about women's right to go shirtless if they choose.</p><div class="widget ad google" >
        <script type="text/javascript">

            function google_ad_request_done(google_ads) {

                if (google_ads.length == 0) {
                    return;
                }

                var i;
                var s = '<div class="sense-content-a">';

                /*
                 * For text ads, append each ad to the string.
                 */
                for(i = 0; i < google_ads.length; i++) {
                    s += '<div class="item">';
                    s += '<h4><a href="' + google_ads[i].url + '" rel="external">' + google_ads[i].line1 + '</a></h4>';
                    if(typeof(google_ads[i].line3) != "undefined"){
                        s += '<p>' + google_ads[i].line2 + ' ' + google_ads[i].line3 + '</p>';
                    }else{
                        s += '<p>' + google_ads[i].line2 + '</p>';
                    }
                    s += '<p class="link"><a href="' + google_ads[i].url + '" rel="external">' + google_ads[i].visible_url + '</a></p>';
                    s += '</div>';
                }

                s += '<h3><a href=\"' + google_info.feedback_url + '\">Ads by Google</a></h3>';


                if (google_ads[0].bidtype == "CPC") {
                    google_adnum = google_adnum + google_ads.length;
                }

                s += '</div>';
                document.write(s);
                return;
            }

            window.google_adnum = window.google_adnum || null;

            google_ad_client = "ca-pub-1729148518344007";
            google_ad_output = "js";
            google_ad_type = "text";

            
            google_ad_channel = '2905088926+3455374145';
            
            google_max_num_ads = '2';
            

            google_skip = window.google_adnum; /* insert this snippet for each ad call */

        </script>

