                    head.ready(function() {
                        var EventHandler = com.inmplc.EventHandler;

                        EventHandler.addHandler('loggedIn', function(e) {
                            getLivefyreToken(function(token) {
                                fyre.conv.login(token);
                            });
                        });

                        EventHandler.addHandler('loggedOut', function(e) {
                            fyre.conv.logout();
                        });
                    });

                    authDelegate.login = function (handlers) {

                        var handleSuccessfulLoginEvent = function(token) {
                            fyre.conv.login(token);
                            handlers.success();
                        };

                        getLivefyreToken(
                                handleSuccessfulLoginEvent,
                                function() {
                                    evolok.showCommentsLoginOverlay({
                                        onCompleted: function() {
                                            getLivefyreToken(
                                                    handleSuccessfulLoginEvent,
                                                    function() {
                                                        handlers.failure();
                                                    }
                                            );
                                        },
                                        onClosed: function() {
                                            handlers.failure();
                                        }
                                    });
                                }
                        );

                        return true;
                    };

                    authDelegate.logout = function (handlers) {
                        handlers.success();

                        evolok && evolok.logout && evolok.logout();

                        return true;
                    };
                    authDelegate.viewProfile = function () {};
                    authDelegate.editProfile = function () {};

                    var livefyreCallback = function(app){
                        try{
                            app.on('commentCountUpdated', function(count) {
                                onLivefyreCommentCountUpdated && onLivefyreCommentCountUpdated(count);
                            });

                            app.on('initialRenderComplete', function() {
                                attachRegisterOption();
                                getLivefyreToken(fyre.conv.login);
                            });

                            app.on('userLoggedIn', function() {
                                detachRegisterOption();
                            });

                            app.on('userLoggedOut', function() {
                                attachRegisterOption();
                            });
                        }
                        catch(error){
                            $.error("Livefyre failed to load", error);
                        }
                    };

