	if(searchInputUnselectedText != jQuery("div#localedge_integratedSearch div.localedge_header span.localedge_buttons input.localedge_searchInput").val() )
	{
		jQuery("div#localedge_integratedSearch div.localedge_header span.localedge_buttons input.localedge_searchInput").addClass("localedge_focus");
	}
	
	jQuery("div#localedge_integratedSearch div.localedge_header span.localedge_buttons input.localedge_searchInput").focus( function() {
		if(searchInputUnselectedText == jQuery(this).val())
		{ 
			jQuery(this).val("");
			jQuery(this).addClass("localedge_focus");
		}
	});
	jQuery("div#localedge_integratedSearch div.localedge_header span.localedge_buttons input.localedge_searchInput").blur( function() {
		if("" == jQuery(this).val())
		{ 
			jQuery(this).val(searchInputUnselectedText);
			jQuery(this).removeClass("localedge_focus");
		}
	} );
});
</script>
</div>
            <!-- src/business/widgets/hearst/collection/widget.tpl -->
