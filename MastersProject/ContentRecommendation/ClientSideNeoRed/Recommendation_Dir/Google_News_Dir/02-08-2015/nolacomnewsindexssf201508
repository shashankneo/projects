   AdvAsyncLoader.require('/static/common/js/galleria/galleria.js', function(){
    AdvAsyncLoader.require('/static/common/js/galleria/gallery_share.js', function(){
     AdvAsyncLoader.require('/static/common/js/galleria/jquery.mCustomScrollbar.js', function(){
      AdvAsyncLoader.require('/static/common/js/galleria/adv_galleria_settings.js', function(){
       initGallery();
      });
     });
    });
   });
  });
 } else {
  AdvAsyncLoader.require('/static/common/js/galleria/finger.js', function(){
   AdvAsyncLoader.require('/static/common/js/galleria/galleria.js', function(){
    AdvAsyncLoader.require('/static/common/js/galleria/plugins/history/galleria.history.js', function(){
     AdvAsyncLoader.require('/static/common/js/galleria/gallery_share.js', function(){
      AdvAsyncLoader.require('/static/common/js/galleria/jquery.mCustomScrollbar.js', function(){
       AdvAsyncLoader.require('/static/common/js/galleria/adv_galleria_settings.js', function(){
        initGallery();
       });
      });
     });
    });
   });
  });
 }
</script>
