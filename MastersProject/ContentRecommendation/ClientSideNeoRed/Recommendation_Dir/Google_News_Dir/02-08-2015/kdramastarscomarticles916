<script type="text/javascript">
$('input[type="button"]').click(function(){
	if($(this).val() == 'Subscribe'){$.ajax({url:'/home/news/newsletter/mailchimp_insert.php',type:'post',data:{email:$('#nmail').val()}}).done(function(e){  $('.rword').html(e);  $("#rword a").css("display","block"); });}
});
if(!$.cookie('nl_popup')){
	$('.wh_transf').show();
	$.cookie('nl_popup', '1', { path: '/', domain: 'kdramastars.com', expires: 1 });
	$('html').on("click",function() { $('.wh_transf').hide(); });
	$('.subscribe_popup').click(function(event){ event.stopPropagation(); });
	$('.close').click(function(){ $('.wh_transf').hide(); return false; })
}
	$("#donotshow").click(function(){
		$.cookie('nl_popup', '1', { path: '/', domain: 'kdramastars.com', expires: 7 });
		$('.wh_transf').hide(); return false; 
	});
</script>

