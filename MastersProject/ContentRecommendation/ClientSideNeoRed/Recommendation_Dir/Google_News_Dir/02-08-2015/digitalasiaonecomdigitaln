//ajax - xt_at to be dynamically fetched
(function($) {
  xt_at = "";

  $.ajax({
    type: 'POST',
    url: 'http://digital.asiaone.com/sites/all/libraries/analytics/atinternet/include/header_details.php',
    dataType: 'json',
    cache: true,
    async: false,
    beforeSend: function(){},
    success: function(data) {
      if (data.HTTP_W !== null && data.HTTP_W.length > 1) {
        xt_at = data.HTTP_W;
        var meta = document.createElement('meta');
        meta.name = 'ldapw';
        meta.content = data.HTTP_W;
        document.getElementsByTagName('head')[0].appendChild(meta);
      }
      else {
        xt_at = "";
      }
    },
    complete:function() {},
    error:function(xhr, status, error) {}
  });

  var script = document.createElement("script");
  script.type = "text/javascript";
  script.text = "xtnv = document;\n" +
    "xtsd = 'http://logw348';\n" +
    "xtsite = '541671';\n" +
    "xtn2 = '7'; // level 2 site ID\n" +
    "xtpage = 'News::Glimmers_emerge_on_Apple_Watch_sales_and_theyre_not_pretty'; //page name (with the use of :: to create chapters)\n" +
    "xtdi = ''; //implication degree\n" +
    "xt_multc = '&x1=Glimmers_emerge_on_Apple_Watch_sales_and_theyre_not_pretty&x2=http%3A%2F%2Fdigital.asiaone.com%2Fdigital%2Fnews%2Fglimmers-emerge-apple-watch-sales-and-theyre-not-pretty&x3=1&x4=1&x5=klim%40a1&x6=1&x7=1&x9=213785&x8=" + window.lotame_id + "';   //all the xi indicators (like '&x1=...&x2=....&x3=...')\n" +
    "xt_at = '" + xt_at + "'; //user ID\n" +
    "xt_ac = ''; //category ID\n" +
    "if (window.xtparam!=null){window.xtparam+='&ac='+xt_ac+'&at='+encodeURIComponent(xt_at)+xt_multc;}\n" +
    "else{window.xtparam='&ac='+xt_ac+'&at='+encodeURIComponent(xt_at)+xt_multc;};\n";

  // Append to body
  document.body.appendChild(script);

  $("body").append("<noscript>" +
    "<img width='1' height='1' alt='' src='http://logw348.ati-host.net/hit.xiti?s=541671&s2=7&p=News::Glimmers_emerge_on_Apple_Watch_sales_and_theyre_not_pretty&di=&at="+encodeURIComponent(xt_at)+"&ac=&x1=Glimmers_emerge_on_Apple_Watch_sales_and_theyre_not_pretty&x2=http%3A%2F%2Fdigital.asiaone.com%2Fdigital%2Fnews%2Fglimmers-emerge-apple-watch-sales-and-theyre-not-pretty&x3=1&x4=1&x5=klim%40a1&x6=1&x7=1&x9=213785&x8=" + window.lotame_id + "'>" +
    "</noscript>");
