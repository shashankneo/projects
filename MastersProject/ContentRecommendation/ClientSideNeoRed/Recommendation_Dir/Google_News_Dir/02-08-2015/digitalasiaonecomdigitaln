<script type="text/javascript">
var $ = jQuery;
$(document).ready(function() {
	/** Clicks **/
	/* Navigation */
	$("#digitalmenu a").on("click", function() {
		var elem = $(this);

		xt_click(
			$(this).get(0), // jQuery version of DOM element this
			'C', // Click
			'2', // Level 2 ID
			'Navigation::'+clean_text(elem.text()),
			'N' // Navigation
		);
		return false;
	});
	/* AsiaOne Navigation */
	$("#digi_topnav a").on("click", function() {
		var elem = $(this);

		xt_click(
			$(this).get(0), // jQuery version of DOM element this
			'C', // Click
			'2', // Level 2 ID
			'Navigation::'+clean_text(elem.text()),
			'N' // Navigation
		);
		return false;
	});
	/* Footer */
	// About us, Contact us, Advertise, Terms of use
	$("#footer_lft").find("a").on("click", function() {
		xt_click(
			$(this).get(0), // jQuery version of DOM element this
			'C', // Click
			'2', // Level 2 ID
			'Footer::'+clean_text($(this).text()),
			'N' // Navigation
		);
		return false;
	});
	// navigation
	$("#footer_lft_bot .a1_lft1").find("a").on("click", function() {
		xt_click(
			$(this).get(0), // jQuery version of DOM element this
			'C', // Click
			'2', // Level 2 ID
			'Footer::'+clean_text($(this).text()),
			'N' // Navigation
		);
		return false;
	});
	$(".footer_social").find("a").on("click", function() {
		var href = $(this).attr("href");
		var title = "";

		if (-1 !== href.indexOf("facebook")) {
			title = "Facebook";
		}
		else if (-1 !== href.indexOf("twitter")) {
			title = "Twitter";
		}
		else if (-1 !== href.indexOf("rss")) {
			title = "RSS";
		}

		xt_click(
			$(this).get(0), // jQuery version of DOM element this
			'C', // Click
			'2', // Level 2 ID
			title,
			'A' // Action
		);
		return false;
	});

	function clean_text(text) {
		return encodeURIComponent($.trim(text));
	}
});
