			  <script>
			  function loginEventHandler(eventObj) {
			  	    gigya.socialize.getUserInfo({callback:printDashboard});
 					gigya.socialize.getUserInfo({callback:printLoginNote});
				}
			  function logoutEventHandler(eventObj) {
			  	    gigya.socialize.getUserInfo({callback:printDashboard});
 					gigya.socialize.getUserInfo({callback:printLoginNote});
				}				
			  	gigya.socialize.getUserInfo({callback:printDashboard});
				gigya.accounts.addEventHandlers({
					onLogin:loginEventHandler,
					onLogout:logoutEventHandler
			   	});
    		function printDashboard(response) { 
			      var user = response['user'];
			      var dashboard_html='';
    			  if ( user['UID'] != 0 ) {  
    			  	if (user['nickname'] !=''){
    			  		var username=user['nickname'];
    			  	}else if (user['firstName'] != ''){
    			  		if (user['lastName'] != ''){
    			  			var username=user['firstName']+' '+user['lastName'];
    			  		}else{
    			  			var username=user['firstName'];
    			  		}
    			  	}else{
    			  		var username=user['email'];
    			  	}
    			  	dashboard_html='<div class="username-logout">';
    			  	dashboard_html+='<a href="#" onclick="gigya.accounts.showScreenSet({screenSet:\'Default-ProfileUpdate\',startScreen:\'gigya-update-profile-screen\'})">Welcome, '+ username +'</a>&nbsp;&nbsp;|&nbsp;&nbsp;';
    			  	dashboard_html+='<a href="#" onclick="gigya.accounts.logout({callback:dashRefresh})">Log Out</a>&nbsp;&nbsp;|&nbsp;&nbsp;';
    			  	dashboard_html+='<a href="http://onlineathens.com/subscribe/" class="wl-dash" id="wl-dash-subscribe" title="Subscribe">Subscribe</a><br/>';
    			  	dashboard_html+='</div>';
    			  } else {
    			    dashboard_html='<div class="username-login">';
    			    dashboard_html+='<a href="#" onclick="gigya.accounts.showScreenSet({screenSet:\'Default-RegistrationLogin\',startScreen:\'gigya-login-screen\',onAfterSubmit:\'dashRefresh\'})">Log in to comment</a>';
    			  	dashboard_html+='&nbsp;|&nbsp;';
    			  	dashboard_html+='<a href="#" onclick="gigya.accounts.showScreenSet({screenSet:\'Default-RegistrationLogin\',startScreen:\'gigya-register-screen\',onAfterSubmit:\'dashRefresh\'})">Join Now!</a>';
    			  	dashboard_html+='&nbsp;|&nbsp;';
    			  	dashboard_html+='<a href="http://onlineathens.com/subscribe/" class="wl-dash" id="wl-dash-subscribe" title="Subscribe">Subscribe</a><br/>';    			  	
    			  	dashboard_html+='</div>';
    			  }
    			  document.getElementById('gigyaDash').innerHTML=dashboard_html;


			  }
			  function dashRefresh(eventObj){
			  	gigya.socialize.getUserInfo({callback:printDashboard});
			  }
			  </script>			  

