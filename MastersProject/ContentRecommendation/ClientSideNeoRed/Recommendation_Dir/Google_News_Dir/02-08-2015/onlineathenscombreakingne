            <div class="subscription_form">
					<script>
						function valandadd() {
							
							
							var email = document.getElementById("contacology_mail").value;
							var atpos = email.indexOf("@");
							var dotpos = email.lastIndexOf(".");
							if(email == ''){
								alert("Email address field is empty");
								return false;
							}
							if (atpos< 1 || dotpos<atpos+2 || dotpos+2>=email.length) {
								alert("Not a valid e-mail address");
								return false;
							}
							
							
							var x = document.getElementById("categorys").value;
							if(x == 'headlines'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="4">';
							}
							if(x == 'sports'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="48">';
							}
							if(x == 'entertainment'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="50">';
							}
							if(x == 'lifestyle'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="52">';
							}
							if(x == 'jobs'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="17">';
							}
							if(x == 'auto'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="54">';
							}
							if(x == 'deals'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="16">';
							}
							if(x == 'shopping'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="56">';
							}
							if(x == 'estate'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="58">';
							}
							if(x == 'obituaries'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="60">';
							}
							if(x == 'horoscopes'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="62">';
							}
							if(x == 'slideshows'){
								document.getElementById("list").innerHTML = '<input type="hidden" name="lid" value="64">';
							}
						}
						
						
					</script>
					
				  
