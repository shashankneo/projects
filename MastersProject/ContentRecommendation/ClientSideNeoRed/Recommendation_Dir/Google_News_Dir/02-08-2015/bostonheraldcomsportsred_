    <div class="node-metainfo clearfix">
      <!-- Published At Date -->
      <time itemprop="datePublished" datetime="2015-08-01T20:30:00-04:00" class="field field-name-field-published-at field-type-datestamp field-label-hidden">Saturday, August 1, 2015</time>
      <div class="node-action">
        <!-- Print/Email -->
