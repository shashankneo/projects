<!-- AFC Football Redirect Handler -->
<!-- Can be removed after AFC championship 2015 -->
<script>
$(function() {
    // in case cover content needs the redirect attributes
    $( "#coverList .content a" ).each(function() {
        var href = $(this).prop('href');
        if ((href.indexOf('nfl\/video\/afc-playoffs') > -1) || (href.indexOf('video\/player\/afc-playoffs') > -1)){
            if ($(this).data('redirect') == null){
                $(this).attr("data-redirect","/nfl/gametracker/NFL_20150118_IND@NE");
            }
        }
    });
    /*
    // in case dropdown content needs the redirect attributes
    $( ".navMenuContent .topStory a" ).each(function() {
        var href = $(this).prop('href');
        if ((href.indexOf('nfl\/video\/afc-playoffs') > -1) || (href.indexOf('video\/player\/afc-playoffs') > -1)){
            if ($(this).data('redirect') == null){
                $(this).attr("data-redirect","/nfl/gametracker/NFL_20150111_IND@DEN");
            }
        }
    });
    */
    // swap out the redir data element for a click event
    $('body').delegate('a[data-redirect]', 'click', function(e) {
        e.preventDefault();
        var ua = navigator.userAgent.toLowerCase();
        if (ua.indexOf('iphone') > -1 || (ua.indexOf('android') > -1 && ua.indexOf('mobile') > -1)){
            window.location.href = "http://mweb.cbssports.com/nfl/afc-playoffs";
        }
        else{
            if (ua.indexOf('android') > -1){
                window.location.href = "http://www.cbssports.com/nfl/afc-playoffs-support";
            }
            else{
                var redirectLink = $(this).data('redirect');
                redirectLink = redirectLink.replace("gametracker/NFL","gametracker/live/NFL");
                var redirectNoBita = redirectLink + "?bita_force_off=1";
                var href = $(this).prop('href');
                openPopup(href, 'afcplayoffs', 722, 463);
                console.log(redirectNoBita);
                window.location.href = redirectNoBita;
            }
        }
    });
});
</script>
