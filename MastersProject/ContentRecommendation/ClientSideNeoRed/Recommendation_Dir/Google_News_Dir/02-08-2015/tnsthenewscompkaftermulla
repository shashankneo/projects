<script type="text/javascript">
function search_post()
{
	var writer = jQuery('#writers').val();
	var title_search = jQuery('#s').val();

	var writer_query = '';
	var title_search_query = '';
	if(writer != '')
	{
		writer_query = '?writers='+writer;
	}
	if(title_search != '')
	{
		var parm = '&';
		if(writer_query == '')
		{
			parm = '?';
		}
		title_search_query = parm+'s='+title_search;
	}

	var serachUrl = 'http://tns.thenews.com.pk/'+writer_query+title_search_query;
	window.location.href = serachUrl;
}
function filter_search_author()
{
	var writer = jQuery('#writers').val();
	if(writer != '')
	{
		writer_query = '?writers='+writer;
		var serachUrlAuthor = 'http://tns.thenews.com.pk/'+writer_query;
		window.location.href = serachUrlAuthor;
	}

}
</script>
