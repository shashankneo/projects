</style><script>$(function(){

    //- NAV
    var dirName = location.pathname.split('/')[1],
        friendlyName = dirName.replace(/-/g, ' ').toLowerCase(),
        $activeLink;
    //- Activate the currently selected section if there is one
    if (dirName) {
        //- some links have # so you can't use href
        $activeLink = $('#sd-nav >li >a[href]').filter(function () {
            return $(this).data('name').toLowerCase() === friendlyName;
        }).first();
    } else {
        $activeLink = $('#sd-nav >li >a[href]').first();
    }

    $activeLink.closest('li').addClass('active');

    //- set current page class
    $('nav a[href^="/' + dirName + '"]').addClass('sd-nav-current-section');

    //- toggle mobile nav menu
    $('#sd-mobile-nav-toggle').on('click', function() {
        $('.sd-header-nav').toggleClass('mobile-nav-open');
    });

    //- toggle mobile subnav menu
    $('.sd-mobile-subnav-toggle').on('click', function() {
        var $this = $(this),
            $item = $this.parent(),
            isOpen = $item.hasClass('sd-mobile-subnav-open');

        $('.sd-mobile-subnav-open').removeClass('sd-mobile-subnav-open');

        if(!isOpen){
            $item.addClass('sd-mobile-subnav-open');
        }
    });

    $('.sd-nav >li').each(function(){
        var subNav = $(this).children('.sd-subnav'),
            subNavLength = subNav.children('li').length;

        if (subNavLength >= 16) {
            subNav.addClass('sd-subnav-three-column');
        } else if (subNavLength >= 10) {
            subNav.addClass('sd-subnav-two-column');
        }
    });
    //- LIVESTREAM
    //- Get livestream
    var liveStreamTimeout;
    function livestreamTimer(){
        clearTimeout(liveStreamTimeout);
        liveStreamTimeout = setTimeout(function(){
            getLivestream();
        }, 60000);
    }

    //- Custom for WJLA NC8 Only
    function getSecondaryLiveStream(){
        $.ajax({
            url : '//livevideostatus.sinclairstoryline.com/status/NC8' ,
            success : function(data) {
                if (data.isLive) {
                    $(".watch-live").attr('href','/newschannel-8');
                    $("#sd-watch-live-container-header").addClass('stream-is-live').attr('data-station','NC8');
                    $("#sd-watch-live-container-hero").addClass('stream-is-live');
                } else {
                    
                    $("#sd-watch-live-container-header").removeClass('stream-is-live');
                    $("#sd-watch-live-container-hero").removeClass('stream-is-live');
                }
                livestreamTimer();
            }
        });
    }
    function getLivestream() {
        $.ajax({
            url : '//livevideostatus.sinclairstoryline.com/status/KUTV' ,
            success : function(data) {
                if (data.isLive) {
                    $(".watch-live").attr('href','/live');
                    $("#sd-watch-live-container-header").addClass('stream-is-live').attr('data-station','KUTV');
                    $("#sd-watch-live-container-hero").addClass('stream-is-live');
                    livestreamTimer();
                } else if('KUTV' ==='WJLA') {
                    getSecondaryLiveStream();
                } else {
                    livestreamTimer();
                }
            }
        });
        

    }
    getLivestream();



    //- SEARCH

    // Define search related elements
    var headerSearch = $('.sd-header-search'),
        headerMobileSearch = $('.sd-mobile-search-widget'),
        searchInput = $('.sd-header-search input'),
        searchButton = $('.sd-header-search button'),
        searchMobilePane = $('.sd-mobile-search-pane'),
        searchMobileInput = $('.sd-mobile-search-widget input'),
        searchMobileButton = $('.sd-mobile-search-widget button'),
        searchMobileCloseBtn = $('.sd-mobile-search-close'),
        searchTimeout;


    // DESKTOP SEARCH WIDGET

    // Cause clicks elsewhere on the page to close expanded search field
    $('html').click(function(e) {
        headerSearch.trigger('retract');
        headerMobileSearch.trigger('retract');
    });
    // Stop search field clicks from closing search field
    headerSearch.click(function(e){
        e.stopPropagation();
    });
    headerMobileSearch.parent().parent().click(function(e){
        e.stopPropagation();
    });

    // Handle desktop search expand behavior
    headerSearch.on('expand', function() {
        searchButton.data('search-expanded', 'true');
        searchInput.parent().animate({
            width: '295px'
        });
        searchInput.focus();
    });

    // Handle desktop search retract behavior
    headerSearch.on('retract', function() {
        searchButton.data('search-expanded', 'false');
        searchInput.parent().animate({
            width: '45px'
        });
    });

    // Handle searchInput typing for typeahead results and capturing enter key to trigger search
    searchInput.keyup(function(e){
        // Reset typeahead timer
        window.clearTimeout(searchTimeout);

        // Check minimum length of search for typeahead query
        //- if( e.currentTarget.value.length >= 3 ) {
        //-     searchTimeout = window.setTimeout(function() {
        //-         $.ajax({
        //-             url: "/api/rest/audience/search?find=" + searchInput.val()
        //-         })
        //-         .done(function(data, status, xhr) {
        //-             //- console.log('find data', data);
        //-             $.each(data.data, function() {
        //-                 var self = this;
        //-                 //- console.log(this);
        //-                 var liEl = $("<li />").addClass('teaser-list-item col-xxs-24'),
        //-                     aEl = $("<a />").attr("href", self.url).appendTo(liEl),
        //-                     imgEl = $("<img />").attr("src", self.medium36x25Url).appendTo(aEl),
        //-                     titleEl = $("<span />").addClass("title").text(self.title).appendTo(aEl);
        //-                     summaryEl = $("<p />").addClass("summary").text(self.summary).appendTo(aEl);
        //-
        //-                 if(self.medium36x25Url) {
        //-                     //- $('.component-header-default_with_weather-v1 ul.quick-result .row').append(liEl);
        //-                 }
        //-             });
        //-         });
        //-     }, 500);
        //- }

        // Trigger search event if enter key is pressed
        if( e.keyCode == 13 && searchInput.val().length >0 ) {
            searchButton.click();
        }
    });

    // Handle contextual search button behavior
    searchButton.click(function(e){
        // Stop typeahead timer
        window.clearTimeout(searchTimeout);

        // Check window width to determine mobile/tablet or desktop behavior
        if( window.innerWidth >= 1025 ) {
            headerMobileSearch.trigger('retract');
            // If search field is open on desktop, send search query
            if( searchButton.data('search-expanded') == 'true' ) {
                if( searchInput.val().length >0 ) {
                    window.location = "/search?find=" + searchInput.val();
                    return;
                }
            } else {
            // If search field is closed on desktop, open it
                headerSearch.trigger('expand');
                return;
            }
        } else {
            // If search field is open on mobile/tablet, close it
            if( headerMobileSearch.data('mobile-search-expanded') == 'true' ) {
                headerMobileSearch.trigger('retract');
            } else {
            // If search field is closed on mobile/tablet, open it
                headerMobileSearch.trigger('expand');
            }
        }

        // If desktop search field is open and has nothing in it, close it
        if( searchInput.val().length == 0 && searchButton.data('search-expanded') == 'true' ) {
            headerSearch.trigger('retract');
        }
    });


    // MOBILE SEARCH WIDGET

    // Handle mobile/tablet search expand behavior
    headerMobileSearch.on('expand', function() {
        headerMobileSearch.data('mobile-search-expanded', 'true');
        $('body').animate({'padding-top': searchMobilePane.css('height')}, 500);
        searchMobilePane.slideDown(500);
        searchMobileInput.focus();
    });

    // Handle mobile/tablet search retract behavior
    headerMobileSearch.on('retract', function() {
        headerMobileSearch.data('mobile-search-expanded', 'false');
        $('body').animate({'padding-top': '0'}, 500);
        searchMobilePane.slideUp(500);
    });

    // Handle searchMobileInput typing for typeahead results and capturing enter key to trigger search
    searchMobileInput.keyup(function(e){
        // Reset typeahead timer
        window.clearTimeout(searchTimeout);

        // Check minimum length of search for typeahead query
        //- if( e.currentTarget.value.length >3 ) {
        //-     searchTimeout = window.setTimeout(function() {
        //-         $.ajax({
        //-             url: "/api/rest/audience/search?find=" + searchMobileInput.val()
        //-         })
        //-         .done(function(data, status, xhr) {
        //-             console.log('find data', data);
        //-             $.each(data.data, function() {
        //-                 var self = this;
        //-                 //- console.log(this);
        //-                 var liEl = $("<li />").addClass('teaser-list-item col-xxs-24'),
        //-                     aEl = $("<a />").attr("href", self.url).appendTo(liEl),
        //-                     imgEl = $("<img />").attr("src", self.medium36x25Url).appendTo(aEl),
        //-                     titleEl = $("<span />").addClass("title").text(self.title).appendTo(aEl);
        //-                     summaryEl = $("<p />").addClass("summary").text(self.summary).appendTo(aEl);
        //-
        //-                 if(self.medium36x25Url) {
        //-                     //- $('.component-header-default_with_weather-v1 ul.quick-result .row').append(liEl);
        //-                 }
        //-             });
        //-         });
        //-     }, 500);
        //- }

        // Trigger search event if enter key is pressed
        if( e.keyCode == 13 && searchMobileInput.val().length >0 ) {
            searchMobileButton.click();
        }
    });

    // Handle mobile/tablet search button behavior
    searchMobileButton.click(function(e){
        // Stop typeahead timer
        window.clearTimeout(searchTimeout);

        // Send search query
        if( searchMobileInput.val().length >0 ) {
            window.location = "/search?find=" + searchMobileInput.val();
            searchMobileCloseBtn.click();
        }
    });

    searchMobileCloseBtn.click(function() {
        headerMobileSearch.trigger('retract');
    });
});



