</span></section><script src="/resources/assets/common/moment/2.10.2/moment.js"></script><script>$(function() {
    var storyDateEl = $('.story-date');
        dateline = moment(storyDateEl.text() / 1);
    storyDateEl.text(dateline.format('dddd, MMMM Do YYYY')).fadeIn();
    //- storyDateEl.text(dateline.format('LLLL'));
