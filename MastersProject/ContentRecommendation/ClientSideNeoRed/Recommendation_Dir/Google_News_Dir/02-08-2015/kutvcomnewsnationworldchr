 </script><noscript><img src="https://b.scorecardresearch.com/p?c1=2&c2=19578925&cv=2.0&cj=1" /></noscript><!-- End comScore Tag --><div class='sd-page-wrapper'><div class='sd-header-wrapper sd-page-width'><div class='row'><div class='col-sm-24'><div class='component-header-alerts-v1'><div class="row alert-row"><article class="clearfix"><div id="sd-alert-close" class="sd-alert-close"><i class="fa-times"></i></div><div class="sd-alert-type-section"><ul class="sd-alert-icons"></ul></div><div class="sd-alert-img"></div><div class="sd-alert-summary-wrapper"></div><div class="sd-alert-mobile-indicators-wrapper"><ul class="sd-alert-mobile-indicators"></ul></div></article></div><script>(function() {
    var initAlerts = function() {
        var displayNoneTimeout = null,
            displayNoneEl = null,
            isHovered = false,
            i = 0,
            alertCycle = null,
            alertCycleTimeout = null;

        function autoCycle() {
            var $current = $('.sd-alert.sd-alert-icon.active'),
                $next = $current.next().length ? $current.next() : $('.sd-alert.sd-alert-icon').first();

            if (!isHovered) {
                $next.trigger('activate', true);
            }
        }
        function prepAutoCycle() {
            if (alertCycleTimeout) {
                clearTimeout(alertCycleTimeout);
            }
            alertCycleTimeout = setTimeout(function() {
                if (alertCycle) {
                    clearInterval(alertCycle);
                }
                alertCycle = setInterval(autoCycle, 4000);
            }, 7000);
        }

        alertCycle = setInterval(autoCycle, 4000);

        $('#sd-alert-close').on('click', function(e) {
            var expires = Date.now() + (1000 * 60 * 10);

            Cookies.set('alertsClosed', true, {expires: new Date(expires), path: '/'});

            $(e.target).closest('article').addClass('closed');
        });

        $('.component-header-alerts-v1').on('mouseenter', function() {
            isHovered = true;
        });

        $('.component-header-alerts-v1').on('mouseleave', function() {
            isHovered = false;
        });
        // TODO: HEED THIS WARNING.
        // WARNING THIS IS A BIT FRAGILE.  CLASSES MUST BE IN A SPECIFIC ORDER FOR THIS TO WORK.  IT SHOULD BE REFACTORED WHEN THERE IS MORE TIME.
        // ENDTODO*************
        $('.sd-alert-icon, .sd-alert-mobile-indicator').on('click', function(e) {
            $(e.currentTarget).trigger('activate');
        });

        $('.sd-alert-icon, .sd-alert-mobile-indicator').on('activate', function(e, continueCycle) {
            var selected = $(e.target).closest('li')[0],
                selectedClass = selected.classList[0],
                selectedType = selected.classList[1];
            // If pending display none action fire it immediately
            // going to use to disable pointer events on "hidden" links
            if (displayNoneTimeout) {
                clearTimeout(displayNoneTimeout);
            }

            if (alertCycleTimeout && !continueCycle) {
                clearTimeout(alertCycleTimeout);
                prepAutoCycle();
            }

            if (alertCycle && !continueCycle) {
                clearInterval(alertCycle);
                prepAutoCycle();
            }

            $('.sd-alert.active').removeClass('active');
            $('.' + selectedClass).addClass('active');
            $('.sd-alert-type-section').attr('class', 'sd-alert-type-section ' + selectedType);
        });
    },
    createAlertIconElement = function(alert, i) {
        var classList = [
            'sd-alert-' + i,
            alert.type,
            'sd-alert-icon',
            'sd-alert'
        ];

        if (i === 0) {
            classList.push('active');
        }


        return $('<li>').addClass(classList.join(' '));

    },
    createAlertImageElement = function(alert, i) {
        var classList = [
            'sd-alert-' + i,
            'sd-alert'
        ],
            $el = $('<div>');

        if (i === 0) {
            classList.push('active');
        }

        $el.css({'background-image': 'url(' +
            (alert.thumbnail ? alert.thumbnail : '/resources/assets/2015-07-30T21-27Z/component/header/alerts/v1/images/' + alert.type + '.jpg') + ')'
        });
        $el.addClass(classList.join(' '));

        if (alert.primaryLink) {
            $el = $('<a class="story-link" href="' + alert.primaryLink.url + '">').append($el);
        }

        return $el;
    },
    createSecondaryLinkElement = function(secondaryLink) {
        var $li = $('<li class="sd-alert-secondary-link-text">'),
            $a = $('<a class="sd-alert-secondary-link">');

            $a.attr('target', '_blank');
            $a.attr('href', secondaryLink.url);
            $a.text(secondaryLink.text);

            return $li.append($a);
    },
    createAlertSummaryElement = function(alert, i) {
        var classList = [
            'sd-alert-' + i,
            'sd-alert'
        ],
            $headline = $('<span class="sd-alert-title">').text(alert.headline),
            $summary = $('<span class="sd-alert-summary">').text(alert.summary),
            $headlineSummaryWrapper = $('<div class="sd-alert-text">'),
            $secondaryLinksList = $('<ul class="sd-secondary-links">'),
            secondaryLinksCount = alert.secondaryLinks ? alert.secondaryLinks.length : 0,
            j = 0;

        if (i === 0) {
            classList.push('active');
        }

        if (alert.primaryLink) {
            $headline = $('<a class="story-link" href="' + alert.primaryLink.url + '">').append($headline);
        }

        $headlineSummaryWrapper.append([$headline, $summary]);
        $headlineSummaryWrapper.addClass(classList.join(' '));
        $secondaryLinksList.addClass(classList.join(' '));

        if (secondaryLinksCount) {

            for (j; j < secondaryLinksCount; j++) {
                $secondaryLinksList.append(createSecondaryLinkElement(alert.secondaryLinks[j]));
            }
            return $($headlineSummaryWrapper).add($secondaryLinksList);
        } else {
            return $headlineSummaryWrapper;
        }

    },
    createAlertMobileIndicatorElement = function(alert, i) {
        var classList = [
            'sd-alert-' + i,
            alert.type,
            'sd-alert-mobile-indicator',
            'sd-alert'
        ];

       if (i === 0) {
            classList.push('active');
        }

        return $('<li>').addClass(classList.join(' '));
    },
    // TODO: This should be refactored so that each alert is nested under a single element isntead of nested partial alerts
    createAlerts = function(alerts) {
        var $alertIcons = [],
            $alertImages = [],
            $alertSummaries = [],
            $alertMobileIndicators = [],
            i = 0,
            count = alerts.length;

        for (i ; i < count; i++) {
            $alertIcons.push(createAlertIconElement(alerts[i], i));
            $alertImages.push(createAlertImageElement(alerts[i], i));
            $alertSummaries.push(createAlertSummaryElement(alerts[i], i));
            $alertMobileIndicators.push(createAlertMobileIndicatorElement(alerts[i], i));
        }

        if(alerts && alerts.length >0 && alerts[0]){

            if (!Cookies.get('alertsClosed')) {
                $('.component-header-alerts-v1').show();
            }
            $('.sd-alert-type-section').addClass(alerts[0].type);
        }
        $('ul.sd-alert-icons').append($alertIcons);
        $('div.sd-alert-img').append($alertImages);
        $('div.sd-alert-summary-wrapper').append($alertSummaries);
        $('ul.sd-alert-mobile-indicators').append($alertMobileIndicators);
        initAlerts();
    };

    $.ajax({
        url: '/api/rest/audience/get-alerts',
        success: function (data, status, xhr) {
            createAlerts(data.data);
        }
    });
})();</script></div><div class='component-ddb-1x1-v1 ddb-no-margin'><div id="ddb_1x1_div" class="ddb-no-label"><div id="ddb_1x1_desktop" class="ddb"></div></div><script type="text/javascript">if(typeof googletag != "undefined") {
