			// if first instance of banner then set cookie with blank first.
			function setCookie_Banner(cname, cvalue, exdays) {
			    var d = new Date(); d.setTime(d.getTime() + (exdays*24*60*60*1000)); var expires = "expires="+d.toUTCString(); document.cookie = cname + "=" + cvalue + "; " + expires;
			}
			setCookie_Banner('bannerDisplayed','',1);
			
			var bannerJsArr = new Array(); //assign blank again, if it is into catch.
			function sleepFor( sleepDuration ) {
			    var now = new Date().getTime(); while(new Date().getTime() < now + sleepDuration){ }
			}//End of function sleepFor	
			
			bannerJsArr[1] = new Array(); bannerJsArr[1][63] = '38394'+':'+'38394,38170';
			
			var clearBannerBinAjaxCache = true; //we used this variable into below ajax fun.
		}
	}
	catch(e1){}
}
</script>
<!-- start ajax banner div -->
<div id="banner-ajax-div-38394-1" class="banner-ajax-div-class">

</div> <!-- end ajax banner div -->
<!-- bannerbinId got from cache. : ajax-reload.php -->
<script type="text/javascript">
function bannerAjaxLoad_38394_1(bannerId, dataAvailable, bannerInstance, bannerBinSlug, bannerBinId) {
	urlVal = 'http://www.mycentraloregon.com/wp-content/plugins/advertise/' + 'advertise-ajax.php?bannerAjax=true&callFromFile=ajax-reload.php&bannerId='+bannerId+'&dataAvailable='+dataAvailable+'&bannerInstance='+bannerInstance+'&siteId=38'+'&bannerBinSlug='+bannerBinSlug+'&bannerbinId='+bannerBinId+'&foundLightWindow=1';
	currentUrl = location.href;
	if(currentUrl.indexOf("https:") >= 0){
		urlVal = urlVal.replace("http:","https:");
	}
	try { //WD-534
		if (clearBannerBinAjaxCache && clearBannerBinAjaxCache != 'undefined') {
			urlVal = urlVal + '&clearBannerBinAjaxCache='+clearBannerBinAjaxCache;
			clearBannerBinAjaxCache = false; //once clear cache for first ajax request, doesnot matter which banner instance it got, then just make it false.
		}
	}
	catch(e) {}
	//urlVal = urlVal + '&instanceVar=true&bannerId_'+bannerInstance+'='+bannerId+ '&dataAvailable_'+bannerInstance+'='+dataAvailable; //no need right now.
	//document.write(urlVal); //alert(urlVal);
	jQuery('#banner-ajax-div-38394-1').html('<div style="height:200px;width:250px;" class="banner-loading-div-class"><br /><br />&nbsp;&nbsp;<br /><br /></div>');
	jQuery.ajax({
		  type: "GET",
          url: urlVal,
          async: true,
          //dataType: "html",
          success: function(data) { //alert(data);
               jQuery('#banner-ajax-div-38394-1').html(data);
          }//End of success function.
    });
}//End of function bannerAjaxLoad


jQuery(document).ready( bannerAjaxLoad_38394_1('38394', '38394,38170', '1', '300x250-inside-top', '63') );

</script>
