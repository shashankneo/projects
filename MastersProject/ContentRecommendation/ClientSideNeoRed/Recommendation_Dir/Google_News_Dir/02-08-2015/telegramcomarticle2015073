<script type='text/javascript'>
		/* New Clickshare code */
		var rURL = window.location.protocol + "//" + window.location.host + window.location.pathname;
		function csLogout() {
			document.cookie = 'CSDemos=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
			document.cookie = 'CSAuthCookie=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
			document.cookie = 'CSPermReg=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
			window.location.href = "https://home.telegram.com/clickshare/logout.do?returnurl=" + rURL;
		}

		function getCookie(c_name) {
			var c_value = document.cookie;
			var c_start = c_value.indexOf(" " + c_name + "=");
			
			if (c_start == -1) {
				c_start = c_value.indexOf(c_name + "=");
			}
			if (c_start == -1) {
				c_value = null;
			} else {
			  c_start = c_value.indexOf("=", c_start) + 1;
			  var c_end = c_value.indexOf(";", c_start);
				
				if (c_end == -1) {
					c_end = c_value.length;
				}
				c_value = unescape(c_value.substring(c_start,c_end));
			}
			
			return c_value;
		}

		function getUserCount(c_name) {
			var cValue = getCookie(c_name);
	
			if (cValue === null) {
				var userObject = 0;
			} else {
				var val = cValue.split("$");
				var userObject = {};
	
				for (var x = 0, length = val.length; x < length; x++ ) {				
					var cArray = val[x].split("|");
					userObject[cArray[0].replace("-", "")] = cArray[1];
				}		
			}
			return userObject;
		}
			
		function getUserInfo(c_name) {
			var cValue = getCookie(c_name);
			if (cValue === null) {
				var userObject = 0;
			} else {
				var cArray = cValue.split("|");
			
				var userObject = {};

				for (var i = 0, len = cArray.length; i < len; i++) {
					var tempArray = cArray[i].split("=");
					userObject[tempArray[0]] = tempArray[1];
				}
			
			}
			return userObject;
		}

		var csUser = getUserInfo('CSDemos');
		var uCount = getUserCount('CSPermReg');

