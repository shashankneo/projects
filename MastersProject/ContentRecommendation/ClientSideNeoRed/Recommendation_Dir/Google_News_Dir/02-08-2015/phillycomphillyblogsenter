<script type="text/javascript">
$("#showComments, #atcComments, #atcComment").click(function() {
	var label = $("#commentsLabel").html();
	var loadComments = true;
	if ( $("#disqus_thread" ).hasClass( "activeComments" ) ) {
		$("#disqus_thread").removeClass('activeComments');	
		$("#commentsLabel").html("Read ")
	} else {
		$("#disqus_thread").addClass('activeComments');
		$("#commentsLabel").html("Close ");
		
		if ($("#showComments a").html() == 'Join the Conversation' ) {
		$("#showComments a").html("Comments");
		}
		if(loadComments) {
			resetDisqus();
			loadComments = !loadComments;
		} else {	}
	}
});
setTimeout(
  function() 
  {
  	var showCom = $("#showComments a").html();
	if (showCom.toLowerCase() == '0 comments' ) {
		$("#commentsLabel").html("");
		$("#showComments a").html("Join the Conversation");
	}
  }, 3000);
</script>
