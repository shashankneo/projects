<!-- betterificator tag -->

	<script type="text/javascript">
	var Better = (function() {

		var Better = {};

		Better.IMPROVED_QUALITY = 90;

		/* request a new & improved image from photon.
		 *
		 */
		var getNewImageUrl = function( imageUrl, width) {

			// Don't touch the gravatar.com links.
			// Should we update this to only touch local links?
			if(imageUrl.match('gravatar.com')) {
				return imageUrl;
			}

			var a = document.createElement('a');
			a.href = imageUrl;
			if( window.location.origin == window.location.href || window.location.origin == window.location.href.slice(0,-1) ){
				var imageUpgrade = { strip: 'all' };
			}else{
				var imageUpgrade = {
							quality: Better.IMPROVED_QUALITY,
			   				w: width
						   };
			}
			a.search = '?' + jQuery.param( imageUpgrade );
			//console.log( a.href, width );
			return a.href;

		};

		Better.findLargerParent = function( element, width ) {
			var parentWidth = element.parent().width();
			var grandParentWidth = element.parent().parent().width();
			var ancestorWidth = element.parent().parent().parent().width();

			if ( width < parentWidth ) {
				console.log('.parent()');
				return parentWidth;
			} else if ( width < grandParentWidth ) {
				console.log('.parent().parent()')
				return grandParentWidth;
			} else if ( width < ancestorWidth ) {
				console.log('.parent().parent().parent()');
				return ancestorWidth;
			}
			return 0;
		}

		Better.findLargerVisibleParent = function( element, width ) {

			var parentElement = element.parent();

			if( !parentElement || parentElement.length <= 0 )
				return width;

			if( !parentElement.is(':visible') || parentElement.width() < width )
				return Better.findLargerVisibleParent( parentElement, width );

			return parentElement.width();

		}

		/*
		 *
		 */
		Better.updateAllImages = function() {
			console.log('Updating ALL images');
			jQuery('img').each( function() {
				var imageUrl =  jQuery( this ).attr( 'src' );
				var domWidth = jQuery( this ).width();
				var styleWidth = jQuery( this ).attr( 'width' );
				var deviceWidth = window.innerWidth;
				var newWidth = 0,
					debug = false,
					widthSource = '';

				// Start with current DOM width, if it's non-zero.
				if ( domWidth > 0 ) {
					newWidth = domWidth;
					widthSource = 'DOM';
				}
				// If there is a width attribute on the img tag,
				// and it is larger than the DOM width use that.
				if( newWidth < styleWidth ) {
					newWidth = styleWidth;
					widthSource = 'STYLE';
				}

				// Check for a parent div with a larger width
				// than the current DOM width and style width.
				var largerParent = Better.findLargerVisibleParent( jQuery(this), newWidth );
				// Maybe the DOM width is too small
				// and there is no style set on the img
				// find the size of the containing div, and use that.
				if ( largerParent > newWidth ) {
					newWidth = largerParent;
					widthSource = 'PARENT';
				}

				// Handle first slideshow image differently
				if( 'slideshow-first-image' == jQuery(this).attr('id') ) {
					newWidth = Better.findLargerParent( jQuery(this), newWidth );
					widthSource = 'SLIDESHOW';
				}

				// As a last check, don't request images larger
				// than the current device.
				if( newWidth > deviceWidth) {
					newWidth = deviceWidth;
					widthSource = 'DEVICE';
				}
				var newUrl = getNewImageUrl( imageUrl, newWidth );
				if ( true == debug ) {
					console.log( newUrl, widthSource, domWidth, largerParent, deviceWidth );
				}
				jQuery( this ).attr('src', newUrl );
			});
		}
		return Better;
	})();
	Better.updateAllImages();
	</script>


