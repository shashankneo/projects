<script type="text/javascript">
// look for elements with classes in the format of
// reorder_<id of target container element>_<position>
// e.g. reorder_story-target_3 would be moved after
// the 3rd child element of #story-target
//
// If there aren't enough child elements,
// (e.g., targeting the 7th element of a 4-paragraph story)
// jQuery won't move it.
jQuery('[class*=reorder]').each(function(){
classes = jQuery(this).attr('class').split(' ');
for (var i=0; i<classes.length; i++) {
if (classes[i].substring(0,8) == "reorder_") {
key = classes[i];
container = '#'+key.split('_')[1];
position = key.split('_')[2];
target = container + '>div>:nth-child(' + position + ')';
jQuery(this).insertAfter(target);
}
} });
</script>
<script type="text/javascript">
jQuery(document).ready(function () {
var displayRelated = false;
if (jQuery('#related-gallery').children().length > 0) {
displayRelated = true;
}
// And now actually show the related content, if we should.
if (displayRelated) {
jQuery('#related-gallery').show();
} else {
jQuery('#related-gallery').attr('style','display:none!important;');
}
});
</script>
