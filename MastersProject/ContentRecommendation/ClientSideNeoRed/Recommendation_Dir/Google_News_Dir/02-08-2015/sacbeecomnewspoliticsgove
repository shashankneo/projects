$(document).ready(function(){
// Removing empty paragraphs first
$("#content-body-19911615 p").filter( function() {
var html = $.trim($(this).html());
if(html == '' || html == '&nbsp;')
return true;
}).remove();
// Taking a subset of paragraphs using offset and maximum
var offset = 1;
var max = -1;
if(offset > 1){
$("#content-body-19911615").children().slice(0, offset-1).remove();
}
if(max > -1){
$("#content-body-19911615").children().slice(max).remove();
}
// Show dateline field at the beginning of the first paragraph
var showDateline = false;
var datelineStr = "";
if(offset <= 1 && showDateline == true && datelineStr != "") {
$("#content-body-19911615 p:first").prepend("<span>"+ datelineStr + "- </span>");
}
});
</script>
</div>
</div>
