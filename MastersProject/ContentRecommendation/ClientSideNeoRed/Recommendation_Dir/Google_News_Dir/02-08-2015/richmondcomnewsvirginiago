<script type="text/javascript">
    function ip_decrypt(sJSON) {
      sIP = sJSON.ip;
      var ipString = '75.146.26.139';
      var aIP = ipString.split(",");
      for (i=0; i < aIP.length; i++) {
        if (sIP == aIP[i]) {
        $.cookie("white_listed", 1, {expires: 7, path: '/'});
          
            $("#services-manager-messages").hide();
            $("#blox-story").css('padding-top','0px');
            resetTrackerCookies();
           
           }  
      }
    } 
            var sClasses = 'encrypted-content';
    if($.cookie('white_listed') == 1){ 
        $(function(){ 
            
                resetTrackerCookies();
                $("#services-manager-messages").hide();
                $("#blox-story").css('padding-top','0px');                
            
        });
    } else {
            $.getJSON("http://www.richmond.com/_services/v1/client_ip_info/",function(data){
                    ip_decrypt(data);
                                       
            });
    }        
    </script>


<script>
// OMNITURE GALLERY HOOKUP
function onGalleryFrame(frame){
	try{
		var $frame = $(frame);
		var imgName = $frame.find('img').attr('alt');
		
		var s=s_gi(s_account);
			s.linkTrackVars='events,eVar25';
			s.linkTrackEvents='event13';
			s.events='event13';
			s.eVar25=imgName;
			s.tl(this,'o','ImageClicks');
	}
	catch(err){};
}

$(function(){
	// OMNITURE TOPS WIDGET HOOKUP
	$('#tabIncludesidebar .tabs-container a').bind('click',function(){
		// TOP HOMES TAB
		if($(this).children('span').text().indexOf('home')!=-1){
			try{
				var s=s_gi(s_account); 
								s.linkTrackVars='events,eVar26';
								s.linkTrackEvents='event14';
								s.events='event14';
								s.eVar26='Top Homes Tab';
								s.tl(this,'o','TopsModule');				
			}
			catch(err){};
		}
		// TOP CARS TAB
		if($(this).children('span').text().indexOf('Cars')!=-1){
			try{
				var s=s_gi(s_account); 
					s.linkTrackVars='events,eVar26';
					s.linkTrackEvents='event14';
					s.events='event14';
					s.eVar26='Top Car Tab';
					s.tl(this,'o','TopsModule');
			}
			catch(err){};
		}
	});
	
	// OMNITURE MORE BUTTON HOOKUP
	$('#tabIncludesidebar .more').bind('click',function(){
		var $t = $(this);
		var linkHREF = $t.attr('href');
		
		// TOP HOMES MORE BUTTON
		if($t.text().indexOf('Homes')!=-1){
			try{
				var s=s_gi(s_account); 
					s.linkTrackVars='events,eVar26';
					s.linkTrackEvents='event14';
					s.events='event14';
					s.eVar26='Top Homes Button';
					s.tl(this,'o','TopsModule');
			}
			catch(err){};
		}
		// TOP CARS MORE BUTTON
		else if($t.text().indexOf('Cars')!=-1){
			try{
				var s=s_gi(s_account); 
					s.linkTrackVars='events,eVar26';
					s.linkTrackEvents='event14';
					s.events='event14';
					s.eVar26='Top Car Button';
					s.tl(this,'o','TopsModule');
			}
			catch(err){};
		}
		
		setTimeout(function(){
			location.href = linkHREF;
		}, 1000);
		
		return false;
	});

	// OMNITURE LISTING LINKS HOOKUP
	$('#tabIncludesidebar .blox-title').each(function(index, element) {
		var $t = $(this);
		if($t.text().indexOf('homes')!=-1){
			$t.next().find('.blox-headline a').each(function(index, element) {
				$(this).bind('click',function(){
					var $t = $(this);
					var linkHREF = $t.attr('href');
					try{
						var s=s_gi(s_account);
							s.linkTrackVars='events,eVar26';
							s.linkTrackEvents='event14';
							s.events='event14';
							s.eVar26='Top Homes address';
							s.tl(this,'o','TopsModule');
					}
					catch(err){};

					setTimeout(function(){
						location.href = linkHREF;
					}, 1000);
					
					return false;
				});
			});
		}
		if($t.text().indexOf('Cars')!=-1){
			$t.next().find('.blox-headline a').each(function(index, element) {
				$(this).bind('click',function(){
					var $t = $(this);
					var linkHREF = $t.attr('href');
					try{
						var s=s_gi(s_account);
							s.linkTrackVars='events,eVar26';
							s.linkTrackEvents='event14';
