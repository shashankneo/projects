                    <script type="text/javascript">
    function submitform() {
        document.getElementById('search_text').value = processText(document.getElementById('search_text').value);
        document.forms["searchbox"].submit();
    }

    function processText(startText) {
        var endText = new String();

        if (startText.substr(0, 1) == '"') {
            endText = startText;
        } else {
            var wordArray = startText.split(" ");
            var i = 0;
            for (i = 0; i < wordArray.length; i++) {
                if (wordArray[i].length > 3) {
                    endText = endText + wordArray[i] + " ";
                }
            }
        }
        return endText;

    }
</script>
                    
