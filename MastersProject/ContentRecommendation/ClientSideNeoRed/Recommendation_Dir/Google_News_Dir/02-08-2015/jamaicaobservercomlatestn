$('.fotorama')
  .on('fotorama:show', function (e, fotorama) {    
    fotorama.$caption = fotorama.$caption || $(this).next('.fotorama-caption');
    var activeFrame = fotorama.activeFrame;
	if(activeFrame.title != ""){
	$(".fotorama-caption").removeClass("remove_caption");	
    fotorama.$caption.html('<div style="height:10px"></div>'+activeFrame.title + '<br>');
