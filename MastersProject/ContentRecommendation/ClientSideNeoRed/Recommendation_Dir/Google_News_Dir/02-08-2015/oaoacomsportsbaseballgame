            $bloxFeaturedListings.googleEvents('business','widget','Blooming Rose:6cc1882e-0829-5617-8e46-5aee640f954d', 1, true);
        
            var $bloxFeaturedListings = $('#blox-featured-listings');
            $bloxFeaturedListings.googleEvents('business','widget','Wendover Family Medicine:ecd28c6e-8bb0-5178-8f59-52fc1ee6cf34', 1, true);
        
            var $bloxFeaturedListings = $('#blox-featured-listings');
            $bloxFeaturedListings.googleEvents('business','widget','Basin Podiatry:8357609f-9e05-58c8-ae31-e531c5f5cd78', 1, true);
        
            var $bloxFeaturedListings = $('#blox-featured-listings');
            $bloxFeaturedListings.googleEvents('business','widget','East University Family Medicine:ac03bad5-33f2-5c2b-83c9-b9ea49a6c8b0', 1, true);
        
            var $bloxFeaturedListings = $('#blox-featured-listings');
            $bloxFeaturedListings.googleEvents('business','widget','John\'s Corner:a5799573-ec13-5d8d-a20d-52027012723d', 1, true);
        

    
        $('#slideshow45').cycle({
            fx:      'scrollHorz',
            timeout:  12000,
            prev:    '#prev45',
            next:    '#next45',
            pager:   '#nav-mp45',
            pagerAnchorBuilder: pagerFactory45,
            after:   onAfter45});
        function onAfter45(curr45, next45, opts45) {
            // commented out by cricketts 2.11.11
            //var index45= opts45.currSlide;
            //$('#prev45')[index45== 0 ? 'hide' : 'show']();
            //$('#next45')[index45== opts45.slideCount - 1 ? 'hide' : 'show']();

                imageItem = $('#slideshow45').find('.business').eq(opts45.currSlide).find('.logo-clip img');
                iWidth = imageItem.width();
                iHeight = imageItem.height();

                if(iWidth && iHeight){
                    if(iWidth > iHeight){
                        imageItem.css("width", "150px");
                        imageItem.css("height", "auto");
                    } else {
                        imageItem.css("width", "auto");
                        imageItem.css("height", "80px");
                    }
                }
        }
        function pagerFactory45(idx45, slide) {
            var s45= idx45> 10 ? ' style="display:none"' : '';
            return '<li'+s45+'><a href="#">'+(idx45+1)+'</a></li>';
        };
    

    // remove loading image
    $('#blox-featured-listings-content').removeClass('blox-loading');
    $('#nav-container-feat-business, #blox-featured-pop-search').removeClass('hide');

    $('a.blox-featured-pop-search-toggle').toggle(
        function() {
           $('#blox-featured-pop-search-list').slideDown(400);
        },
        function() {
           $('#blox-featured-pop-search-list').slideUp(400);
        }
    );
    $('.ui-state-default').hover(
        function(){
            $(this).addClass('ui-state-hover');
        },
        function(){
            $(this).removeClass('ui-state-hover');
        }
    );
});
//]]>
</script>

