<script type="text/javascript">
        function on_yq_suggest(suggestions) {
            //check if there are suggested results, if so populate 'youneeq' div
            if (suggestions && suggestions.suggest && suggestions.suggest.node) {

                var nodeids = suggestions.suggest.node;
                var stories = "";
                var recommend_header = '<div class="blox-title"><h3>' +
                        'Recommended For You...' +
                        '</h3></div>';
                var link_begin = '<li><a ';
                var link_local = ' href="';
                var link_remote = ' target="_blank" href="';
                var link_end = "</a></li>";
                var current_host = window.location.hostname.replace("www.","");

                for (var i = 0; i < nodeids.length; i++) {
                    var articleTitle = nodeids[i].title;
                    var articleLink = nodeids[i].url;
                    var trimmedTitle = Yq.titleTrim(nodeids[i].title);
                    var linkClickId = 'id="yq-rec-link-0' + i + '" onmousedown="' + 
                                "Yq.yq_panel_click('" + articleLink + "','" + trimmedTitle + "','" + nodeids[i].id + "')" + '"';
                    var linkStart = link_begin + linkClickId + link_local;
                    if(articleLink.indexOf(current_host) < 0){
                        linkStart = link_begin + linkClickId + link_remote;
                    }
                    //list item
                    stories += linkStart + articleLink + '">' + articleTitle + link_end;
                }
                //opening and closing tags for list + css class ref
                stories = recommend_header + "<ul class='bull-list'>" + stories + "</ul>";
                $("#youneeq").append(stories);
                $("#yq-recommended-modal").css("display", "block");
            }
        }

        function my_yq_init() {
            //may be cleaner to populate server side
            var title = "";
            var categories = "";
            var content_id = "";
            var image_url = "";
            var description = "";
            var create_date = "";
            if($('meta[property="og:type"]').attr('content') === "article"){ //if page has a title populate these...
                try{
                    //populate title field, used with panel results option, with open graph title
                    title = $('meta[property="og:title"]').attr('content');
                    //the uuid after 'article_' in the url, if not correct then replace with appropriate code
                    content_id = window.location.pathname.substr(window.location.pathname.lastIndexOf(".") - 36, 36);
                    image_url = $('meta[property="og:image"]').attr('content');
                    description = $('meta[property="og:description"]').attr('content');
                    create_date = $('span.updated').attr('title');
                    //this will build an array of the values between forward slashses in the path portion of the url, and then remove the last item
                    categories = window.location.pathname.substr(1, window.location.pathname.length - 1).split('/');
                    if(categories.length > 2){
                        categories = categories.splice(0,categories.length - 2);
                    }
                    else{
                        categories.pop();
                    }
                }
                catch(e){console.log(e.message); title = "";};
            }
            //content or not content page logic
            if (title.length > 0) { //if 'condition that's only true on content pages' then do this...
                Yq.observe({
                    'observe': [{
                        'type': 'node',
                        'name': content_id,
                        'title': title, //required for 'is_panel_builder'
                        'description': description,
                        'categories': categories,
                        'image': image_url,
                        'create_date': create_date
                    } ],
                    //fetches 10 recommendations that can come from any site running the observe observable, with no category restrictions and returning content_id, url and title
                    'suggest': [{ 'type': 'node', 'count': 10, 'is_panel_builder': 'true', 'domains': ['oaoa.com','cbs7.com']  }]
                },
                on_yq_suggest);
            }
            else { //if identifier not found (i.e. non-content page) do this...
                Yq.observe({
                    //fetches 10 recommendations that can come from any site running the observe observable, with no category restrictions and returning content_id, url and title
                    'suggest': [{ 'type': 'node', 'count': 10, 'is_panel_builder': 'true', 'domains': ['oaoa.com','cbs7.com'] }]
                },
                on_yq_suggest);
            }
        }
        Yq.onready(my_yq_init);
</script>


