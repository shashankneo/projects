<script>
    var _sf_async_config={};
    /** CONFIGURATION START **/
    _sf_async_config.uid = 53628;
    _sf_async_config.domain = dfm.data("actualDomain");
    _sf_async_config.useCanonical = true;
	if(dfm.data("pageType")=='frontpage'){
		_sf_async_config.sections = dfm.data("Taxonomy1");
	}
	else{
		if(dfm.data('sectionName')){
			_sf_async_config.sections = dfm.data('sectionName');
		}
		else{
			_sf_async_config.sections = dfm.data('kv');
		}
	}
	if(dfm.data("byline")){
		_sf_async_config.authors = dfm.data("byline");
	}
    /** CONFIGURATION END **/
    (function(){
      function loadChartbeat() {
        window._sf_endpt=(new Date()).getTime();
        var e = document.createElement('script');
        e.setAttribute('language', 'javascript');
        e.setAttribute('type', 'text/javascript');
        e.setAttribute('src', '//static.chartbeat.com/js/chartbeat.js');
        document.body.appendChild(e);
      }
      var oldonload = window.onload;
      window.onload = (typeof window.onload != 'function') ?
         loadChartbeat : function() { oldonload(); loadChartbeat(); };
    })();
</script>
<!-- End: ChartBeat -->

<!-- Verticals links -->
<script type="text/javascript">
(function($) {
   var domain = "http://local.timesheraldonline.com";
   //JSON file lives in Akamai netstorage
   var jsonToGet = domain + '/assets/verticals.json';
   $.ajax({
      url: jsonToGet,
      dataType: 'jsonp',
      contentType: 'application/json',
      cache: true,
      async: false,
      jsonpCallback: 'pubData',
      success: function (data) {
         //Check to see if each item exists, and if it does, append it to verticalsLinks list in FinishHead.pbo
         //Autos
         if(typeof data.verticals.autos.link !== 'undefined' && data.verticals.autos.link !== '') {
            if(data.verticals.autos.link.substring(0,1) == "/") {
               data.verticals.autos.link = "http://www.timesheraldonline.com" + data.verticals.autos.link;
            }
            $('ul#verticalsLinks').append('<li><a href="'+ data.verticals.autos.link +'">' + data.verticals.autos.title + '</a></li><li>|</li>');
         }
         //Real Estate
         if(typeof data.verticals.realestate.link !== 'undefined' && data.verticals.realestate.link !== '') {
            if(data.verticals.realestate.link.substring(0,1) == "/") {
               data.verticals.realestate.link = "http://www.timesheraldonline.com" + data.verticals.realestate.link;
            }
            $('ul#verticalsLinks').append('<li><a href="'+ data.verticals.realestate.link +'">' + data.verticals.realestate.title + '</a></li><li>|</li>');
         }
         //Jobs
         if(typeof data.verticals.jobs.link !== 'undefined' && data.verticals.jobs.link !== '') {
            if(data.verticals.jobs.link.substring(0,1) == "/") {
               data.verticals.jobs.link = "http://www.timesheraldonline.com" + data.verticals.jobs.link;
            }
            $('ul#verticalsLinks').append('<li><a href="'+ data.verticals.jobs.link +'">' + data.verticals.jobs.title + '</a></li>');
         }
      },
      error: function(jqXHR, textStatus, errorThrown) {
         //log the error to the console
         console.log("The following error occured: " + textStatus, errorThrown);
      },
   });
}(jQuery));
</script>

