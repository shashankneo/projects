<script type="text/javascript">

		function ChangeAction()
		{
			var query = $('#searchQuery').val();
			var searchValue = "Growth Engine";
			
			if(query.toUpperCase() == searchValue.toUpperCase())
			{
				var action =getURL_of_Publication();
				
				window.location.href = action + "/indepth/growthengines/";
				
			}
			else
			{
			    var action =getURL_of_Publication()+"/search/"+query;
			    
			    if((query == null)||(query =='Search Keywords'))
			    {
			    	action =getURL_of_Publication()+"/search/";
			    }
			    
			    $('#searchForm').attr("action",action);	
			    document.getElementById('searchForm').submit();
			}
		    
		}
</script>



