<script type="text/javascript">
    $(function () {
        $('#divTwitter').hide();
        $('.ShareTab ul li:eq(0)').attr("class", "active");
    });

    function ShowSocialTab(id) {
        $('#divTwitter').hide();
        $('#divFacebook').hide();
        if (id == '1') {
            $('#divFacebook').show();
            $('.ShareTab ul li:eq(0)').attr("class", "active");
            $('.ShareTab ul li:eq(1)').attr("class", "");
        }
        else {
            $('#divTwitter').show();
            $('.ShareTab ul li:eq(0)').attr("class", "");
            $('.ShareTab ul li:eq(1)').attr("class", "active");
        }
    }
</script>



