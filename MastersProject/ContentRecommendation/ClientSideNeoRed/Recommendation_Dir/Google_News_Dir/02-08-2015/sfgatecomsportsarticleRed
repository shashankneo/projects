        <script type="text/javascript">HDN.onLoginSuccess = print_sfgate_un;</script>
        <script src="/js/hdn/utils/jquery.placeholder.js"></script>
        <script type="text/javascript">
            jQuery(function(){
                $("#modal_signin_close").on("click",function(){
                    $('#modal_signin_div').hide();
                    return false;
                });
                $('#modal_signin_div .checkbox-div').on("click",function(){
                    if ($(this).text() == "X") {
                        $(this).text("");
                        $('#modal_signin_rememberme').removeAttr('checked');
                    } else {
                        $(this).text("X");
                        $('#modal_signin_rememberme').attr('checked', 'checked');
                    }
                    return false;
                });
                try {$('input, textarea').placeholder();} catch (e) {}
            });
        </script>
        <div id="modal_signin_div" class="signin">
            <div id="modal_signin_error_div" style="display:none;"></div>
