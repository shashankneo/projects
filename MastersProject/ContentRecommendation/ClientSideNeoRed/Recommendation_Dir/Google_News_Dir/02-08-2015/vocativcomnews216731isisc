					<span>Tweet this</span>
				</div>
			</div>
			<!-- .share-inner -->
		</div>
		<!-- .top-share -->
		<# if ( 'vocativ-video' == data.post.post_type && data.post.video_post_item ) { #>
			<div class="video-content">
				{{{ data.post.video_post_item }}}
			</div>
		<# } #>
		<div class="author-meta author-top">
			<span>

				<# var authortypes = data.post.author_types #>
				<# var index = 0; #>
				<#for(var i in authortypes){ #>
					<# if(authortypes[i].length){ #>
						<# var authors = authortypes[i] #>
						<#for (var j in authors ) { #>
							<# var author = authors[j]; #>
							<#if ( index == 0 ) { #>
								<span>By</span>
							<# } #>
							<#if( ( index > 0 || j > 0) && index < 2 ) {#>
								<span>and</span>
							<# } #>
							<#if( index < 2 ) {#>
								<span><a class="writer" href="{{window.location.origin}}/author/{{author.user_nicename}}/" title="Posts by {{author.display_name}}" rel="author">{{author.display_name}}</a></span>
								<# index = index+1; #>
							<# } #>

						<# } #>
					<# } #>

				 <# } #>
				 <span>on</span>
				 <span>{{data.post.posted_date_byline}}</span>
			 </span>
		</div>

		<# if ( data.post.headline_image && data.post.headline_image_src ) { #>
			<div class="article-image">
				<img src="{{data.post.headline_image_src}}">
				<#if ( data.post.headline_image['options']['caption'] || data.post.headline_image['options']['credit'] ) { #>
					<span class="article-image-caption">
						<span class="">{{{data.post.headline_image['options']['caption']}}}</span>
						<span class="article-image-credit">{{{data.post.headline_image['options']['credit']}}}</span>
					</span>
				<# } #>
			</div>
		<# } #>

			<!-- .entry-meta -->
		<div class="article-content">
			{{{data.post.post_content}}}
		</div><!-- .entry-content -->

		
