	<a href="{{document.location.origin}}/news/{{data.post.ID}}/{{data.post.post_name}}" data-id="{{data.post.ID}}" data-news-path="{{data.post.ID}}/{{data.post.post_name}}" data-permalink="{{data.post.permalink}}" data-relative-path="{{data.post.relative_path}}" data-edit-link="{{data.post.edit_link}}" class="raw-blog-link">
		<# if ( data.post.thumb ) { #>
			<div class="thumb-wrapper">{{{data.post.thumb}}}</div>
		<# } #>
		<div class="post-wrapper">
			<span class="post-title">{{data.post.post_title}}</span>
			<div class="post-detail">
				<# var authortypes = data.post.author_types #>
				<# var index = 0; #>
				<#for(var i in authortypes){ #>
					<# if(authortypes[i].length){ #>
						<# var authors = authortypes[i] #>
						<#for (var j in authors ) { #>
							<# var author = authors[j]; #>
							<#if( index < 1 ) {#>
								<span class="author-name">{{author.display_name}}</span>
								<span> - </span>
								<# index = index+1; #>
							<# } #>

						<# } #>
					<# } #>
				 <# } #>
				 <# var oneminago = new Date().getTime() - (1 * 60 * 1000); #>
				 <# var twodaysago = new Date().getTime() - (2 * 24 * 60 * 60 * 1000); #>
				 <# var today = new Date(data.post.post_date); #>
				 <# if( today > oneminago ){ #>
				    <span class="post-date">Just now</span>
				 <# } else if( today < twodaysago ){ #>
				    <span class="post-date">{{data.post.posted_date_word}}</span>
				 <# } else { #>
				    <span class="post-date">{{moment(data.post.posted_date_parsed).fromNow()}}</span>
				 <# } #>
			</div>
		</div>
	</a>
