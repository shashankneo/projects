				function moresectionnews(text){
var fileurl;
   if(text=="latest"){
        fileurl="/test_redis.php?t=latestbox_latest";
            }else{
        fileurl="/test_redis.php?t=latestbox_"+text;   
      }
    $.get(fileurl, function(data)
    { 							
          
            $("#latestdata").html(data).hide().fadeIn();
            
         


    }); 

}
 moresectionnews("latest");


