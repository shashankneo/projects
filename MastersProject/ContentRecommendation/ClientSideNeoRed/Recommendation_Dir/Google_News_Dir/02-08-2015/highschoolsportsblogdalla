<script type="text/javascript">
  function formatTweet(text){//This function is redundant, but is needed because common.js isn't being called before gigya initializes on blogs
    var via = ' | @' + gigya_tw_via,
      returnText = text+via,
      linkCharOffset = 31,
      arrayText = [];

    if (returnText.length + linkCharOffset <= 140){
      return returnText;
    } else {
      arrayText = text.split(' ');
      while ((returnText.length + linkCharOffset) > 140){
        arrayText.pop();
        returnText = arrayText.join(' ')+via;
      }
      return returnText;
    }
  }
</script>
