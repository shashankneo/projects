<script>
function GetXmlHttpObject1()
	{
		if (window.XMLHttpRequest)
		  {
			  // code for IE7+, Firefox, Chrome, Opera, Safari
			  return new XMLHttpRequest();
		  }
		if (window.ActiveXObject)
		  {
			  // code for IE6, IE5
			  return new ActiveXObject("Microsoft.XMLHTTP");
		  }
		return null;
	}

function processEmailOptin1()
{

        var xmlhttp = GetXmlHttpObject1();
        var lfckv = document.getElementById("agreement-check1").checked

        //alert(xmlhttp);
        
	if(lfckv)
	{

		var url;
		var email = document.getElementById("email_input1").value;

		if( email == "")
		{
			document.getElementById("email_results1").innerHTML = "Please enter your email address.";
			document.getElementById("email_results1").style.display="block";
		}
		else
		{
			url = "/inc/process_email_optin.php?email="+ email +"&ip=10.25.64.67";

			//alert(url);
			document.getElementById('signup_form1').innerHTML = "<img src='http://www.etonline.com/inc/2010/08/23823617/ajax-loader.gif' />";

			xmlhttp.onreadystatechange=function()
				{
					if(xmlhttp.readyState==4)
					  {
						if(xmlhttp.status == 200)
						{
							var res = xmlhttp.responseText;
							//alert("here"+res);
//MWH FOR TESTING IN DEV ONLY
//res = "Success";
							if(res.indexOf("Success") > -1)                                               
							{
     						           document.getElementById('df_thanks1').style.display = "block";							   
							   document.getElementById('email_results1').innerHTML = "";
							   document.getElementById("email_results1").style.display="none";
              				}
							else
							{
							   document.getElementById('df_error1').style.display = "block";
							}
						}
					}
				}
				xmlhttp.open("GET",url,true);
				xmlhttp.send();
		
			


		}
	

	}
	else
	{
		document.getElementById('email_results1').innerHTML = "Please Accept The Terms and Conditions before submitting the request";
		document.getElementById("email_results1").style.display="block";
	}
}
</script>



