fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));
</script><div class="masthead"><div class="content"><div class="corner-container"><div class="content"><div class="greeting"></div><div class="nav language bar"><ul><li><a href="/" class="english active">English</a></li><li><a href="http://fr.allafrica.com/" class="french">En Français</a></li></ul></div><!--/.nav.language.bar--></div><!--content--></div><!--/.corner-container--><div class="search"><a class="my-account" href="/commerce/user/manage/">My Account</a><form style="float: right;" action="/search/" class="search" method="GET" name="searchform"><input type="text" value="" name="search_string" size="20" maxlength="42" onchange="document.searchform.submit()" /><input type="submit" name="search-submit" value="Search" /></form></div><!--/.search--><div class="clear"></div><div class="logo"><h1><a class="aa" href="http://allafrica.com/">AllAfrica</a></h1></div><div class="clear"></div><div class="nav secondary"><div class="content"><div class="sites tabs"><ul><li class="first"><a href="/inhouse/">By AllAfrica</a></li><li><a href="http://myafrica.allafrica.com/">myAfrica</a></li><li><a href="http://travel.allafrica.com/">Travel</a></li><li><a href="http://blogafrica.allafrica.com/">blogAfrica</a></li><li class="last"><a href="/list/publisher/editorial/editorial/type/pub.html">Sources</a></li></ul></div><!--/.tabs--></div><!--content--></div><!--/.nav.secondary--></div><!--/.content--></div><!--/.masthead--><div class="nav primary"><div class="content"><ul class="first-tier"><li class="first countries"><a href="/misc/sitemap/countries.html"><span>Countries</span></a><ul class="second-tier"><li class="has-children"><a href="/misc/sitemap/countries.html">All&nbsp;Countries</a><div class="third-tier"><div><a href="/algeria/">Algeria</a><a href="/angola/">Angola</a><a href="/benin/">Benin</a><a href="/botswana/">Botswana</a><a href="/burkinafaso/">Burkina&nbsp;Faso</a><a href="/burundi/">Burundi</a><a href="/cameroon/">Cameroon</a><a href="/capeverde/">Cape&nbsp;Verde<