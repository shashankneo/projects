			return false;
		}
	}

	function getAdv(size){
		try{
			return vizPAM.result[size]['adv'];
		}catch(i){
			return '';
		}
	}

	function getImpr(size){
		try{
			return vizPAM.result[size]['impid'];
		}catch(i){
			return '';
		}
	}

	function getSizes(){
		
		try {
			for( var size in vizPAM.result){
				sizes[itemp]=size;
				itemp++;
			} return sizes;      
		} catch (i) {
				return '';
		}
	}
