			var twits = jQuery("#story_content_parts #part1").html();
			jQuery( ".full-details p:nth-child(2)" ).after( twits );
			
			jQuery(".main-story .story-image img").each(function(){
				var width = jQuery(this).width(); 
				if(width <= 480){
					jQuery(".main-story").css({"float":"left", "width":"480"});
				}else{
					jQuery(this).css({"float":"none", "width":"100%", "height":"auto", "margin":"0 auto", "display":"block",});
				}
			});
			jQuery(".full-details img").each(function(){
				var width = jQuery(this).width();
				if(width > 480){
					jQuery(this).css({"float":"none", "width":"100%", "height":"auto", "margin":"0 auto", "display":"block",});
				}
			});
		}
