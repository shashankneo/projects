<script>
if(!$.cookie('nl_popup')){
	$('.nletter_popup').fadeIn();
	$('#popup-letter input[type="button"]').click(function() { newsletter('popup-letter'); });
	$('.subsc_popup input[type="checkbox"],.nletter_popup, #letter-x').click(function(){ SetCookie('nl_popup', 'ischoolguide.com', 1);ClearPopup('article.nletter_popup'); });
	$('.subsc_popup').click(function(e){e.stopPropagation();});
}
function SetCookie(na, dom, date){
	$.cookie(na, '1', { path: '/', domain: dom, expires: date });
}
function ClearPopup(na){
	$(na).fadeOut(); return false;
}
</script>
