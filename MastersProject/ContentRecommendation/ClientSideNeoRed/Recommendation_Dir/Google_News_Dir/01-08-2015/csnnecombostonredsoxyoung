        <div class="article-extra-left-1 article-extra">
          <div class="panel-pane pane-views-panes pane-rsn-articles-panel-pane-6"  >
  
      
  
  <div class="pane-content">
    <div class="view view-rsn-articles view-id-rsn_articles view-display-id-panel_pane_6 view-dom-id-e476e81313bd0e79332268188def9c9e">
        
  
  
      <div class="view-content">
        <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
      
  <div class="views-field views-field-field-person-picture">        <div class="field-content"><a href="http://twitter.com/Jimmy_Toscano"><img typeof="foaf:Image" src="http://www.csnne.com/sites/csnne/files/2014/06/06/byline-toscano14c.png" width="249" height="75" /></a></div>  </div>  </div>
    </div>
  
  
  
  
      <div class="view-footer">
      <iframe allowtransparency="true" frameborder="0" scrolling="no" src="//platform.twitter.com/widgets/follow_button.html?screen_name=Jimmy_Toscano&show_count=false" style="width:300px; height:20px;"></iframe>    </div>
  
  
