	<script type="text/javascript">
		
			
				$("ul.navigation.main > li").css('float', 'left');
				var nUsedWidth  = 0;
				var nTotalWidth = $("ul.navigation.main").width();
				$("ul.navigation.main > li").each(function(){
					nUsedWidth += $(this).outerWidth();
				});
				var nUnusedWidth = nTotalWidth - nUsedWidth;
				var nGapCount = $("ul.navigation.main > li").length - 1;
				var nGapWidth = nUnusedWidth / nGapCount;
				for (i=0; i<nGapCount; i++) {
					$("ul.navigation.main > li").eq(i).css('margin-right', nGapWidth);
				}		
					$("ul.navigation.main > li").eq(nGapCount).css('margin-right', "-1px");
			
		

		

		

                
                    var menuOffset = $('#main-nav-container').offset();
                    var fullWidth =  $("#blox-container").width();
					var nStickyCombined = $('#main-nav-container h2#nav-full-width-logo').width() + 960;
                
                    $(window).scroll(function() {
                        var docScroll = $(document).scrollTop();
                
                        if (docScroll >= menuOffset.top) {
							if($(window).width() < 960) {
								$('#main-nav-container').addClass("narrow-width");
							}
                            $('#main-nav-container').css("width", "100%").addClass("stuck-nav");
							if(nStickyCombined  < $(window).width()) {
								$('#main-nav-container h2#nav-full-width-logo').show();
							}
                        } else {
                            $('#main-nav-container').removeClass("stuck-nav").removeClass("narrow-width").css("width", fullWidth);
                            $('#main-nav-container h2#nav-full-width-logo').hide();
                        }
                
                    });
                
		
		
		
		
		
		
		
		

		
			$(".mega-nav-container").each(function() {

			
				var bLeftSideEmpty = "true";
				$(".nav-content-left .tncms-region", this).each(function() {
					if($(this).html() != '') {
						bLeftSideEmpty = "false";
					}
				});
				if(bLeftSideEmpty == "true"){$(".nav-content-left", this).addClass("hide");}
				
				var bRightSideEmpty = "true";
				$(".nav-content-right .tncms-region", this).each(function() {
					if($(this).html() != '') {
						bRightSideEmpty = "false";
					}
				});
				if(bRightSideEmpty == "true"){$(".nav-content-right", this).addClass("hide");}
				if(bLeftSideEmpty == "true" && bRightSideEmpty == "true") {
					$(this).addClass("unmega");
					var unmegaPosition = $(this).parent().position();
					if(unmegaPosition.left + 160 > 960) {
						$(this).css('right', '0px');
					}
				}
				if((bLeftSideEmpty == "true" && bRightSideEmpty == "false") || (bLeftSideEmpty == "false" && bRightSideEmpty == "true")){
					$(this).addClass("onemega");
					var onemegaPosition = $(this).parent().position();
					if(onemegaPosition.left + 550 > 960) {
						$(this).css('right', '0px');
					}
				}
			});
			
		
		
	</script>

