    cmg.query(document).ajaxError(function(evnt, jqxhr) {
        var error;
        try { error = JSON.parse(jqxhr.responseText); }
        catch(e) { error = jqxhr.responseText; }
        // Uncomment next line in local dev to see ajax errors in console
        // console.error('ajax error:', error);
    });

    
    cmg._ = _.noConflict();
    
    cmg.error_dialog = function(msg) {
        cmg.query('<div/>').addClass('cmErrorDialog')
            .text(msg = msg || 'Oops, something went wrong. Please reload the page and try again.')
            .appendTo('body');
    };
    
    cmg.get_cookie = function get_cookie(name) {
        var cookie_array = document.cookie.split(';');
        var cookie_regex = new RegExp('^ *' + name + '="?([^"]+)"?');
        for (var x in cookie_array) {
            if(!cookie_array.hasOwnProperty(x)) continue;
            var match = cookie_array[x].match(cookie_regex);
            if(match){
                return match[1];
            }
        }
    };
    
    cmg.parse_qs = function parse_qs(str) {
        if (!str) { return {}; }
        if (str[0] === '?') { str = str.slice(1); }
        var ret = {};
        var query_args = str.split('&');
        for (var key in query_args) {
            if (!query_args.hasOwnProperty(key)) { continue; }
            var tuple = query_args[key].split('=');
            ret[tuple[0]] = tuple[1];
        }
        return ret;
    };
    cmg.get_user = function (views_total_count) {
        if (!views_total_count) { return {}; }
        var same_domain = window.location.hostname == 'www.ajc.com';
        cmg.query.ajax({
            type: 'POST',
            dataType: same_domain ? 'json' : 'jsonp',
            url: 'http://www.ajc.com/auth/ajax/loginclient-user-data',
            data: {
                views_total_count: views_total_count,
                nmredir: "true"
            },
            success: function(data) {
                if (data.result.photos[0]) {
                    var picURL = data.result.photos[0].value;
                    cmg.query("#cm-user-icon").css({'background-image' : 'url(' + picURL + ')'});
                }
                var menu_timer;
                cmg.query('.cm-user-hover-target').hover(function() {
                    var $this = this;
                    menu_timer = setTimeout(function() {
                        if (cmg.query($this).is(":hover")) {
                            cmg.query('#cm-janrain-overlay-container').show();
                        }
                    }, 500);
                }, function() {
                    var $this = this;
                    menu_timer = setTimeout(function() {
                        if (!cmg.query($this).is(":hover")) {
                            cmg.query('#cm-janrain-overlay-container').hide();
                        }
                    }, 500);
                });
            }
        });
    };
    
    cmg.update_auth_message = function(str) {
        var cookie_name = cmg.query.cookie('ur_name', {path:'/'});
        var views_total_count = cmg.query.cookie('ur_views_total_count', {path:'/'});

        if (typeof cookie_name === 'string' && typeof views_total_count == 'string') {
            cmg.get_user(views_total_count);
            
            if (cmg.query('#cm-user-icon').css('background-image') != '') {
                cmg.query('#cm-user-icon').css({
                        'background-image': 'url(http://www.ajc.com/profile/avatar_jp2/' + encodeURIComponent(cookie_name) + '/)'
                        }).removeClass('cmOpenJanrainModal cm-icon cm-icon-user').addClass('cm-user-avatar');
            }
            cmg.query('.cm-user-hover-target').addClass('cm-janrain-active');
            cmg.query('#cmUserNameUpdate').text(cookie_name);

        } else {
            cmg.query('#cm-user-icon').css({
                    'background-image': 'none'
                    }).attr('href', '#');
            cmg.query('#cm-user-icon').removeClass('cm-user-avatar').addClass('cmOpenJanrainModal cm-icon cm-icon-user');
        }
    }
    
