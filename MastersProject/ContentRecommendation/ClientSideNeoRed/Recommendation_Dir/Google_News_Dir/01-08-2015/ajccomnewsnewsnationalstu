            </div><div class="cm-lc-view" id="livefyre-chatter-app"></div><div class="cm-lc-nocontent"><p>There are no comments yet.  Be the first to post your thoughts.  <span class="cm-lc-not-logged-in"><a href="#signin" class="cmOpenJanrainModal" title="Sign in">Sign in</a> or <a href="#register" class="cmOpenJanrainModal" title="Register">register</a>.</span></p></div></div><div class="cm-lc-footer"></div></div></div><script>
    var janrain_timeout_int = 500,
        livefyre_livecomments = null,
        $comment_count_display = cmg.query('.cm-comment-count, .cm-navbar-comment-bubble'),
        $chatter_large = cmg.query('.cm-lc-widget-expanded'),
        $chatter_large_close = cmg.query('.cm-lc-widget-close'),
        toggle_expanded_chatter = function($) {
            var $ = $ || cmg.query,
                $pinned_header = $('.cm-navbar-pinned-story-container'),
                $pinned_header_comment_count = $('.cm-navbar-comments'),
                $chatter_large_content = $('.cm-lc-widget-expanded .cm-lc-widget-container'),
                $modal_close = $('.cm-close-modal'),
                height = $(window).height() - 80;
            if ($chatter_large.is(':visible')) {
                $chatter_large
                    .animate({
                        height: 0
                    }, {
                        duration: 200,
                        complete: function() {
                            $chatter_large_close.fadeOut();
                            $modal_close
                                .fadeIn()
                                .removeClass('cm-hidden');

                            $(this).fadeOut('slow', function() {
                                /* Reset header after the fadeOut is complete. This has to happen after the
                                 * fadeOut because cmgMainNav.headline_check checks whether the widget is
                                 * visible to determine whether or not to display the headline, and it's
                                 * visible during the animation. */
                                if ($pinned_header.length) {
                                    $pinned_header_comment_count.removeClass('active');
                                    if (typeof cmg.cmgMainNav !== 'undefined') {
                                        if (typeof cmg.cmgMainNav.show_navbar_for_height === 'function') {
                                            cmg.cmgMainNav.show_navbar_for_height();
                                        }
                                        if (typeof cmg.cmgMainNav.headline_check === 'function') {
                                            cmg.cmgMainNav.headline_check()();
                                        }
                                    }
                                }
                            });
                        }
                    });
            } else {
                $chatter_large
                    .fadeIn('slow')
                    .animate({
                        height: height
                    }, {
                        duration: 200,
                        complete: function() {
                            if ($pinned_header.length) {
                                $pinned_header_comment_count.addClass('active');
                                if (typeof cmg.cmgMainNav !== 'undefined' && typeof cmg.cmgMainNav.display_story_headline !== 'undefined') {
                                    cmg.cmgMainNav.display_story_headline();
                                }
                            }
                            $chatter_large_close.fadeIn();
                            $modal_close
                                .fadeOut()
                                .addClass('cm-hidden');
                            $(this).css({
                                overflow: 'visible'
                            })
                        }
                    });
            }
        },
        set_comment_count = function($) {
            var comment_count = $('.fyre-stream-stats .fyre-comment-count').text().replace(/[^0-9]+/g, '');
            $comment_count_display.text(comment_count);
        },
        update_comment_count = function(count) {
            $comment_count_display.text(count);
        },
        cmg_ping_to_pull = function() {
            if (typeof janrain != 'undefined' && localStorage && localStorage.getItem('janrainCaptureProfileData')) {
                var profileData = localStorage.getItem('janrainCaptureProfileData');
                profileData = typeof profileData == "string" ? JSON.parse(profileData) : profileData;
                if (profileData.uuid) {
                    jQuery.get('/auth/ajax/ping-pull/' + profileData.uuid, function(data) {
                        if (data.indexOf('http') !== -1) {
                            jQuery.get(data, function(response) {
                                // do nothing; the request kicks off a sync on livefyre's side
                            });
                        } else {
                            //console.error(data);
                        }
                    });
                } else {
                    //console.error('Problem with uuid/profileData: ', profileData);
                }
            }
        },
        check_janrain_loggedin_status = function() {
            var cookie_ur_name = cmg.query.cookie('ur_name', {path: '/'});
            if (!janrain.capture.ui.hasActiveSession() && typeof cookie_ur_name !== 'string') {
                // not signed in but livefyre thinks we are
                // delete the backplane-channel cookie and
                // remove the fyre-auth entry from localstorage
                clear_livefyre_creds();
            }
        },
        clear_livefyre_creds = function() {
            var yesterday = new Date();
            yesterday.setTime(yesterday.getTime() + (-1 * 24 * 60 * 60 * 1000));
            document.cookie = 'backplane-channel' + '=; expires=' + yesterday.toUTCString() + '; path=/';
            localStorage.removeItem('fyre-auth');
        },
        livefyre_commenting_init = function() {
    //      console.log('commenting init; backplane: ' + typeof window.Backplane, janrain.capture, localStorage['fyre-auth']);
            var $ = cmg.query,
                cookie_ur_name = cmg.query.cookie('ur_name', {path: '/'});
            if (typeof window.Backplane !== 'undefined' && !livefyre_livecomments) {
                janrain.events.onCaptureLoginFailed.addHandler(check_janrain_loggedin_status());
                var lf_network_config = {
                        network: 'coxnews.fyre.co',
                        strings: {
                            postAsButton: "Comment"
                        }
                    },
                    lf_conv_config = [{
                        siteId: '365676',
                        collectionMeta: 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..LiTKCoii9uvSXZ-yXwU0WJug3fLs3z84St6H3yxzkPU',
                        checkSum: '9ba8c7934b5890caca50f9715c3129c6',
                        articleId: '3011631',
                        'delegate': 'authDelegate',
                        el: 'livefyre-chatter-app'
                    }],
                    $chatter_module = $('.cm-lc-widget-collapsed, .cm-comments-heading'),
                    authDelegate = new fyre.conv.BackplaneAuthDelegate(window.Backplane),
                    livefyre_livecomments = fyre.conv.load(
                        {
                            "network": "coxnews.fyre.co",
                            "authDelegate": authDelegate
                        },
                        lf_conv_config,
                        function (widget) {
                            // callbacks
                            widget.on('commentCountUpdated', function(data){
                                if (data > 0) {
                                    $('.cm-lc-nocontent').hide();
                                    if ($('.cm-lc-widget.cm-lc-empty').length) {
                                        $('.cm-lc-widget.cm-lc-empty').removeClass('cm-lc-empty');
                                    }
                                    update_comment_count(data);
                                }
                            });
                            widget.on('userLoggedIn', function() {
                                if (janrain.capture.ui.hasActiveSession()) {
                                    $chatter_module.show();
                                    cmg_ping_to_pull();
                                }
                            });
                            widget.on('initialRenderComplete', function() {
                                $chatter_module.show();
                                set_comment_count(cmg.query);
                                if ($('.fyre-auth .fyre-user-loggedin').length) {
                                    $('.cm-lc-nocontent .cm-lc-not-logged-in').hide();
                                }
                                if (flipper.is_active('DTMmetrics_Enable')){
                                    if($('div.fyre-stream-more').length){
                                        $('div.fyre-stream-more').find('*').on('click',function(e){
                                            cmg.DDO.action("additionalComments");
                                        });
                                    }
                                }
                            });
                        }
                    );

                authDelegate.login = function(delegate) {
                    janrain.events.onCaptureLoginFailed.addHandler(check_janrain_loggedin_status());
                    var successCallback = function() {
                        janrain.events.onCaptureLoginSuccess.removeHandler(successCallback);
                        delegate.success();
                    };
                    var closeCallback = function() {
                        // If the user closes the login window without logging in, the livefyre auth
                        // request needs to trigger failure events.
                        delegate.failure();
                    };
                    janrain.events.onCaptureLoginSuccess.addHandler(successCallback);
                    $('.janrain-close-modal').click(closeCallback);
                    $('.cmOpenJanrainModal').trigger('click');
                };
                authDelegate.editProfile = function(delegate, author) {
                    location.href = '/profile/edit/private?next=' + location.href;
                };
                authDelegate.viewProfile = function(delegate, author) {
                    cmg_ping_to_pull();
                };
                authDelegate.logout = function(delegate) {
                    janrain.capture.ui.endCaptureSession();
                };
            } else if (!livefyre_livecomments) {
                setTimeout(function() { livefyre_commenting_init(); }, janrain_timeout_int);
                janrain_timeout_int = janrain_timeout_int * 2;
            }
        };

    cmg.query(document).on('ready', function(e) {

        var $ = cmg.query;

        $.getScript('http://zor.livefyre.com/wjs/v3.0/javascripts/livefyre.js')
            .done(function() {
                livefyre_commenting_init();
            })
            .fail(function() {
                $('.cm-lc-widget').hide();
                if (typeof console !== 'undefined') {
                    console.error('Fetch of http://zor.livefyre.com/wjs/v3.0/javascripts/livefyre.js failed.  Hiding commenting.');
                }
            });

        if (location.search.indexOf('showcomments') > -1) {
            toggle_expanded_chatter($);
        }

        $chatter_large.css({
            right: function() {
                return ($(window).width() - $('.cm-modal-content').width()) / 2
            }
        });

        $chatter_large_close.css({
            right: function() {
                return ($(window).width() - $('.cm-modal-content').width()) / 2 + $chatter_large.width() - ($(this).width() / 2)
            }
        });

        $('a.cm-lc-expand, a.cm-story-comments, div.cm-lc-widget-close, .cm-navbar-comments').off('click.lc-expand').on('click.lc-expand', function(e) {
            e.preventDefault();
            toggle_expanded_chatter($);
        });
    });
