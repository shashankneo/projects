            function makeAdCalls() {
                cmg.ads.size.bind();
                
                    cmg.harmony.load({
                        targeting: {
                            ybot: yieldbot.getPageCriteria()
                        }
                    });
                
                googletag.enableServices();
                cmg.harmony.show.breakpoint('0px-infinity');
            }

            
            function postload() {
                cmg.harmony.load({
                    slots: cmg.ads.conf.flush()
                });
                cmg.query(window).trigger('ads/ready');
                cmg.harmony.trigger('ready');
            }

            
                cmg.ads.conf.load(conf.slots);
                srmset = cmg.ads.conf.filter(
                    cmg.query('.adgeletti-ad-div.bp-0px-infinity')
                );
                cmg.harmony.load({
                    slots: srmset,
                    targeting: conf.targeting
                });
                googletag.pubads().enableSingleRequest();
                makeAdCalls();

                
                if (srmset.length) {
                    googletag.pubads().addEventListener('slotRenderEnded', function () {
                        if (!loaded) {
                            loaded = true;
                            postload();
                        }
                    });
                } else {
                    postload();
                }
            
        });
    });
