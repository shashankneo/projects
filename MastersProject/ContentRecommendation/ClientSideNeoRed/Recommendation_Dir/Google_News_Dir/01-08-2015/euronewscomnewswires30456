<script type="text/javascript"> 
var ajaxActions = {"logout":"logoutTopFrame", "login":""},
    ajaxData = {"form-action":"bootstrap", "success":"logout"};

function doNextStep(msg) {
        var action;                                
        if("success" in msg && msg.success == 1 && ("result" in msg)) {               
                for(action in msg.result) {  
                        if((action in ajaxActions)) {                      
                                if(action == "login") {
                                        renderLoginSignupForm();
                                } else if(ajaxActions[action] != "" && $("#" + ajaxActions[action]).length) {
                                        $("#" + ajaxActions[action]).html(msg.result[action]);        
                                } else {
                                        $(document.body).append(msg.result[action]); 
                                };
                        };
                };                
        };  
};

function highlight(elem, err) {
        if(err) { $("#" + elem).addClass("err"); } else { $("#" + elem).removeClass("err"); };
};
                                                    
(function() {
        if(!EN.cookie.readCookie("EN_lgn")) {
                renderLoginSignupForm();
        } else {
                $.ajax({
                        type: "POST",
                        url:  String(window.location),
                        data: ajaxData,                        
                        dataType: "json",
                        success: function(msg){
                                doNextStep(msg);                                                   
                        },
                        failure: function(msg) {
                                                                    
                        }
                });
        };  
})();                          

function renderLoginSignupForm() {        
        if($("#cummunityLinks").length) {
                return;
        };       
        $("#community-bar-inner").append('<div id="cummunityLinks"><a href="/community/subscribe" id="signup-init" title="Join us">Join us</a> <a href="#login" id="login-init" title="Log in">Log in</a></div>');
        //$.colorbox.init();
        $("#community-bar").show();        
        $("#login-init").click(function() {        
                $("#boxHover_overlay").fadeIn(500,function () {$("#boxHover_content").fadeIn(500);});
                //$.colorbox({scrolling:false,width:"320px",inline:true, href:"#login-signup-wrap", overlayClose:false, onComplete:function(e) {$("#unm").focus();}});
                return false;
        });
};
</script>
    <script type="text/javascript">
$("#login-signup-wrap-close").click(function(e) {
	e.preventDefault();    
	$("#boxHover_content").fadeOut(500,function () {$("#boxHover_overlay").fadeOut(500);});
});

$("#lgn-signup").click(function() {
        //$.colorbox.close();
		
	$("#boxHover_content").fadeOut(500,function () {$("#boxHover_overlay").fadeOut(500);});
});
$("#login").submit(function(evt){         
        evt.preventDefault();       
        var ajaxPayload = {
                "success":("success" in ajaxData) ? ajaxData.success : "logout",
                "unm":$("#unm").val(),
                "pwd":$("#pwd").val(),
                "form-action":"login"
                        },
        err = 0;
    
        if(ajaxPayload["unm"] == "") {
                err++;
                highlight("unm", 1);
                $("#login-message").html("Username is a required field");    
        } else {
                highlight("unm", 0);
                if(ajaxPayload["pwd"] == "") {
                        err++;
                        highlight("pwd", 1);
                        $("#login-message").html("Password is a required field");    
                } else {
                        highlight("pwd", 0);
                };
        };

        if(err) {
                $("#login-message").addClass("err");
                return false;
        } else {
                $("#login-message").html("Loading…");
                $("#login-message").removeClass("err");
                $("#login-button-init").addClass("disabled");
                $("#login-button-init").attr("disabled", true);
                $("#lgn-signup-callout").hide();       
        };
       
        $("#login").addClass("isloading");
        $("#cboxClose").hide();
                        
        $.ajax({
                type: "POST",
                url:  String(window.location),
                data: ajaxPayload,                        
                dataType: "json",
                cache:false,
                success: function(msg){                                        
                        $("#cboxClose").show();
                        if("success" in msg) {
                                $.colorbox({html:"<p style='padding:0 25px;margin-bottom:0'><em>" + msg.unm + ".</em></p>"});
                                doNextStep(msg);
                        } else if("formErrors" in msg) {
                                var map = ["unm", "pwd"];
                                
                                for(var i = 0, elem; elem = map[i]; i++) {
                                        if(elem in msg.formErrors) {
                                                $("#" + elem).addClass("err");
                                        } else {
                                                $("#" + elem).removeClass("err");
                                        };
                                };
                                
                                if("message" in msg) {
                                        $("#login-message").html(msg.message);  
                                        $("#login-message").addClass("err");
                                        $.colorbox.resize();
                                };
                                
                                $("#login-button-init").removeClass("disabled");
                                $("#login-button-init").attr("disabled", false);
                                
                                $("#login").removeClass("isloading");
                                $("#lgn-signup-callout").show();        
                        } else {
                                $.colorbox({html:"Apologies, we encountered an error on the server and could not proceed. Please try again later."});
                                $.colorbox.resize();
                        };         
                },
                failure: function(msg) {
                        $.colorbox({html:"Apologies, we encountered an error on the server and could not proceed. Please try again later."});
                        $.colorbox.resize();
                }
        }); 
       
        return false;        
});
    </script>
