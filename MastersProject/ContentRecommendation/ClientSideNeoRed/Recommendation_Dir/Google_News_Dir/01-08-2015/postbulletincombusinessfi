function swapSubstituteHTML(id,element){
    if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"swapping active video"});}
    var playURL = $($(element).parents(".item")[0]).find(".video-url");
    var width = $(id).width();
    var height = $(id).height();
    
    if(playURL.length){
        $(id).html("");
        
        $(id).html(playURL.html().replace(/<!--\(TEMPLATE|\/TEMPLATE\)-->/g,'').replace('"','\"'));
        
        $(id).css({"height":height,"width":width});

        $(id).find("embed").css({"height":height,"width":width});
        $(id).find("embed").attr({"height":height,"width":width});   
    }
}

}

