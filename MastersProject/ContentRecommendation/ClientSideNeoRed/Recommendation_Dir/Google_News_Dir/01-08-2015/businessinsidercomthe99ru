	<div data-bi-ad class="ad dfp rubicon" data-adunit="desktop/finance/clusterstock/post" data-authors="gselevator" data-pagetype="post" data-region="Upper 300" data-rubicon_zone_id="" data-sizes="300x250,300x600,300x400,300x1050" data-tag="features,culture,gentleman,man-cave,gselevator,rules" data-url="/the-99-rules-all-men-should-live-by-2015-7" data-views="10001-500000"></div>

	<script type="text/javascript">
		(function() {
			'use strict';
