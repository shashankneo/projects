<!-- Sponsored Post Tag -->
    
<h1>The 99 rules all men should live by</h1>

<div id="content" class="content">
    <div class="post-top">
        <!-- Byline -->
        <div class="flex byline">
			<div class="flex-auto byline-icon">
							<a class="author-photo" href="http://www.businessinsider.com/author/gselevator"><img class="author" src="http://static3.businessinsider.com/image/51f16fb969bedd831e000004-50-50/gselevator.jpg"></a>
					</div>
	    
	
	<div class="flex-item">
		<div class="list-pipes">
			<ul>
																		<li class="single-author"><a href="http://www.businessinsider.com/author/gselevator" rel="author">GSElevator</a></li>
		                <li>
    <div class="contact">
        
        
        
                    <a href="http://gselevator.wordpress.com/" data-toggle="tooltip" data-placement="top" title="" data-original-title="http://gselevator.wordpress.com/"><i class="fa fa-link"></i></a>
        
