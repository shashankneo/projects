                            //jsinit - 52
                CNBC_Utils.execute('chartbeat most popular', function(){
			var refreshInterval = 300;
			function renderModule() { 
				$.ajax( { 
					url: 'http://www.cnbc.com/id/102535788/services/chartbeat', 
					success: function(data) { 
						var result = $.trim($(data).text());
						if(!result) { 
							return false;
						} ;
						if( $.trim($('#chartbeatWidget_102535788').text()) != result ) { 
							$('#chartbeatWidget_102535788').html(data);
						} ;						
					}  
				} );
			} ;
			renderModule();
			if(refreshInterval) {
				window.setInterval(renderModule, refreshInterval*1000 );
			};			
		}, 'chartbeatWidget_102535788');
                            //jsinit - 53
                CNBC_Utils.execute('social gigya bar', function(){

			var shareButtons = [];
			shareButtons.push({ provider:'twitter', iconImgUp: 'http://fm.cnbc.com/applications/cnbc.com/staticcontent/img/social_tools/icon-social-twitter-35px.png'});
			shareButtons.push({ provider:'facebook', iconImgUp: 'http://fm.cnbc.com/applications/cnbc.com/staticcontent/img/social_tools/icon-social-facebook-35px.png'});
			shareButtons.push({ provider:'reddit', iconImgUp: 'http://fm.cnbc.com/applications/cnbc.com/staticcontent/img/social_tools/icon-social-reddit-35px.png'});
			shareButtons.push({ provider:'linkedin', iconImgUp: 'http://fm.cnbc.com/applications/cnbc.com/staticcontent/img/social_tools/icon-social-linkedin-35px.png'});
			shareButtons.push({ provider:'email', iconImgUp: 'http://fm.cnbc.com/applications/cnbc.com/staticcontent/img/social_tools/icon-social-email-35px.png'});

			
				shareButtons.push({ provider:'share',enableCount:false, iconImgUp: 'http://fm.cnbc.com/applications/cnbc.com/staticcontent/img/social_tools/icon-social-share-nobox-35px.png'});
			

			//isMobile = 
			var moreEnabledProviders = '';
			
				
					moreEnabledProviders = 'facebook, reddit, twitter, delicious, linkedin, tumblr, googleplus, pinterest, digg, stumbleupon';
				
			

			var params = {
				userAction: window.ua
				,'shareButtons': shareButtons
				,containerID: 'social_bar_top'
				,showCounts : 'none'
				,iconsOnly: 'true'
				,deviceType: 'auto'
				
					
						,onSendDone:
						
							CNBC_Gigya_Omniture.onSendDoneTop
						 // onSendDone method is called after Gigya finishes the publishing process.
					
					
					
					,onLoad : function(e) {
						var targetID = e.containerID;
						var options = {
						callback : fnResponse
						};
						function fnResponse(response){
							var shareCount = formatTotal(getTotal(response));
							var container = $('#'+targetID).prev();
							if(shareCount) {
								container.removeClass('hide');
								container.find('.share-total').text(shareCount);
							};
						};
						function getTotal(r) {
							var total=0;
							for (var item in r.shareCounts){
								if (r.shareCounts.hasOwnProperty(item) && typeof r.shareCounts[item]=='number') {
									total+=r.shareCounts[item];
								};
							};
							return total;
						};
						function formatTotal(t) {
							if(!t || typeof t!='number' || t<2) { return false; };
							if(t<1000) {
								return t;
							}
							else if(t>=1000 && t<10000) {
								return (Number(t.toString().slice(0, -2))/10).toFixed(1)+'K';
							}
							else if(t>=10000) {
								return Math.floor(t/1000)+'K';
							};
						};
						gigya.socialize.getProviderShareCounts(options);
					}
				
				
				,onConnectionAdded: CNBC_Gigya_Omniture.onConnectionAdded // Fired whenever a user is connected to a provider
				,onConnectionRemoved: CNBC_Gigya_Omniture.onConnectionRemoved
				,onLogin: CNBC_Gigya_Omniture.trackLoginEvent // call trackLoginEvent when Social Login finishes successfully
				,onLogout: CNBC_Gigya_Omniture.onLogout
				,showEmailButton:false
				,moreEnabledProviders: moreEnabledProviders
			};
			gigya.services.socialize.showShareBarUI(params);
		});
