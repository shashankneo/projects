                // Getting quotes
		
			CNBC_Utils.execute('get quotes', function() {
				
				
					var tableChartRenderer = new CNBC_Quotes.TableChartRenderer();
				
				
				
				var chartApiUrl =  'http://api-chart.cnbc.com/api/chart/chart.asp';
				var quoteService = 'http://quote.cnbc.com/quote-html-webservice/quote.htm';
				var quoteServiceParams = 'requestMethod=quick&exthrs=1&noform=1&fund=1&output=jsonp&symbols=';
				var partnerId = 2;
				var quoteUrl  = quoteService + '?partnerId=' + partnerId + '&' + quoteServiceParams;
				var futureQuoteUrl = 'http://quote.cnbc.com/quote-html-webservice/fvquote.htm';
				var futureQuoteParams = 'requestMethod=quick&noform=1&realtime=0&client=fairValue&output=jsonp&symbols=';
				var futureUrl =  futureQuoteUrl + '?' + futureQuoteParams;
	
				var settings = {
				    maxSymbolsPerRequest: 30
			    
				    ,streaming: true
				  	,streamingInterval: 10000
			  	
				  	,detailedQuoteBase: CNBC_Settings.baseurls.detailedquotebase
				};
	
				CNBC_Quotes.init(chartApiUrl, [
					
					{
						url: quoteUrl
						, listeners: [null, tableChartRenderer]
					  	, settings: settings
					}
					
					
				]);			
			});
                            //jsinit - 60
                CNBC_Utils.execute('Tablet Friendly Touch', function() {
	
			if(CNBC_Utils.isTouchEnabled() == true) {
				var tabletFriendlyTouch = new CNBC_TabletFriendlyTouch({ targetElement: $('.cnbc-menu').find('a') });
				tabletFriendlyTouch.init({ targetElement: $('.cnbc-menu').find('a') });
			}		
	
		});
                            //jsinit - 61
                CNBC_Utils.execute('table sort', function(){
		if($('table.csvData, article table.quoteTable').length) {
			$('table.csvData, article table.quoteTable').CNBC_table_sorter({
				exclude : {
					'class' : ['arrow'],
					'tag' : ['img']
				}
			});
		}
	});
                            //jsinit - 62
                CNBC_Utils.execute('Remove URL Params', function() {
				$(function () {
					var intervalVar, url, maxTries, tries, sleepMiliseconds;
					sleepMiliseconds = 2000;
					maxTries = 10;
					tries = 0;
					intervalVar = setInterval(function () {
						tries++;
						if (tries > maxTries) {
							clearInterval(intervalVar);
							return;
						}
						if (window.s && window.s.prop1) {
							url = CNBC_Utils.removeUrlParams(window.location.href, ["trknav"]);
							CNBC_Utils.replaceBrowserUrl(url);
							clearInterval(intervalVar);
						}
					}, sleepMiliseconds);
				});
			});
                            //jsinit - 63
                CNBC_Utils.execute('Chartbeat', function(){
					window._sf_async_config = window._sf_async_config || { };					
					/** CONFIGURATION START **/
					
						window._sf_async_config.playerdomain = 'player.cnbc.com'; //For the Platform Player
					
					window._sf_async_config.authors      = 'Jane Wells,CNBC US Source';
					window._sf_async_config.sections     = 'Articles,Economy,Entrepreneurs,Food Retail,Weather,US: News,Utilities,Water Utilities,Agriculture,Water,Sam Zell,Whole Foods Market Inc';
					window._sf_async_config.zone         = mps.pagevars.zone;
					/** CONFIGURATION END **/
					(function(){
						function loadChartbeat() {
							window._sf_endpt=(new Date()).getTime();
							var e = document.createElement('script');
							e.setAttribute('language', 'javascript');
							e.setAttribute('type', 'text/javascript');
							e.setAttribute('src', '//static.chartbeat.com/js/chartbeat_pub.js');
							document.body.appendChild(e);
						}
						$(document).ready(function(){
							loadChartbeat();
						});
					})();
				});
                            //jsinit - 64
                CNBC_Utils.execute('CommentsCount', function(){
			CNBC_Settings.commentsCountParam = {
                provider: {
                    lib:         'CNBC_Disqus',
                    apiEndpoint: 'http://commentsapi.cnbc.com',
                    authTimeout: 1
                },
                disqus: {
                    shortname:      'cnbc'
                },
                label: {showZero: 1, position: 'after', templates: {zero: 'Ghost town thread', singular: ' $COUNT$ comment ', plural: ' $COUNT$ comments '}, templatesArticle: {zero: '<div class=\"story_commentsCount zero\"><span id=\"comments-text\">COMMENTS</span><a id=\"discussion\">Start the Discussion</a></div>', singular: '<div class=\"story_commentsCount\"><div id=\"total-comments\"><span>$COUNT$</span></div><span id=\"comments-text\">COMMENT</span><a id=\"discussion\">Join the Discussion</a></div>', plural: '<div class=\"story_commentsCount\"><div id=\"total-comments\"><span>$COUNT$</span></div><span id=\"comments-text\">COMMENTS</span><a id=\"discussion\">Join the Discussion</a></div>'}},
                anchorAttrName: 'data-disqus-identifier'
			};
			
				var commentsObj = new CNBC_Comments();
				commentsObj.counts(CNBC_Settings.commentsCountParam);
			
		});
