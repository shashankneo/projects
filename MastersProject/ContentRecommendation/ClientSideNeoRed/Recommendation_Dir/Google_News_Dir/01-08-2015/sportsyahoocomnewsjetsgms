<section id="mediacontentstory" class="yom-mod yom-card-main yom-article" data-uuid="bb36f11d-c495-36c9-9515-5fde9150bdea" data-type="story" data-ylk="t1:a3;t2:ct-mod;sec:ct-mod;itc:0;rspns:nav;">
