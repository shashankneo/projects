        <section class="ib-container-item first item-0">
                        <div>
        <div class="article" itemscope itemtype="http://www.schema.org/NewsArticle">
            <meta itemprop="url" content="http://www.kwch.com/news/national/zimbabwe-calls-for-dentists-extradition/34457486"/>
<div itemprop="publisher" itemscope itemtype="http://schema.org/Organization">
        <meta itemprop="name" content="kwch.com"/>
            </div>
<meta itemprop="dateCreated" content="2015-08-01T10:14:27"/>
    <meta itemprop="dateModified" content=""/>
<hgroup>
                <div class="header">
                    <h1 itemprop="headline">Zimbabwe wants dentist's extradition over lion</h1>
                    <!--sub headline-->
                        <h2 class="subHeadline" itemprop="alternativeHeadline">Nation has begun extradition for Walter Palmer</h2>
                    <div itemprop="author" itemscope itemtype="http://schema.org/Person">

    <div class="author">
             <span itemprop="name">By Faith Karimi, Michael Martinez and Laura Smith-Spark CNN</span>
        </div>
    </div>
<div class="detailTimestamp">
                <span class="date postedAt">POSTED:&nbsp;04:53 AM CDT Jul 31, 2015 </span>
                        <span class="date separator">&nbsp;</span>
                    <span class="date updatedAt">UPDATED:&nbsp;10:14 AM CDT Aug 01, 2015 </span>
                        </div>
        </div>
            </hgroup>

            <div class="utils clear">
        <div class="resizer ">
        <ul class="clear">
            <li class="small"><a href="#"><span class="alt">Small Text</span></a></li>
            <li class="medium"><a href="#"><span class="alt">Medium Text</span></a></li>
            <li class="large"><a href="#"><span class="alt">Large Text</span></a></li>
        </ul>
    </div>

    <div class="addThis">
        <div class="addthis_toolbox addthis_default_style ">
                    <a class="addthis_button_facebook_like" fb:like:layout="button_count" fb:like:action="recommend"></a>
                            <a class="addthis_button_tweet"></a>
                            <a class="addthis_button_google_plusone" g:plusone:size="medium"></a>
                        <a class="addthis_button_pinterest_share"></a>
                    <a class="addthis_button_email"></a>
                    <a class="addthis_button_print"></a>
                    <a class="addthis_button_compact"></a>
                </div>
            </div>

    <script src="//s7.addthis.com/js/250/addthis_widget.js#async=1"></script>

    <script>
        var addthis_share = {
