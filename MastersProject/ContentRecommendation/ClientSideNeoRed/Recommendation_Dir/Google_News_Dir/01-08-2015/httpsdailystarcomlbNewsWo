<div id="headerControl1_divPaymentScripts">
    <iframe class="overlay" id="overlayRegisterFrame" style="position: fixed; display: none; overflow: hidden; top: 100px; z-index: 1000; border: 0; margin-right: 10px;
        background-color: #ffffff;"></iframe>
    <script type="text/javascript">
        function enterRegisterOverlay(content, sizes) {

            $("#overlayRegisterFrame").attr("src", content);
            $("body").css("overflow", "hidden");

            if ($(window).height() <= 800) {
                var heightValue = ($(window).height() - 150);
            }
            else { var heightValue = 700 };

            var sideMargin = ($(window).width() - 966) / 2;
            if (sizes == 1) {
                $("#overlayRegisterFrame").css({ "height": heightValue, "left": sideMargin + 300 });
                $(".overlay").fadeIn(1000);

                $("#overlayRegisterFrame").css({ "width": "320" });
                $("#overlayRegisterFrame").css({ "height": "365" });
            }
            else {
                $("#overlayRegisterFrame").css({ "height": heightValue, "left": sideMargin + 150 });
                $(".overlay").fadeIn(1000);

                $("#overlayRegisterFrame").css({ "width": "600" });
            }

            $("#overlayRegisterFrame").css({ "display": "" });
            $("#overlayframeNav").css({ "display": "none" });
            $("#overlayframeAd").css({ "display": "none" });
            $("#overlayframeContent").css({ "display": "none" });
        };

        var data = getCookie("tdslogin");
        if (data != "") {
            var divSearchText = document.getElementById("divSearchText");
            divSearchText.style.margin = "0";

            var divUsername = document.getElementById("divUsername");
            divUsername.innerHTML = "Welcome <a style='color:#616163;font-size:12px;' onclick='enterRegisterOverlay(\"/UserProfile.aspx\", 2);' href='#'>" + data + "</a>&nbsp;&nbsp;|&nbsp;&nbsp;<a style='font-size:12px;' href='/Logout.aspx'>Logout</a>";
        }
        else {
            var divSearchText = document.getElementById("divSearchText");
            divSearchText.style.margin = "0";

            var divUsername = document.getElementById("divUsername");
            divUsername.innerHTML = "<a style='color:#616163;font-size:12px;' href='/Register.aspx' target='_blank'>Register</a>&nbsp;&nbsp;|&nbsp;&nbsp;<a style='font-size:12px;' onclick='enterRegisterOverlay(\"/LoginPage.aspx\", 1);' href='#'>Login</a>";
        }
    </script>
</div>
<script type="text/javascript">
    $('#txtSearch').keypress(function (e) {
        if (e.which == 13) {
            var txtSearch = document.getElementById("txtSearch");
            if (txtSearch.value != "") {

                //    alert(location.href);
                location.href = "/SearchArticles.aspx?search=" + txtSearch.value;
                //   alert(location.href);
                return false;
            }
        }
    });

    function searchArticles() {
        var txtSearch = document.getElementById("txtSearch");
        if (txtSearch.value != "") {

            //    alert(location.href);
            location.href = "/SearchArticles.aspx?search=" + txtSearch.value;
            //   alert(location.href);
            return false;
        }
        else
            alert("missing word")
    }
</script>

