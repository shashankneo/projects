                        <div id="bodyHolder_divMemberScript">
                            <script type="text/javascript">

                                var data = getCookie("tdslogin");
                                if (data != "") {
                                    //    alert(data);
                                }

                                var divTeaser = document.getElementById("bodyHolder_divTeaser");
                                var divLogin = document.getElementById("bodyHolder_divLogin");
                                var divDetails = document.getElementById("bodyHolder_divDetails");

                                var divSummary = document.getElementById("bodyHolder_divSummarySection");

                                divLogin.style.display = "none";
                                divTeaser.style.display = "";
                                divTeaser.innerHTML = "<img src='/images/loading.gif' alt='loading ...' />";
                                if (divSummary) {
                                    divSummary.style.display = "none";
                                }
                                var articleWebUrl = "";
                                var urlReferrer = document.referrer;

                                if (urlReferrer.indexOf("google.com") >= 0 || urlReferrer.indexOf("bing.com") >= 0) {
                                    articleWebUrl = "/GetArticleBody.aspx?id=309261&fromgoogle=1";
                                }
                                else {
                                    if (data != "")
                                        articleWebUrl = "/GetArticleBody.aspx?id=309261&username=" + data;
                                    else
                                        articleWebUrl = "/GetArticleBody.aspx?id=309261";
                                }

                                var randomNum = Math.random();
                                articleWebUrl = articleWebUrl + "&random=" + randomNum * 1000;

                                var objXmlHttpNewArticle = GetXmlHttpObject();

                                objXmlHttpNewArticle.onreadystatechange = function () { ValidateArticleResponse(objXmlHttpNewArticle) };
                                XmlHttpGet(objXmlHttpNewArticle, articleWebUrl);

                                var enableGoogle = "";
                                function ValidateArticleResponse(objXmlHttp) {
                                    if (objXmlHttp.readyState == 4 || objXmlHttp.readyState == 'complete') {
                                        if (objXmlHttp.status == 200) {
                                            var sResponse = objXmlHttp.responseText;
                                            var values = sResponse.split('||');

                                            var divSummary1 = document.getElementById("bodyHolder_divSummarySection");
                                            //CanOpen, Teaser, Body, Status, Message
                                            if (values.length > 0) {
                                                var canOpen = values[0];

                                                if (canOpen == "True") {

                                                    enableGoogle = canOpen;
                                                    divLogin.style.display = "none";
                                                    divTeaser.style.display = "none";
                                                    divDetails.style.display = "";
                                                    if (divSummary1) {
                                                        divSummary1.style.display = "";
                                                    }
                                                    divTeaser.innerHTML = "";
                                                    divDetails.innerHTML = values[2];
                                                }
                                                else {
                                                    enableGoogle = "";
                                                    var status = values[3];
                                                    if (status == "active") {
                                                        divLogin.style.display = "none";
                                                        divTeaser.style.display = "none";
                                                        divDetails.style.display = "";
                                                        if (divSummary) {
                                                            divSummary.style.display = "";
                                                        }
                                                        divTeaser.innerHTML = "";
                                                        divDetails.innerHTML = values[2];
                                                    }
                                                    else if (status == "null" || status == "") {

                                                        enableGoogle = "";
                                                        divLogin.style.display = "";
                                                        divTeaser.style.display = "";
                                                        divDetails.style.display = "none";
                                                        if (divSummary) {
                                                            divSummary.style.display = "none";
                                                        }
                                                        divTeaser.innerHTML = values[1];
                                                        divDetails.innerHTML = "";
                                                    }
                                                    else if (status == "expired") {

                                                        enableGoogle = "";
                                                        divLogin.style.display = "";
                                                        divTeaser.style.display = "";
                                                        divDetails.style.display = "none";
                                                        if (divSummary) {
                                                            divSummary.style.display = "none";
                                                        }
                                                        divTeaser.innerHTML = values[1];
                                                        divDetails.innerHTML = "";

                                                        var divMessage = document.getElementById("bodyHolder_divMessage");
                                                        divMessage.innerHTML = "Your account is not active, please <a href='" + values[4] + "'>click here</a> to proceed the payment page in order to activate your account";
                                                    }
                                        }
                                    }
                                }
                            }
                        }
                            </script>
