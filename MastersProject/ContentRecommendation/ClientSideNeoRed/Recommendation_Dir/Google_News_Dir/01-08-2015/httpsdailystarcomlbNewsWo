                            addBanner_asideControl_ad4_adRotator_divAd("<iframe id='frm_ad_asideControl_ad4_adRotator_divAd' name='frm_ad_asideControl_ad4_adRotator_divAd' frameborder='0' scrolling='no' vspace='0' hspace='0' align='middle' width='300' height='250' src='http://www.dailystar.com.lb//dailystar/Ads/js_65.htm' />", 180);addBanner_asideControl_ad4_adRotator_divAd("<iframe id='frm_ad_asideControl_ad4_adRotator_divAd' name='frm_ad_asideControl_ad4_adRotator_divAd' frameborder='0' scrolling='no' vspace='0' hspace='0' align='middle' width='300' height='250' src='http://www.dailystar.com.lb//dailystar/Ads/js_30.htm' />", 180);
						var count_asideControl_ad4_adRotator_divAd = 0;
						function showAd_asideControl_ad4_adRotator_divAd() 
						{
							if (count_asideControl_ad4_adRotator_divAd == banners_asideControl_ad4_adRotator_divAd.length)
							{
								count_asideControl_ad4_adRotator_divAd = 0;
							}
				
							var adPlaceHolder = document.getElementById("asideControl_ad4_adRotator_divAd");
							adPlaceHolder.innerHTML = banners_asideControl_ad4_adRotator_divAd[count_asideControl_ad4_adRotator_divAd];
							count_asideControl_ad4_adRotator_divAd++;clearTimeout(timer_asideControl_ad4_adRotator_divAd);timer_asideControl_ad4_adRotator_divAd = setTimeout('showAd_asideControl_ad4_adRotator_divAd()', bannersTimers_asideControl_ad4_adRotator_divAd[count_asideControl_ad4_adRotator_divAd]);}timer = setTimeout('showAd_asideControl_ad4_adRotator_divAd()', bannersTimers_asideControl_ad4_adRotator_divAd[count_asideControl_ad4_adRotator_divAd]);showAd_asideControl_ad4_adRotator_divAd();</script></div></div>

            </div>
