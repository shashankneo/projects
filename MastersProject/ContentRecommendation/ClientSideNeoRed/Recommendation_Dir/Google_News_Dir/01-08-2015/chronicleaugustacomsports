			<div class="wl-content">
				<div class="wl-ad">
	<script type="text/javascript">
$(document).ready(function(){


$('#edit-subject,#edit-comment').focus(function(){
	  
	  $(document).unbind('keydown');
	  
	  });
	  
	  $('#edit-subject,#edit-comment').blur(function(){
	  
	      $(document).keydown(function(e) {
				var url = false;
				if (e.which == 37) {  // Left arrow key code


					url = $('#page-navigation-pre a').attr('href');
			  
				}
				else if (e.which == 39) {  // Right arrow key code


					url = $('#page-navigation-next a').attr('href');
				  
				}
				if (url) {
				   window.location = url;
				}      
               
			});
	  
	  }); 
	$(window).resize(function(){  
			  var screen_size = $(window).width();
				if(screen_size <=1150)
				{
					$(document).unbind('scroll');
					$(' .key-nav, #arrow-left-shadow, #arrow-right-shadow ').hide();
				} 
				else
				{
					var ht = $(document).height();	
	$(document).scroll(function () {
		
		var y = $(this).scrollTop();
		if (y > 200 && y < (ht-1000))
		{
			$(' .key-nav, #arrow-left-shadow, #arrow-right-shadow ').fadeIn();
		} 
		else
		{
			
			$(' .key-nav, #arrow-left-shadow, #arrow-right-shadow ').fadeOut();
		}
	}); 
					$(' .key-nav, #arrow-left-shadow, #arrow-right-shadow ').fadeIn();
				}
		});
   });
</script>	<span class="wl-clear"></span>
</div>

