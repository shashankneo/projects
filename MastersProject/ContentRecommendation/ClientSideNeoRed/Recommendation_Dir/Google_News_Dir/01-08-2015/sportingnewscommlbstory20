            <div id="sn-marketing-article-right"></div>
<script type="text/javascript">
(function($) {
    function weightPromoItems(items, weights) {
        var totalWeight = 0;
        $.each(weights, function() {
            totalWeight += this;
        });
        var weightedItems = [];
        var currentItem = 0;
        while (currentItem < items.length){
            for (i = 0; i < weights[currentItem]; i++) {
                weightedItems.push(items[currentItem]);
            }
            currentItem++;
        }
        return {
            items: weightedItems,
            total: totalWeight
        };
    }

    var sport = '';

    var products = [];
    var weights = [];

    switch (sport) {
        case 'mlb':
            products = ['fanatics', 'draftkings'];
            weights = [1, 1];
            break;
        case 'nfl':
            products = ['fanatics'];
            weights = [1];
            break;
        case 'nhl':
        case 'nba':
            products = ['fanatics', 'draftkings'];
            weights = [4, 5];
            break;
        case 'ncaa-basketball':
            products = ['draftkings'];
            weights = [1];
            break;
        case 'ncaa-football':
            products = ['fanatics', 'draftkings'];
            weights = [13, 5];
            break;
        case 'nascar':
        default:
            products = ['fanatics'];
            weights = [1];
    }

    weightedItems = weightPromoItems(products, weights);

    var pick = Math.floor(Math.random() * weightedItems.total);

    if (weightedItems.items[pick] == "fanatics") {
        var url = "http://www.sportingnews.com/ad-modules/sportingnews/fanatics/mlb/300x250?sport=&ad_page=article";
    } else {
        var url = "http://www.sportingnews.com/ad-modules/sportingnews/"
            + weightedItems.items[pick]
            + ".php?sport=";
    }

    $.ajax({
        url: url,
        success: function(data) {
            $('#sn-marketing-article-right').html('<div class="marketing-promo">'+data+'</div>').show();
        }
    });
})(jQuery);
</script>
        </aside>
