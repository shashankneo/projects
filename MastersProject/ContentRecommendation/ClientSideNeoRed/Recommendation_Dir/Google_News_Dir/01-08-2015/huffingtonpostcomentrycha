<meta property='og:title' content='Channing Tatum Is Playing Gambit After All'>
<script type="text/javascript">define("runtime_data/language",function(){ return "en"; });</script>
<script type="text/javascript">define("runtime_data/section",function(){ return {"name":"Entertainment","slug":"entertainment","socialAccounts":{"facebook":"HuffPostEntertainment","twitter":"HuffPostEnt"},"sailthruList":"Entertainment Brief","contactUs":"entertainment-corrections@huffingtonpost.com","comscoreFooterOverride":"Part of HuffPost Entertainment on Moviefone","customHeader":"http://img.huffingtonpost.com//asset/55956adc1d00009b0b3d0e85.gif"}; });</script>
<meta property='og:image' content='http://img.huffingtonpost.com//asset/600_450/55bd3b44170000250056648e.jpeg?cache=Mb2pwIt4GD'>
