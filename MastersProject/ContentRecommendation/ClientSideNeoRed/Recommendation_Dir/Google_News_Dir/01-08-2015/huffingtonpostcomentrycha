<script>
 try {
window.adSonarCallback = function(listings, isStacked, div) {
var width = isStacked ? '100%' : (Math.floor(100/listings.length-1) + '%');
var adSonarBlock = function(l) {
var track = l.viewThruURL ?  ('<img src="'+l.viewThruURL+'" style="display:none">') : '';
track = track.replace('[timestamp]', new Date().getTime());
return [
'<a href="',l.targetUrl,'">',
'<div class="asl-block" style="width:',width,';">',
  '<div class="asl-image">',
    (l.image ? '<img src="http://'+l.image.src+'">' : ''),
  '</div>',
  track,
  '<div>',
    '<div class="asl-title">',l.title,'</div>',
    '<div class="asl-description">',l.description,'</div>',
    '<div class="asl-domain">',l.domain,'</div>',
  '</div>',
'</div>',
'</a>'
].join('');
};

var inner = listings.map(adSonarBlock).join('');

var style = "<style>\
.asl-wrapper{background:#fff;font:normal 12px arial,sans-serif;padding:3px;position:relative;height:100%;}\
.asl-wrapper a{color:#000;text-decoration:none;}\
.asl-description{color:#000;}\
.asl-splinks{font-weight:bold;margin-bottom:3px;}\
.asl-image{float:left;}\
.asl-image img{width:60px;height:60px;margin-right:5px;}\
.asl-block{display:inline-block;overflow:hidden;height:65px;padding:2px;margin-bottom:10px;}\
.asl-title{font-weight:bold;color:#0C4790;}\
.asl-domain{color:#828282;}\
.asl-buylink{font-weight:bold;font-size:10px;position:absolute;bottom:5px;right:5px;}\
</style>"

var html = [
style,
'<div class="asl-wrapper">',
'<div class="asl-splinks">Sponsored Links</div>',
inner,
'<div class="asl-buylink">',
  '<a href="http://aolhuffingtonpost.sl.advertising.com/admin/advertisers/indexPl.jsp">',
    'Buy a link here',
  '</a>',
'</div></div></div>'
].join('');

var ifr = document.createElement('iframe');
ifr.src = 'about:blank';
ifr.style.cssText = 'width:100%;height:100%;';
var props = {
  marginWidth: 0, marginHeight: 0,
  frameBorder: 0, scrolling: 'no',
  src: 'about:blank'
};
for (var key in props) { if (props.hasOwnProperty(key)) { ifr.setAttribute(key, props[key]); } }
div.appendChild(ifr);
var idoc = ifr.contentDocument;
idoc.open();
idoc.write(html);
idoc.close();
};

window.loadAdSonar = function(pid, plid, nb, div, callback) {
  var cb_name = 'adsonar_cb' + Math.floor(Math.random()*1e16);
  var url = [
  'http://ads.adsonar.com/adserving/getAdsAPI.jsp?c=', cb_name,
  '&version=3&type=0&nb=', nb,
  '&key=', encodeURIComponent(window.location.href),
  '&placementIds=', plid, '&pubId=', pid
  ].join('');

  window[cb_name] = function(result) { try {
    var isStacked = div.offsetHeight > 100;
    window.adSonarCallback(result.response.data.placement[0].listing, isStacked, div);
  } catch(err) { console.error(err); } };

  var s = document.createElement('script');
  s.async = true; s.src = url;
  document.querySelector('body').appendChild(s);
};
} catch(e) { window.console && window.console.log(e) } 
</script>


