<script language="javascript">
function hideMsg(){
	document.getElementById("system-message").style.display="none";
}setTimeout("hideMsg()",5000);
var bannHidden=false;
function hideAllBan(){
	if(bannHidden==false){
		jQuery(".gb").each(function(){
			jQuery(this).addClass( "hidBan" );
		});
		bannHidden=true;
	}else{
		bannHidden=false;
		jQuery(".gb").each(function(){
			jQuery(this).removeClass( "hidBan" );
		});
	}
}
function hiderow(d){
	document.getElementById(d).style.display="none";
}
