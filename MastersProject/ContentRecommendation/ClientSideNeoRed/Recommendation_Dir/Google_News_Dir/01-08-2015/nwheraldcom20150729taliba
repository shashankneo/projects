	<script>
 		function createCookie(name,value,days) {
			if (days) {
				var date = new Date();
				date.setTime(date.getTime()+(days*24*60*60*1000));
				var expires = "; expires="+date.toGMTString();
			}
			else var expires = "";
			document.cookie = name+"="+value+expires+"; path=/";
		}
		
		function readCookie(name) {
			var nameEQ = name + "=";
			var ca = document.cookie.split(';');
			for(var i=0;i < ca.length;i++) {
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1,c.length);
				if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
			}
			return null;
		}
		
		function eraseCookie(name) {
			createCookie(name,"",-1);
		}

		function trackBlockers() {
			if (jQuery('.mrec').height() == 0) {
				var uniqueBl = readCookie('unbl');
				var uniqueSe = readCookie('sebl');
				if (uniqueBl) {
					if (uniqueSe) {
						jQuery.ajax({type: "POST",url: "/pagesblocked.xml",cache: false});
						// console.log("Another page");
					} else {
						jQuery.ajax({type: "POST",url: "/totalblockers.xml",cache: false});
						// console.log("New Visit");
					}
				} else {
					jQuery.ajax({type: "POST",url: "/uniqueblockers.xml",cache: false});
					// console.log("New Unique Blocker");
				}	
				createCookie('unbl','yes',30);
				createCookie('sebl','yes',0);
			}
		}
	</script>

<script>
$(trackBlockers);
</script>
    
      <script type="text/javascript">
      function toggleSubNav(dir,speed){
        if(dir=="up"){
          jQuery("#nav ul.sub").slideUp(speed);
        }
	else if(dir=="down"){
	  jQuery("#nav ul.sub").slideDown(speed);
        }
      }
      function toggleNav(){
        windowW = jQuery(window).width();
        if( jQuery("#menuicon").is(":visible") ){
          jQuery("#nav ul.primary").css("display","none");
        } else {
          jQuery("#nav ul.primary").css("display","inline-block");
        }			
      }
      // BUTTON CLICK EVENTS
      jQuery("#closesubnav").click(function(){
        toggleSubNav("up",150);
      });     
      jQuery("#menuicon").click(function(){
        jQuery("#nav ul.primary").slideToggle(150);
      });
      jQuery("#nav .nav").each(function(){
        jQuery(this).click(function(evt){
          liID = jQuery(this).attr("id");
          submenu = jQuery("#nav ul.sub").find("ul#"+liID).length;

          if(submenu>0){
            evt.preventDefault();
            if( jQuery("#nav ul.sub ul#"+liID).is(":visible") ){
              jQuery("#nav ul.sub").slideUp(150);
            } else { // SUBMENU IS *NOT* VISIBLE
              jQuery("#nav ul.sub ul").css("display","none"); // HIDE ALL SUBMENUS
              jQuery("#nav ul.sub ul#"+liID).css("display","block"); // DISPLAY CHOSEN SUBMENU
              toggleSubNav("down",150); // OPEN SUBNAVIGATION
            }
          } else return true;

          if(liID=="search"){
            jQuery("#shaw-query").focus();
          }
        });
      });
      jQuery(window).resize(function(){
        toggleNav();
      });
      </script>
    
    <script type="text/javascript">
    function moveMap(){
      if (jQuery("#pt_preview_map").length > 0) { 
        jQuery("#pt_preview_map").css({
          "float":"none",
          "clear":"both",
          "max-width":"125px",
          "margin-right":"auto",
          "margin-left":"auto"
        });
      }
    }
    jQuery(document).ready(function(){
      setTimeout(moveMap,1000);
    });
    </script>

    <script type="text/javascript">
    function changeLoginURL() {
        if (nonMeter != null && nonMeter == 1)
        {
            $("#mast-login-link").prop("onclick", null);
            $("#mast-login-link").attr("href", "/login/index.xml")
        }
    }
    function photoClass(){
      var photoHasClass = false;
      photoHasClass = $("#lead-article-photo div.photo").hasClass("full");

      var windowW = $(window).width();
      if (windowW < 1000){
        if(photoHasClass==true){
          // IF LEAD ARTICLE PHOTO NOT ALREADY IN STORYHEADER, MOVE IT
          if($("#storyheader").has("#lead-article-photo").length==0){
            $("#lead-article-photo").appendTo("#storyheader");
          }
          $("#lead-article-photo").appendTo("#storyheader");
        }
      } else {
        if(photoHasClass==true){
          // IF LEAD ARTICLE PHOTO NOT ALREADY IN DIV.ARTICLE, MOVE IT
          if($(".article").has("#lead-article-photo").length==0){
            $("#lead-article-photo").prependTo(".article");
          }
        }
      }

      if (photoHasClass==true){
        $("#lead-article-photo").addClass("full");
      } else {
        $("#lead-article-photo").addClass("half");
      }
    }

    function resetShare(){
      var top = $("div#share").offset().top - parseFloat($("div#share").css("marginTop").replace(/auto/, 0));
      var offset = $("#sharewrapper").offset();
      var mastheadHt = $("#masthead div#search a").outerHeight();
      var shareTop = offset.top - $("#masthead div#search a").outerHeight();
      var articleHt = $("#story .article").outerHeight();
      var articleTop = $("#story .article").offset().top;
      var shareHt = $("#share").outerHeight();
      $(window).scroll(function(event){
        // what the y position of the scroll is
        var y = $(this).scrollTop() + mastheadHt;

        // SCROLL IS GREATER THAN ARTICLE HEIGHT, REMOVE FIXED POSITION
        if (y > (articleHt + articleTop - shareHt - 60)){
          $("div#share").removeClass("fixed");
        }
        // SCROLL IS GREATER THAN ADJUSTED TOP, ADD FIXED POSITION FOR SCROLL
        else if (y >= shareTop) {
          $("div#share").addClass("fixed");
        } 
        // SCROLL IS LESS THAN ARTICLE HEIGHT, REMOVE FIXED POSITION
        else {
          $("div#share").removeClass("fixed");
        }
      });
    }

    // WHEN COMMENTS ICON OR LINK CLICKED, SCROLL TO THAT SECTION
    $("#comments-btn").click(function(){
      var commentsTop = $("h2.comments").offset().top - $("#masthead li#search a").outerHeight() - 20;
      $("html, body").animate({scrollTop:commentsTop},"medium");
    });
    $("#comments-link").click(function(){
      var commentsTop = $("h2.comments").offset().top - $("#masthead li#search a").outerHeight() - 20;
      $("html, body").animate({scrollTop:commentsTop},"medium");
    });

    $(".caption-btn").click(function(){
      var captionID = $(this).attr("id").substring(11);
      $(this).toggleClass("visible");
      $("#caption-string"+captionID).slideToggle(500);
    });
    
    // ON WINDOW RESIZE, REPOSITION AD
    $(window).resize(function(){
      photoClass();
      resetShare();
    });
    // ON DOCUMENT READY, POSITION AD AND DISPLAY IT
    $(document).ready(function(){
      //changeLoginURL(); //commented out because it was preventing the rest of the function from working -Adam
      photoClass();
      resetShare();
      // IF THIS IS A SPONSORED ARTICLE, ADD TRACKING EVENT
      
    });

// Google Consumer Surveys IE iFrame Fix to display correctly
var currentSurveyHeight = 1;
$( window ).load(function() {
	var newSurveyHeight = $('#t402-prompt-iframe').contents().height();
	if (newSurveyHeight > currentSurveyHeight)
	{
		$( "#t402-prompt-iframe" ).height(newSurveyHeight);
		currentSurveyHeight = newSurveyHeight;
	}
	var surveyHTML = $('#t402-prompt-iframe').contents().html('body');
	var loginHTML = surveyHTML.find("#t402-login-payment").html();
	var altAction = $('#google-alt-action').html()
	if (loginHTML == "Missing RHS unit")
	{
		surveyHTML.find("#t402-login-payment").html(altAction);
	}
});

    </script>

