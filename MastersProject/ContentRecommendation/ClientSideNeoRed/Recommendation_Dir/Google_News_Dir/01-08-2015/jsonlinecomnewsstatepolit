var getWebBrowserWidth = function() {
	var winW = 0;
	if (document.body && document.body.offsetWidth) {
	 winW = document.body.offsetWidth;
	}
	if (document.compatMode=='CSS1Compat' &&
	    document.documentElement &&
	    document.documentElement.offsetWidth ) {
	 winW = document.documentElement.offsetWidth;
	}
	if (window.innerWidth && window.innerHeight) {
	 winW = window.innerWidth;
	}
	return winW
}
var _screenWidth = getWebBrowserWidth();
if(_screenWidth  >= 1320) {
ga('send', 'event','Page Load', 'Wide Screen View', 'Wide' ,_screenWidth);
} else if(_screenWidth  == 0) {
ga('send', 'event', 'Page Load', 'Zero Width View','Zero' ,_screenWidth);
} else {
ga('send', 'event', 'Page Load', 'Narrow Screen View','Narrow' ,_screenWidth);
}
</script>
