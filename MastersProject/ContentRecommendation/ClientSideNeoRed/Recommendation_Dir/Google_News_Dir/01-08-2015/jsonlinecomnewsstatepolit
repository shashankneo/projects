<script type="text/javascript">
 $("#reg1,#reg2,#reg3,#regD").click(function (e) {
                e.preventDefault();
                $("#reg-modal").attr("src", "https://subscriber.jsonline.com/ActivateAccount.html?mode=paywall");
                $("#reg-modal").width("650");
                $("#reg-modal").height("700");
                $.unblockUI();
                $("#reg-dialog").dialog({
                    width: 650,
                    height: 710,
                    dialogClass: 'alert',
                    modal: true,
                    close: function (event, ui) { window.location.reload(true); }
                });
                $(".ui-button").blur();
            });
</script>
