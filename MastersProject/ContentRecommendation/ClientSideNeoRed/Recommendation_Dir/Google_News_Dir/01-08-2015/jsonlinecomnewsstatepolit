<script>
	var isSubscriber = false;
	var isMobile =  false ;
	var isResponsive =  false ;
        var adIdPrepend = "div-gpt-ad-";
        
        //define the location we are for targeting
        var locationSpecific = "np.mjs";
        var locationSection = "news/statepolitics";
        
        var adRefreshInterval =  105 ;
        if(typeof adRefreshInterval !== "number" || adRefreshInterval < 0){
        	adRefreshInterval = 0;
        }
        
        var gptAdCodeString = "";
        var contentId = "320399272";
        var pageType = "11864";
        
        var tid = "";


        if(window.location.href.indexOf("jsonline") > -1){
        	        	//I used websiteSection.topicId
        		        	        	
        	tid = "2006,2";
        	
        	        }else{
        	tid = "Scott Walker courts donors at Koch brothers' event in California";
        	//tid = "Scott Walker courts donors at Koch brothers' event in California";
        }
        
        var delayedNewsWatchAds = [];
        var delayedAboveAds = [];
        




        function createAd(data, containerId){
        	/*Newswatch and buswatch ad slots NEED to be called together, so when we
        	  see one, we delay for short while to see if we can find its match.     */
        	if(data.slot == "newswatch" || data.slot=="buswatch"){
        		delayedNewsWatchAds.push([data, containerId]);
        		
        		if(delayedNewsWatchAds.length >= 2){
	        		for(var i = 0; i < delayedNewsWatchAds.length; i++){
	        			_doCreateAd(delayedNewsWatchAds[i][0], delayedNewsWatchAds[i][1]);
	        		}
	        		
	        		delayedNewsWatchAds = [];
        		}
        		
        		//saftey catch, incase we werent able to find a match.
        		setTimeout(function(){
        			if(delayedNewsWatchAds.length > 0){
        				_doCreateAd(delayedNewsWatchAds[0][0], delayedNewsWatchAds[0][1]);
        			}
        		}, 500);
        	/* Above slots have a similar requirement, except two above ads of different sizes
        	   might need to load together.  */
        	}else if(data.slot == "above" && (data.size == "728x90" || data.size == "300x250" || data.size == "300x250,300x600")){
        		delayedAboveAds.push([data, containerId]);
        		
        		//we need to see two of these paired ads before we can render them on the page.
        		if(delayedAboveAds.length >= 2){
	        		for(var i = 0; i < delayedAboveAds.length; i++){
	        			_doCreateAd(delayedAboveAds[i][0], delayedAboveAds[i][1]);
	        		}
	        		
	        		delayedAboveAds = [];
        		}
        		
        		//saftey catch, incase we werent able to find a match.
        		setTimeout(function(){
        			if(delayedAboveAds.length > 0){
        				_doCreateAd(delayedAboveAds[0][0], delayedAboveAds[0][1]);
        			}
        		}, 1000);
        	/* if not one of these cases, just go straight to the creation. */
        	}else{
        		_doCreateAd(data, containerId);
        	}
        }
        


	function _doCreateAd(data, containerId){
		var myData = data;
		var myContainer = containerId;
		var sizeString = myData.size;
		
		var sizeX = 0;
		var sizeY = 0;
		var sizeArray = [];
		
		//see if we have more than one possible size.  If we dont, take the easy case.
		if(myData.size.indexOf(',') == -1){
			sizeX = parseInt(myData.size.split('x')[0]);
			sizeY = parseInt(myData.size.split('x')[1]);
			sizeArray = [sizeX, sizeY];
		//if we do, take the hard case and make a size array.
		}else{
			var possibleSizes = myData.size.split(',');
			for(var i = 0; i < possibleSizes.length; i++){
				sizeX = parseInt(possibleSizes[i].split('x')[0]);
				sizeY = parseInt(possibleSizes[i].split('x')[1]);
				sizeArray.push([sizeX, sizeY]);
			}
		}
		var slotName = myData.slot;
		//to control if gutter ads get loaded on the page or not.
		var isWideEnoughForGutter = window.innerWidth > 1350 ? true : false;
		
		var mobileSizeX = 0;
		var mobileSizeY = 0;
		var hasMobileSize = false;
		
		//if we have a mobile size override, parse it here.
		if(data.mobileSize){
			mobileSizeX = parseInt(myData.mobileSize.split('x')[0]);
			mobileSizeY = parseInt(myData.mobileSize.split('x')[1]);
			hasMobileSize = true;
		}
		
		var mobileSizeArray = [mobileSizeX, mobileSizeY];
		var respectMobile = ((isMobile || (isResponsive && window.innerWidth < 685)) && hasMobileSize);
		
		if(locationSpecific.length > 0){
			gptAdCodeString = '/29731549/'+/*""+*/locationSpecific;
			
			if(locationSection.length > 0){
				gptAdCodeString += "/"+ locationSection;
			}
		}else{
			//in the event we didnt get any location param, lets try to get it
			//from the parent (for iframe widget ads)
			if(parent.gptAdCodeString && parent.gptAdCodeString > 0){
				gptAdCodeString = parent.gptAdCodeString
			}
		}
		
		//Handle slot-specific ad codes
		if(slotName === "hover"){
			gptAdCodeString += "/hover";
		}else if(slotName =="wallpaper"){
			gptAdCodeString += "/wallpaper";
		}else if(slotName ==="carousel" && window.location.href.indexOf("metroparent") > -1){
			gptAdCodeString += "/metro_carousel";
		}else if(slotName ==="carousel" && (window.location.href.indexOf("lakecountrynow") > -1 || window.location.href.indexOf("wisconsintrails") > -1)){
			gptAdCodeString += "/carousel";
		}
		
		//the actual googletag createAd Call.
		googletag.cmd.push(function(){
			var gt;
			
			if(slotName === "wallpaper" || slotName === "insidewallpaper"){
				gt = googletag.defineOutOfPageSlot(gptAdCodeString, adIdPrepend+myContainer); 
			}else{
				gt = googletag.defineSlot(gptAdCodeString, (respectMobile) ? mobileSizeArray : sizeArray, adIdPrepend+myContainer);
			}
				
			gt.addService(googletag.pubads());
			gt.setTargeting('sub', isSubscriber);
			gt.setTargeting('tid', tid);
			gt.setTargeting('targetSize', sizeString);
			gt.setTargeting('pos', slotName);
			gt.setTargeting('contentID', contentId);
			gt.setTargeting('type', pageType);
			gt.setTargeting('sr', isWideEnoughForGutter);
			
			gt.setCollapseEmptyDiv(true);
				
			googletag.pubads().enableSingleRequest();
			googletag.enableServices();
			
			//we refresh only certain ads.
			if(adRefreshInterval > 0 && (slotName === "below" || slotName === "way_below" || slotName === "spans" || window.location.href.indexOf("m.jsonline.com") > -1)){
				setInterval(function(){
					googletag.pubads().refresh([gt]);
				}, adRefreshInterval*1000);
			}
			
			/* special carousel support.  When the ad loads, we remove the existing article image 
			   and title.  Then, we replace the small image on the overlay with the ad.           */
			if(slotName === "carousel"){
				gt.oldFetchStarted = gt.fetchStarted;
				gt.fetchStarted = function(){
					console.log('Fetch started for: '+slotName+'!');
					
					//this is dirty but theres no way to hook into the ad to detect if its loaded
					setTimeout(function(){
						var iframes = document.getElementById(gt.getSlotId().f).getElementsByTagName('iframe');
						var iframeImg = iframes[0].contentWindow.document.getElementsByTagName('img');
						
						if(iframeImg.length > 0){
														document.getElementById('overlay4').style.display = "none";
							
							//sub the ad image into the final pointer
							document.getElementById('smallimg4').getElementsByTagName('img')[0].src = iframeImg[0].src;
						}else{
														document.getElementById(adIdPrepend+myContainer).remove();
							
						}
					}, 750);
				
					gt.oldFetchStarted();
				}
			} 
			
			googletag.cmd.push(function(){ 
				googletag.display(adIdPrepend+myContainer);
			});
		});
	}
</script>























