    <!-- Begin HTML Module -->
    <div class="side_section_container html" data-vr-zone="Scott Walker - index refer walkerpage">
        <div class="side_container_01" data-vr-contentbox="Scott Walker - index refer walkerpage 1">
            <div class="title">The Scott Walker File
            </div><!-- title -->
            <div class="side_container_01_content">
<div><a href="http://www.jsonline.com/news/statepolitics/scott-walker-290106981.html"><img width="100%" alt="" src="http://media.jrn.com/images/Scott_Walker_through_the_years.jpg" /></a></div>
<div><a href="http://www.jsonline.com/news/statepolitics/scott-walker-290106981.html"><strong>Special Section</strong>: Everything you need to know about Scott Walker and his place in the 2016 presidential race</a></div>            </div><!-- side_container_01_content -->
        </div><!-- side_container_01 -->
    </div><!-- side_section_container html -->
    <!-- End HTML Module -->
