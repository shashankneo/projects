<script type="text/javascript">
    $(document).ready(function(){
        var minIframeWidth = ($(window).width() > (1180+30)) ?  ($(window).width()-30) : 1180;
        var minIframeHeight = ($(window).height() > (600+30)) ?  ($(window).height()-30) : 600;
        $(".supergallery").bind("click",function(e){
            $("#ad-wrapper-left").remove()
            e.preventDefault();
           var continueRunning = true;
            var id = $(this).attr("href").indexOf("-") == -1 ? $(this).attr("href").split("/")[$(this).attr("href").split("/").length-1].split(".")[0] : $(this).attr("href").split("-")[$(this).attr("href").split("-").length-1].split(".")[0];
            var item = "";
            if (typeof $(this).attr("selectItem") !== typeof undefined && $(this).attr("selectItem") !== false && $(this).attr("selectItem") != "") {
                item = "&item="+$(this).attr("selectItem");
            }            
            if( typeof _dynamicTrack == 'function'){
                continueRunning = _dynamicTrack(id);
           }
            if(! continueRunning)
                return;
            //alert("FancyBox check");
            $.fancybox({
                href: "http://www.jsonline.com/supergallery?id="+id+item,
                type: 'iframe',
                autoSize:false,
                autoResize:true,
                //fitToView:false,
                margin:15,
                padding:0,
                //minWidth: minIframeWidth,
                //minHeight: minIframeHeight,
                width:minIframeWidth,
                height:minIframeHeight,
                helpers : { 
                   overlay: {
                    opacity: 0.8,
                    css: {'background-color': '#444444'}
                   }
                }
            });
        });
    });
</script>
