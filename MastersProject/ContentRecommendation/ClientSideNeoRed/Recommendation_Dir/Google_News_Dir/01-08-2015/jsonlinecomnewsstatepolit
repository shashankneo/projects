    <script type="text/javascript">
                       var Triangle = function (a, b, c) {
                            this.A = a;
                            this.B = b;
                            this.C = c;

                            this.detT = 1.0*(this.B.Y-this.C.Y)*(this.A.X-this.C.X) + (this.C.X-this.B.X)*(this.A.Y-this.C.Y)
                            this.IsInside = function (P) {
                                var l1 = 1.0*((this.B.Y-this.C.Y)*(P.X-this.C.X)+(this.C.X-this.B.X)*(P.Y-this.C.Y))/this.detT;
                                var l2 = 1.0*((this.C.Y-this.A.Y)*(P.X-this.C.X)+(this.A.X-this.C.X)*(P.Y-this.C.Y))/this.detT;
                                var l3 = 1-l1-l2;
                               return l1 >= 0 && l1 <=1 && l2 >= 0 && l2 <=1 && l3 >= 0 && l1 <=3;
                               
                            }

                        }
                        var Point = function (x, y) {
                            this.X = x
                            this.Y = y;
                        }

                        function traverseChildren(elem) {
                            var children = [];
                            var q = [];
                            q.push(elem);
                            while (q.length > 0) {
                                var elem = q.pop();
                                children.push(elem);
                                pushAll(elem.children);
                            }
                            function pushAll(elemArray) {
                                for (var i = 0; i < elemArray.length; i++) {
                                    q.push(elemArray[i]);
                                }
                            }
                            return children;
                        }
                        

                        function runMenu(menu, itemContainer, menuItem, submenuClass) {
                            this._activeElement = null;
                            this._activeSubMenu = null;
                            this._isInTriangle = false;
                            this._hoverItem = null
                            this.mouseX = 0;
                            this.mouseY = 0;
                            this._t = null; // the active triangle
                            base = this;
                            this.activated = false;
                            this.timer = null;
                            //this.previousElement = null;
                            
                            this.ShowMenu = function (item) {
                                this.activated = true;
                                var topY =  $(item).outerHeight();
                                var drop = $(item).find(submenuClass);
                                drop.css('top', topY);
                                drop.show()
                                $(item).addClass("hover");
                                $(item).on("mouseout", function (e) {
                                    var list = traverseChildren(this);
                                    var el = e.toElement || e.relatedTarget;
                                    if (!!~list.indexOf(el)) {
                                        return;
                                    }
                                    //if (base._activeElement === null || base._t === null) {
                                    //    $(submenuClass).hide();
                                    //    $(itemContainer).removeClass("hover");
                                    //}
                                    e = jQuery.event.fix(e);
                                    var p = new Point(e.pageX, e.pageY);
                                    if (base._t != null && base._t.IsInside(p))
                                        return;
                                    base.HideMenu($(this));
                                    base.previousElement = base._activeElement
                                    base._activeElement = null;
                                    base._t = null;
                                    base.timer = setTimeout(function(){
                                        base.activated = false;
                                        },500);
                                });;
                                sendGAevents("navigation","hover",item.find("a.item").text())
                               
                            }
                            
                            this.HideMenu = function (item) {
                                var drop = $(item).find(submenuClass);
                                drop.hide()
                                $(item).removeClass("hover");
                                
                            }
                            this.SetTriangle = function (item, e, centered) {
                                var sub = item.find(submenuClass)
                                base._activeSubMenu = sub;
                                e = jQuery.event.fix(e);
                                var a = new Point(base.mouseX, base.mouseY - 1);
                                if(centered){
                                   var a = new Point(item.offset().left+(item.width()/2), e.pageY - 1);
                                }
                                
                                var b = a;
                                var c = a;
                                
                                if (sub.length != 0) {
                                    b = new Point($(sub).offset().left, $(sub).offset().top - 0)
                                    c = new Point($(sub).offset().left + $(sub).width(), $(sub).offset().top - 0)
                                }
                                base._t = new Triangle(a, b, c);
                            }
                            $(itemContainer + " " + menuItem).on("mouseenter", function (e) {
                                if (!base.activated) {
                                   return
                                }
                                if(base.timer != null){
                                    clearTimeout(base.timer)
                                    base.timer = null;
                                }
                                
                                e = jQuery.event.fix(e);
                                if (base._t === null) {
                                    base.SetTriangle($(this).parent(), e);
                                }
                                var p = new Point(e.pageX, e.pageY);
                                if (base._t.IsInside(p))
                                    return;
                                else
                                {
                                   if (base._activeElement != null) {
                                    base.HideMenu(base._activeElement.parent());
                                    }
                                    base._activeElement = $(this);
                                    base.ShowMenu($(this).parent());
                                    base.SetTriangle($(this).parent(), e,true);
                                }
     
                                });
                            
                            $(itemContainer + " " + menuItem).on("mousemove", function (e) {
                                e = jQuery.event.fix(e);
                                base.mouseX = e.pageX
                                base.mouseY = e.pageY;
                                if (base._activeElement === null) {
                                    return
                                }
                                base.activated = true;
                                if (base._t === null) {
                                    base.SetTriangle($(this).parent(), e);
                                }

                                if ($(this).text() == base._activeElement.text()) {
                                //base.SetTriangle($(this).parent(), e);
                                    return;
                                }
                                
                                var p = new Point(e.pageX, e.pageY);
                                if (base._t.IsInside(p))
                                    return;
                                else {

                                    base.HideMenu(base._activeElement.parent());
                                    base._activeElement = $(this);
                                    base.ShowMenu($(this).parent());
                                    base.SetTriangle($(this).parent(), e);
                                }
                            })          
                           
                            $(itemContainer).mousestop(function (event) {
                                base.activated = true;
                                if (base._activeElement === null) {
                                   base._activeElement = $($(this).find(menuItem));
                                }
                                else{
                                     if ($(this).find(menuItem).text() == base._activeElement.text()) {
                                        base.SetTriangle($(this), event);
                                            return;                                    }

                                     $(base._activeSubMenu).hide()
                                    $(base._activeElement).parent().removeClass("hover");            
                                }
                                base._activeElement = $($(this).find(menuItem));
                                    base._hoverItem = $($(this).find(menuItem));
                                    base.ShowMenu($(this));
                                    base.SetTriangle($(this), event);
                             });
                        }
                        hovernav = new runMenu('#topNav', '#topNav li', ".item", ".dropmenu");
    </script>

<!-- end header -->
            
