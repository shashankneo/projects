<script type="text/javascript">
var _scrollThreshold = $(window).height();
var _nextThreshold = _scrollThreshold
var _scrollCount = 0;
// record the first screen view
ga('send', 'event', 'Scrolling', 'Screen View', window.location.pathname, _scrollCount);
$(window).scroll(function(){
	var position = $(window).scrollTop();
	if (position > _nextThreshold){
		// fire off event
		_nextThreshold += _scrollThreshold;
		_scrollCount++;
		ga('send', 'event', 'Scrolling', 'Screen View', window.location.pathname, _scrollCount);
	}
})
</script>

