<script id="HDN-OB-WIDGET-MARKUP" type="text/javascript">
    (function () {
        var viewportMaxSide = (window.innerHeight || document.documentElement.clientHeight);
        if (viewportMaxSide < (window.innerWidth || document.documentElement.clientWidth)) {
            viewportMaxSide = (window.innerWidth || document.documentElement.clientWidth);
        }
        var formatToWidget = [
                         570,  'MB_2',
                         //696,  'AR_9',
                         //768,  'AR_9',
                         //900,  'AR_9',
                         1024,  'AR_9'
                        ];
        var widgetID = formatToWidget[formatToWidget.length-1];
        for (w = 0; w < formatToWidget.length; w++) {
            if (formatToWidget[w] >= viewportMaxSide) {
                widgetID = formatToWidget[w+1];
                break;
            }
        }
        var hn = document.location.host.split('.');
        var plh = 'www';
        if (hn[hn.length-3] == 'blog') {
            plh = 'blog';
        }
        cl = 'http://'+plh+'.'+hn[hn.length-2]+'.com'+document.location.pathname;
        document.write('<div staged-class="OUTBRAIN" data-src="http://www.sfgate.com/bayarea/article/Widlfires-rage-across-California-6419360.php"  data-widget-id="'+widgetID+'" data-ob-template="'+hn[hn.length-2]+'" data-src="'+cl+'"></div>');
        var sct = document.getElementById("HDN-OB-WIDGET-MARKUP");
        sct.parentNode.removeChild(sct);
})();
</script>

