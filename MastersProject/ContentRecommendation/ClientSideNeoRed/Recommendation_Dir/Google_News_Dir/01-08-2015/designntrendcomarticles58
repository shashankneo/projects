<script type="text/javascript">
	function view_topic_content(cate){

		  $.ajax({
				url: "/home/news/head_ajax.php",
				data: "category="+cate,
				cache : false,
				dataType: 'html',
				success: function(data){
					$('.headnews').html(data);
				}
		  });

	}

 var is_big_browser = true;

$(document).ready(function(){
	if($( window ).width()>783 ) {
		is_big_browser = true;
	} else {
		is_big_browser = false;
		$("#ulnav").hide();
	}
		$("#design").hide();$("#spaces").hide();	$("#listicles").hide(); $("#technology").hide();$("#entertainment").hide();$("#science").hide();$("#videos").hide();  //initial hide all

		$(".m1").hover( function(e){if(is_big_browser == false){return;}; $(".tcon2").slideDown("slow"); 	view_topic_content( $(this).attr("data-cate") );
		$("#design").show(); $("#technology").hide();$("#entertainment").hide();$("#science").hide();$("#listicles").hide(); $("#videos").hide();
		 });

		// $(".m2").hover( function(e){ $(".tcon2").slideDown("slow"); 	view_topic_content( $(this).attr("data-cate") );
		// $("#design").show(); $("#technology").hide();$("#entertainment").hide();$("#science").hide();$("#listicles").hide(); $("#videos").hide();
		//  });
        //
		// $(".m3").hover( function(e){   $(".tcon2").slideDown("slow"); 	view_topic_content( $(this).attr("data-cate") );
		// $("#design").hide();$("#spaces").hide();	$("#technology").hide();$("#entertainment").hide();$("#science").hide();  $("#fromatob").show();
		//  });

		$(".m4").hover( function(e){ if(is_big_browser == false){return;};$(".tcon2").slideDown("slow"); 	view_topic_content( $(this).attr("data-cate") );
		$("#technology").show(); $("#design").hide();$("#entertainment").hide();$("#science").hide();$("#listicles").hide(); $("#videos").hide();
		 });

		$(".m5").hover( function(e){if(is_big_browser == false){return;}; $(".tcon2").slideDown("slow"); 	view_topic_content( $(this).attr("data-cate") );
		$("#entertainment").show(); $("#technology").hide();$("#design").hide();$("#science").hide();$("#listicles").hide(); $("#videos").hide();
		 });

		$(".m6").hover( function(e){ if(is_big_browser == false){return;};$(".tcon2").slideDown("slow"); 	view_topic_content( $(this).attr("data-cate") );
		$("#science").show(); $("#technology").hide();$("#design").hide();$("#entertainment").hide();$("#listicles").hide(); $("#videos").hide();
		 });

        $(".m8").hover( function(e){if(is_big_browser == false){return;}; $(".tcon2").slideDown("slow"); 	view_topic_content( $(this).attr("data-cate") );
		$("#listicles").show(); $("#technology").hide();$("#design").hide();$("#entertainment").hide();$("#science").hide(); $("#videos").hide();
		 });

		$(document).on("hover", ".m7", function(e){ if(is_big_browser == false){return;};$(".tcon2").slideDown("slow"); 	view_topic_content( $(this).attr("data-cate") );
		$("#videos").show(); $("#technology").hide();$("#design").hide();$("#entertainment").hide();$("#science").hide(); $("#listicles").hide();
		 });

		$("#ulnav").mouseleave( function(){ if(is_big_browser == false){return;}; e.preventDefault();	$(".tcon2").hide();	});
		$(document).on("hover", '.m7', function(){ if(is_big_browser == false){return;}; $(".tcon2").hide();		$("#follow").hide();		$("#hd-search").hide();	});
		$("#ulnav").hover(function(){ if(is_big_browser == false){return;}; $("#tcon3").hide(); $("#follow").hide();		$("#hd-search").hide();	});

			$("div.tcon2").mouseleave( function(){if(is_big_browser == false){return;};	 	$(".tcon2").slideUp();	});
			$("#tcon3").mouseleave( function(){if(is_big_browser == false){return;};	$("#tcon3").slideUp();	});
			$("#t-sns").hover(function(){	 	$(".tcon2").hide(); $("#tcon3").hide(); 	$("#hd-search").hide();		$("#follow").show();	});
			$("#follow").mouseleave( function(){	 	$("#follow").hide();	});
			$("#t-srch").hover(function(){		 $(".tcon2").hide();		$("#follow").hide();		$("#hd-search").show();	});
			$("#hd-search").mouseleave( function(){	 	$("#hd-search").hide();	});

		$("#navBtn").click(function(){
			console.debug("sdf");
			$("#ulnav").toggle();
		});
		$("#morehid").hover(function(){

			$("#tcon2").hide();
			$("#tcon3").show();
			$("#follow").hide();
		});

});

$( window ).resize(function() {

	$("#ulnav").hover(function(){
		$("#tcon2").hide();
		$('#tcon3').hide();
	});

  if( $( window ).width() > 783){
  is_big_browser = true;
  }
  if( $( window ).width() < 783){
  $("#ulnav").hide();
  } else {
  $("#ulnav").show();
  }
});

	</script>

