//			gv_mobile_toggle_button(".post-taxonomy-button", ".post-taxonomy", ".small-screen-post-taxonomy-container", 'medium-screen-hidden');
//		});
		// Remove href value from postmeta buttons (which link to the sidebar ID unless activated by JS.
//		$('.small-screen-postmeta-buttons a').prop('href', '#');

		/**
		 * FLEXSLIDER PLUGIN
		 * 
		 * Animate cyclebox (ul.slides with big images) to fade between three slides
		 * use 'before' callback to add .active class to the matching headlines li
		 */
		if ($('.cyclebox').length) {
			$('.cyclebox').flexslider({
				animation: "fade",
				controlNav: false,
				pauseOnHover: true,
				prevText: '<span class="icon icon-angle-left"></span>',
				nextText: '<span class="icon icon-angle-right"></span>',
//				slideshow: false,
				before: function(slider) {
					$("li[data-slide-id='" + slider.currentSlide + "']").removeClass('active');
					$("li[data-slide-id='" + slider.animatingTo + "']").addClass('active');
				}
			});
			// When headlines are moused over switch slider to the matching slide (data-slide-id from html)
			$('.featured-headlines .headlines li').on('mouseover', function() {
				$('.cyclebox').flexslider($(this).data('slide-id'));
			});
		}
		
			/**
		 * Convert blockuotes with blockquote.translation's after them into jQuery UI tabs
		 */
		$("blockquote + .translation, div + .translation").each(function(index) {
			// Prepare values for this instance (there are likely several per article)
			var $this = $(this);
			var $prev = $this.prev();
			var originalId = 'original-tab-' + index;
			var translationId = 'translation-tab-' + index;
			
			// Do not convert the quotes if source is a tweet embed
			if ($prev.hasClass('twitter-tweet')) {
				return true;
			}

			// Insert a new div.translation-tabs with the tabs UL and blockquotes inside
			$this.after(
				$("<div></div>").addClass('translation-tabs').attr('id', 'translation-tabs-' + index)
					.append('<ul><li><a href="#' + translationId + '">Translation</a></li> <li><a href="#' + originalId + '">Original Quote</a></li></ul>')
					.append($("<blockquote></blockquote>").addClass($this.attr('class')).attr('id', translationId).html($this.html()))
					.append($("<blockquote></blockquote>").addClass($prev.attr('class')).attr('id', originalId).html($prev.html()))
			);

			// Remove old elements
			$prev.remove();
			$this.remove();
		});
		// Activate jQuery UI tabs	}); // end document.ready

		$(".translation-tabs").tabs();
		
	/**
	 * Activate FitVids
	 * @param {type} dropdown
	 * @returns {undefined}
	 */
    $(".post").fitVids();

	
