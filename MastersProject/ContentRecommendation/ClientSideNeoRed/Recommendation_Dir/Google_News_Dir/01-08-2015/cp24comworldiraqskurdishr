	<script type="text/javascript">
	requiresDependency('http://cauth.9c9media.com/auth9/axisAuthApi.js', function() {
  	axisAuthApi.events.addEvent('on_authentication_state_change', function (authenticationStatus, options) {
  		if (authenticationStatus == true) { 
  			$('#divLogIn-7_525535_1402440635_468869779').hide(); $('#divLogOut-7_525535_1402440635_468869779').show(); 
			}
  		else { 
				$('#divLogIn-7_525535_1402440635_468869779').show(); $('#divLogOut-7_525535_1402440635_468869779').hide(); 
			}
  	});
		});
				function jump(h){
    var top = document.getElementById(h).offsetTop;
    window.scrollTo(0, top);
}
	</script>
