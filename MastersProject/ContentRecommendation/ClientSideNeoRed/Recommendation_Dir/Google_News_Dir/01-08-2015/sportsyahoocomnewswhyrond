   (function(){if (!document.cookie.match(/GZ=Z=[0,1]/)) {
     document.cookie = 'GZ=Z=0';
     var i = document.createElement('iframe');
     i.src = '/__compressiontest.gzipped.html';
     i.style.display = 'none';
     document.body.appendChild(i);
   }})();
