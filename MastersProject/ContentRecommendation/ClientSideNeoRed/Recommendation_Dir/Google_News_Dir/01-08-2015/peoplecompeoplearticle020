        document.write('<div class="OUTBRAIN" data-src="' + OBprodURL + '" data-widget-id="AR_4" data-ob-template="people" ></div>');
    </script>
    <script type="text/javascript" src="http://widgets.outbrain.com/outbrain.js"></script>
    <script>
        $(document).ready(function() {
            var anchor_tag = '<a class="see-more" href="http://www.people.com/people/videos" title="See More Videos">see more videos</a>',
                $OBfooter = $('#right-rail #outbrain_widget_1 .AR_4.ob-widget .ob_what');
            if( $OBfooter.length ) $OBfooter.append(anchor_tag);
        });
    </script>
</div>
<!-- End: Outbrain -->

