<script>

function showThankYou(){
	// linktrack the submit button
	omniTrackEv('footer-newsletter-submit');
	
	// show thank you
	$('#newsletter-footer-form').hide();
	
	var $container = $('#newsletter_signup_footer_2'),
		$form = $container.find('form'),
		$checked = $form.find('.checkbox:checked'),
		$thankyou = $container.find('.thankyou'),
		$thankyouItem = $thankyou.find('li');

	$checked.each(function(){
		var id = $(this).attr('id'),
			matchedThanks = id.replace('footer', 'thankyou');

		$('.' + matchedThanks).removeClass('hidden');
	});
	
	$thankyou.fadeIn(100);
}

function validateEmail(email) {
	var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	return re.test(email);
}

$(document).ready(function(){
	if ( $('#newsletter_signup_footer_2').length != 0 ) {
		var $container = $('#newsletter_signup_footer_2'),
			$email = $('#footer-newsletter-email'),
			$checkbox = $container.find('.checkbox'),		// all checkboxes in subscribe module
            $all = $container.find('#footer-select-all'),	// the 'select all' checkbox
			$tvwatch = $container.find('#footer-tv-watch'),
			$duggars = $container.find('#footer-duggars'), 
			$bachelor = $container.find('#footer-bachelor'),
			$seeAll = $container.find('.see-all');

        $email.focus(function(){
            $(this).addClass('focus');
        });

        $all.click(function(){
            if ( $(this).is(':checked') ) {
                $checkbox.prop('checked', true);
            } else {
                $checkbox.prop('checked', false);
            }
        });

        $tvwatch.click(function(){
            if ( $(this).is(':checked') ) {
                $duggars.prop('checked', true);
				$bachelor.prop('checked', true);
            } else {
                $duggars.prop('checked', false);
                $bachelor.prop('checked', false);
            }
        });

        $seeAll.click(function(){
            linkTrack('S6_NL', 'All Newsletters');
        });


	} // end: if newsletter footer module exists


});
</script>
