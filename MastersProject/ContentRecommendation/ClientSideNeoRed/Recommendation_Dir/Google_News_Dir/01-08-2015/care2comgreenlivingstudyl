        <script>

        $(function() {
            var share_div_appended = false;
            var share_div = $("div.care2_side_float");

            var resize_share_div = function () {
                var left = $("#care2_wrapper").offset().left - 20 - share_div.outerWidth();
                share_div.css({
                    "left"       : left,
                    "visibility" : (left < 0) ? "hidden" : "visible"
                });
            }
            var append_share_div = function () {
                if(!share_div_appended) {
                    $(document.body).append(share_div.show());
                    share_div_appended = true;
                }
                resize_share_div();
                $(window).unbind("scroll", append_share_div);
            }

            $(window).resize(resize_share_div);
            $(window).scroll(append_share_div);
        });

    </script>
    

