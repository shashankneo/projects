        <script type="text/javascript">
            var criteo = {topOffset : 54};

            jQuery(function() {
                criteo.ad = jQuery('#criteoscroller');
                criteo.adTop = criteo.ad.offset().top;
                criteo.adWidth = criteo.ad.children(":first").width();
                criteo.adHeight = criteo.ad.children(":first").height();
                criteo.limitDiv = jQuery('.footer_ad');

                jQuery(window).scroll(fixDiv);
                fixDiv();
            });

            function fixDiv() {
                var scrollTop = jQuery(window).scrollTop();
                var limitTop = criteo.limitDiv.offset().top;

                if (scrollTop >= limitTop - criteo.adHeight - criteo.topOffset) {
                    criteo.ad.css(
                        {'position': 'relative', 'top': (limitTop - criteo.adHeight - criteo.adTop) + 'px'}
                    );
                } else if (scrollTop >= criteo.adTop - criteo.topOffset) {
                    criteo.ad.css(
                        {'position': 'fixed', 'top': criteo.topOffset + 'px', 'z-index' : '10', 'width' : criteo.adWidth}
                    );
                } else {
                    criteo.adTop = criteo.ad.offset().top;
                    criteo.ad.css(
                        {'position': 'static', 'top': 'auto'}
                    );
                }
            }
        </script>

</div>




