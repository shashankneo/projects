<div id="container">


<script type="text/javascript">jQuery("#menu-item- a").css({paddingTop:"7px",paddingBottom:"2px",marginTop:"6px",height:"17px",WebkitBorderTopLeftRadius: 16,  MozBorderRadiusTopLeft: 16, BorderTopLeftRadius: 16, WebkitBorderTopRightRadius: 0,MozBorderRadiusTopRight: 0, BorderTopRightRadius: 0})</script><div id="firehose" class="firehose">
<div class="fhhandle">At a Glance <span class="arrow">&#9660;</span></div>
</div>
<script>
jQuery(document).ready(function(){

if(jQuery.support.cssFloat && !isMobile.any()){

//console.log(jQuery(window).width() )
	if(jQuery(window).width() >= 1350){
		jQuery(settings.objSlidePanel).animate({
					'left' : '0px'
				});
				//Add the out class
				jQuery(settings.objSlidePanel).addClass('out');
	}
		jQuery(".firehose").insertBefore("#container");
		jQuery(".firehose").append("<span class='loading'>Loading the latest articles from JewishPress.com...</span>");
		slideOutTab();
}else{
	jQuery(".firehose").empty()
	jQuery(".fhhandle").empty()
}

});

///////
///Settings object
///////
var settings = {
	objSlideTrigger: '.fhhandle',
	objSlideArrow: '.arrow',
	objSlidePanel: '.firehose'
}
///////
///Slide out tab
///////
function slideOutTab() {
	//Bind a click handler to the trigger
	jQuery(settings.objSlideTrigger).bind('click' , function() {
		//If the panel isn't out
		if(!jQuery(settings.objSlidePanel).hasClass('out')){
			//Animate it to left 0px
			jQuery(settings.objSlidePanel).animate({
				'left' : '0px'
			});
			//Add the out class
			jQuery(settings.objSlidePanel).addClass('out');
			jQuery(settings.objSlideArrow).html('&#9660;');
		}
		else {
			//Otherwise, animate it back in
			jQuery(settings.objSlidePanel).animate({
				'left' : '-180px'
			});
			//Remove the out class
			jQuery(settings.objSlidePanel).removeClass('out');
			jQuery(settings.objSlideArrow).html('&#9650;');
		}
	});
}

var isMobile = {
    Android: function() {
        return navigator.userAgent.match(/Android/i);
    },
    BlackBerry: function() {
        return navigator.userAgent.match(/BlackBerry/i);
    },
    iOS: function() {
        return navigator.userAgent.match(/iPhone|iPad|iPod/i);
    },
    Opera: function() {
        return navigator.userAgent.match(/Opera Mini/i);
    },
    Windows: function() {
        return navigator.userAgent.match(/IEMobile/i);
    },
    any: function() {
        return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
    }
};

   
   </script>
<div style="clear:both;height:4px;"></div><script type="text/javascript">jQuery("#menu-item- a").css({paddingTop:"7px",paddingBottom:"2px",marginTop:"6px",height:"17px",WebkitBorderTopLeftRadius: 16,  MozBorderRadiusTopLeft: 16, BorderTopLeftRadius: 16, WebkitBorderTopRightRadius: 0,MozBorderRadiusTopRight: 0, BorderTopRightRadius: 0})</script><script>
pagecat = 'news';
</script>
