            var pagin = {
        		changepg:function(pg,track,ps){
        		    $('#pgctrlp').removeClass('pginactive').addClass('pgactive')
        		    $('#pgctrln').removeClass('pginactive').addClass('pgactive')
        		    if(pg=='next'){
        		       artcurrent++;
        		       if(artcurrent<=totalpg){
        		       	  pg = artcurrent;
        		       }else{
	        		      pg = totalpg;
	        		      artcurrent = totalpg;
        		       } 
        		     }
        		    if(pg=='prev'){
        		       artcurrent--;
        		       if(artcurrent<=1){
        		       	   pg = 1;
        		       	   artcurrent = 1;
        		       }else{
	        		       pg = artcurrent;
        		       }
        		     }    
        	    	for(var i=1;i<=totalpg;i++){
						$('#pagination'+i).hide();
					}   
					if(ps){History.pushState({}, 'Nick Gordon Begs to Attend Bobbi Kristina\'s Funeral - Entertainment - Holland Sentinel - Holland, MI', '?Start='+pg+'')}
				    $('#pagination'+pg).show();
				    for(tp=1;tp<=totalpg;tp++){
				    	$('#pgctrl'+tp).removeClass('pgNumactive');
				    }
				    $('#pgctrl'+pg).addClass('pgNumactive');
				    if(pg==totalpg){
					    $('#pgctrln').addClass('pginactive').removeClass('pgactive')
				    }
