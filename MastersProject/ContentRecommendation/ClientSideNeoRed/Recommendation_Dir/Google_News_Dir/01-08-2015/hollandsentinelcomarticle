<script type="text/javascript">
var fullartlink = "/article/ZZ/20150731/ENTERTAINMENT/307319963";
var authtext = "/author: By TV Guide/Nick Gordon Begs to Attend Bobbi Kristina\'s Funeral/20150731/307319963";
function track_art_page_facts(){track_elem(fullartlink+'/factbox');track_art_auth(authtext);}
function track_art_page_relimages(){track_elem(fullartlink+'/relatedimage');track_art_auth(authtext);}
function track_art_page(currBtn){track_elem(fullartlink+'/pagination/'+currBtn);track_art_auth(authtext);}
function track_art_auth(t){track_event(t);}
function prev_art_pg(topg){changepg(topg);track_art_page(topg);track_art_auth(authtext);}
function next_art_pg(topg){changepg(topg);track_art_page(topg);track_art_auth(authtext);}
function f5_art_ads() {googletag.cmd.push(function() {googletag.pubads().refresh([gptAdSlots[728901],gptAdSlots[3002501],gptAdSlots[728902]]);});}
$(document.body).on('click','.pgctrls',function(){
	if($(this).attr('which') == 'prev'){
		if((currartpg-1) > 0){
			prev_art_pg(currartpg-1);
		}
	}else if($(this).attr('which') == 'next'){
		if((currartpg+1) <= totalpg){
			if(currartpg == 0){
				next_art_pg(2);
			}else{
				next_art_pg(currartpg+1);
			}
		}
	}
});
</script>
