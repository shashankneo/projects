	<script type="text/javascript">
		function nl2br (str, is_xhtml) {   
			var breakTag = (is_xhtml || typeof is_xhtml === 'undefined') ? '<br />' : '<br>';    
			return (str + '').replace(/([^>\r\n]?)(\r\n|\n\r|\r|\n)/g, '$1'+ breakTag +'$2');

		}

		/*
		 * JavaScript Pretty Date
		 * Copyright (c) 2011 John Resig (ejohn.org)
		 * Licensed under the MIT and GPL licenses.
		 */

		// Takes an ISO time and returns a string representing how
		// long ago the date represents.
		function prettyDate(time){
			var date = new Date((time || "").replace(/-/g,"/").replace(/[TZ]/g," ")),
				diff = (((new Date()).getTime() - date.getTime()) / 1000)+21600, //add 21600 to convert to our time zone
				day_diff = Math.floor(diff / 86400);
					
			if ( isNaN(day_diff) || day_diff < 0 || day_diff >= 31 )
				return;
					
			return day_diff == 0 && (
					diff < 60 && "just now" ||
					diff < 120 && "1 minute ago" ||
					diff < 3600 && Math.floor( diff / 60 ) + " minutes ago" ||
					diff < 7200 && "1 hour ago" ||
					diff < 86400 && Math.floor( diff / 3600 ) + " hours ago") ||
				day_diff == 1 && "Yesterday" ||
				day_diff < 7 && day_diff + " days ago" ||
				day_diff < 31 && Math.ceil( day_diff / 7 ) + " weeks ago";
		}
		
		// If jQuery is included in the page, adds a jQuery plugin to handle it as well
		if ( typeof jQuery != "undefined" )
			jQuery.fn.prettyDate = function(){
				return this.each(function(){
					var date = prettyDate(this.title);
					if ( date )
						jQuery(this).text( date );
				});
			};

		function popularComments(storyid) {
			var threadsDetailsURL = 'http://disqus.com/api/3.0/threads/details.jsonp?forum=sltrib&api_key=tFtFDzLhJJyly1tuFVLVOym1iubnkcbTEnnpcMxKLT5fx4w2QcsEe8itp45LZbvx&thread:ident=' + storyid;
			$.ajax({
			     url: threadsDetailsURL,
			     dataType: 'jsonp',
				 crossDomain: 'true',
				 error: function(data) {
				 },
			     success: function(data) {
				 	$("#commentCount").html("("+ data.response.posts + ")")
					var commentCount = data.response.posts;
					var title = data.response.title;
			     	var threadid = data.response.id;
			     	if(commentCount > 0) {
						var commentsArray = new Array();
						var postsListPopularURL = 'http://disqus.com/api/3.0/posts/listPopular.jsonp?api_key=tFtFDzLhJJyly1tuFVLVOym1iubnkcbTEnnpcMxKLT5fx4w2QcsEe8itp45LZbvx&forum=sltrib&thread='+threadid+'&order=popular&limit=5&strict=1';
						$.ajax({
							 url: postsListPopularURL,
							 dataType: 'jsonp',
							 crossDomain: 'true',
							 error: function(data) {
							 },
							 success: function(posts) {
							 	popularCommentsCount = 0;
								$.each(posts.response,function(i,item){
									comment_thread_id = item.thread;
									comment_text = item.raw_message;
									comment_id = item.id;
									commentsArray.push(comment_id);
									comment = "";
									comment += '<div class="disqus_item">';
										comment += '<div class="disqus_avatar">';
											comment += '<a href="' + item.author.profileUrl + '" target="_blank"><img src="' + item.author.avatar.small.permalink + '" width="32" height="32"></a>';
										comment += '</div>';
										comment += '<div class="disqus_comment_content">';
											comment += '<div class="disqus_commenter_details">';
												comment += '<a href="' + item.author.profileUrl + '" target="_blank">' + item.author.name + '</a>';
											comment += '</div>';
											comment += '<div class="disqus_comment_text">';
												comment += nl2br(item.raw_message);
											comment += '</div>';
											comment += '<div class="disqus_comment_datos">';
												comment += prettyDate(item.createdAt) + ' &nbsp;&nbsp; ' + item.likes + ' likes';
											comment += '</div>';
										comment += '</div>';
									comment += '</div>';
									$("#disqus_popular_comments").append(comment);
									popularCommentsCount++;
								});
								if(popularCommentsCount < 5) {
									var extraLimit = 5 - popularCommentsCount; //how many additional comments we need
									var postsListURL = 'http://disqus.com/api/3.0/posts/list.jsonp?api_key=tFtFDzLhJJyly1tuFVLVOym1iubnkcbTEnnpcMxKLT5fx4w2QcsEe8itp45LZbvx&forum=sltrib&thread='+threadid+'&order=asc&strict=1&limit=' + extraLimit;
									$.ajax({
										 url: postsListURL,
										 dataType: 'jsonp',
										 crossDomain: 'true',
										 error: function(data) {
										 },
										 success: function(posts) {
											$.each(posts.response,function(i,item){
												comment_id = item.id;
												if(commentsArray.indexOf(comment_id) == -1) {
													comment = "";
													comment += '<div class="disqus_item">';
														comment += '<div class="disqus_avatar">';
															comment += '<a href="' + item.author.profileUrl + '" target="_blank"><img src="' + item.author.avatar.small.permalink + '" width="32"></a>';
														comment += '</div>';
														comment += '<div class="disqus_comment_content">';
															comment += '<div class="disqus_commenter_details">';
																comment += '<a href="' + item.author.profileUrl + '" target="_blank">' + item.author.name + '</a>';
															comment += '</div>';
															comment += '<div class="disqus_comment_text">';
																comment += nl2br(item.raw_message);
															comment += '</div>';
															comment += '<div class="disqus_comment_datos">';
																comment += prettyDate(item.createdAt) + ' &nbsp;&nbsp; ' + item.likes + ' likes';
															comment += '</div>';
														comment += '</div>';
													comment += '</div>';
													$("#disqus_popular_comments").append(comment);
													popularCommentsCount++;
												}
											});
											if(popularCommentsCount == 0) {
												comment = "<h4>We're still trying to determine which comments are the best.<br><dti:story:link fid=156>Click here to see all the comments and help us decide.</dti:story:link></h4><br><br>";
												$("#disqus_popular_comments").html(comment);
												$(".commentLink").hide();
											}
											
										 }
									});
									
								}
							 }
						});
					} else {
						comment = "<h4><dti:story:link fid=156>No comments on this story.</dti:story:link></h4><br><br>";
						$("#disqus_popular_comments").html(comment);
						$(".commentLink").hide();
					}

			     }
			});
		}

	</script>
	
	
<script type="text/javascript">
	var BrowserDetect = {
	init: function () {
		this.browser = this.searchString(this.dataBrowser) || "An unknown browser";
		this.version = this.searchVersion(navigator.userAgent)
			|| this.searchVersion(navigator.appVersion)
			|| "an unknown version";
		this.OS = this.searchString(this.dataOS) || "an unknown OS";
	},
	searchString: function (data) {
		for (var i=0;i<data.length;i++)	{
			var dataString = data[i].string;
			var dataProp = data[i].prop;
			this.versionSearchString = data[i].versionSearch || data[i].identity;
			if (dataString) {
				if (dataString.indexOf(data[i].subString) != -1)
					return data[i].identity;
			}
			else if (dataProp)
				return data[i].identity;
		}
	},
	searchVersion: function (dataString) {
		var index = dataString.indexOf(this.versionSearchString);
		if (index == -1) return;
		return parseFloat(dataString.substring(index+this.versionSearchString.length+1));
	},
	dataBrowser: [
		{
			string: navigator.userAgent,
			subString: "Chrome",
			identity: "Chrome"
		},
		{ 	string: navigator.userAgent,
			subString: "OmniWeb",
			versionSearch: "OmniWeb/",
			identity: "OmniWeb"
		},
		{
			string: navigator.vendor,
			subString: "Apple",
			identity: "Safari",
			versionSearch: "Version"
		},
		{
			prop: window.opera,
			identity: "Opera",
			versionSearch: "Version"
		},
		{
			string: navigator.vendor,
			subString: "iCab",
			identity: "iCab"
		},
		{
			string: navigator.vendor,
			subString: "KDE",
			identity: "Konqueror"
		},
		{
			string: navigator.userAgent,
			subString: "Firefox",
			identity: "Firefox"
		},
		{
			string: navigator.vendor,
			subString: "Camino",
			identity: "Camino"
		},
		{		// for newer Netscapes (6+)
			string: navigator.userAgent,
			subString: "Netscape",
			identity: "Netscape"
		},
		{
			string: navigator.userAgent,
			subString: "MSIE",
			identity: "Explorer",
			versionSearch: "MSIE"
		},
		{
			string: navigator.userAgent,
			subString: "Gecko",
			identity: "Mozilla",
			versionSearch: "rv"
		},
		{ 		// for older Netscapes (4-)
			string: navigator.userAgent,
			subString: "Mozilla",
			identity: "Netscape",
			versionSearch: "Mozilla"
		}
	],
	dataOS : [
		{
			string: navigator.platform,
			subString: "Win",
			identity: "Windows"
		},
		{
			string: navigator.platform,
			subString: "Mac",
			identity: "Mac"
		},
		{
			   string: navigator.userAgent,
			   subString: "iPhone",
			   identity: "iPhone/iPod"
	    },
		{
			string: navigator.platform,
			subString: "Linux",
			identity: "Linux"
		}
	]

};
BrowserDetect.init();
	browsername = BrowserDetect.browser;
	browserversion = BrowserDetect.version;
	
	oldbrowser = "false";
	
	if(browsername == "Firefox" && browserversion < "38") {
		oldbrowser = true;
	}
	
	if(browsername == "Safari" && browserversion < "5.2") {
		oldbrowser = true;
	}
	
	if(browsername == "Chrome" && browserversion < "36.9") {
		oldbrowser = true;
	}
	
	if(oldbrowser == true) {

	}else{
	
popularComments(2790976);

	}
	
	
	
</script>	
	
	
	
	
