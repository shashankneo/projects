	<script>
		// SIDE BAR STUFF
		$("#sidebar-toggle").click(function(e) {
			e.preventDefault();
			$("#wrapper").toggleClass("toggled");
		});
		$('#sidebar-wrapper .dropdown-item-toggle').click(function() {
			if($(this).next('.children').is(":visible")) {
				closeSidebarParent($(this).parent());
			} else {
				closeSidebarParents();
				openSidebarParent($(this).parent());
			}
		});
		function closeSidebarParents() {
			$('#sidebar-wrapper .parent').each(function() {
				console.log('closing side parent');
				if($(this).find('.children').is(":visible")) {
					closeSidebarParent($(this));
				}
			});
		}
		function closeSidebarParent(e) {
			var children = e.find('.children');
			children.slideUp();
			e.removeClass('open');
			e.children('i').removeClass('fa-caret-up').addClass('fa-caret-down');
		}
		function openSidebarParent(e) {
			e.find('.children').slideDown();
			e.addClass('open');
			e.children('i').removeClass('fa-caret-down').addClass('fa-caret-up');
		}
	</script>
	
	
	




	

