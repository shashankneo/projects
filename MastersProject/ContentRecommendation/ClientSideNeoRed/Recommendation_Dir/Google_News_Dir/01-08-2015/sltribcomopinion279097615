		<script language="javascript">
			$(document).ready(function() {
				var star = "/csp/mediapool/sites/Shared/assets/img/stars/star_new.png";
				var starSmall = "/csp/mediapool/sites/Shared/assets/img/stars/star_sm_new.png";
				var halfStar = "/csp/mediapool/sites/Shared/assets/img/stars/star_half_new.png";
				var halfStarSmall = "/csp/mediapool/sites/Shared/assets/img/stars/star_half_sm_new.png";
				var	bellSmall = "/csp/mediapool/sites/Shared/assets/img/stars/bell_sm_new.png";
				var	halfBellSmall = "/csp/mediapool/sites/Shared/assets/img/stars/bell_half_sm_new.png";
				
				// Large Stars
				if ($("p.BOX_Head_Stars").length){
					var box = $("p.BOX_Head_Stars");
					var boxLength = $("p.BOX_Head_Stars").length;
					for (b=0;b<boxLength;b=b+1){
						var boxText= box[b].innerHTML;
						newText= boxText.replace(/H/g, '<img src="'+star+'" />');
						newText= newText.replace('hj', '<img src="'+halfStar+'" />');
						newText= newText +'<div class="clear"></div>';
						box[b].innerHTML = newText;
					}
				}
				// Large Stars
				if ($("p.BOX_Head_Stars_G").length){
					var box = $("p.BOX_Head_Stars_G");
					var boxLength = $("p.BOX_Head_Stars_G").length;
					for (b=0;b<boxLength;b=b+1){
						var boxText= box[b].innerHTML;
						newText= boxText.replace(/H/g, '<img src="'+star+'" />');
						newText= newText.replace('hj', '<img src="'+halfStar+'" />');
						newText= newText +'<br>';
						box[b].innerHTML = newText;
					}
				}
				// Small Stars
				if ($("p.BOX_Text_w_Stars").length){
					var box = $("p.BOX_Text_w_Stars");
					var boxLength = $("p.BOX_Text_w_Stars").length;
					for (b=0;b<boxLength;b=b+1){
						var boxText= box[b].innerHTML;
						newText= boxText.replace(/H/g, '<img src="'+starSmall+'" />');
						newText= newText.replace('hj', '<img src="'+halfStarSmall+'" />');
						newText= newText.replace('Food »', '<b>Food &raquo;</b>');
						newText= newText.replace('Mood »', '<b>Mood &raquo;</b>');
						newText= newText.replace('Service »', '<b>Service &raquo;</b>');
						newText= newText.replace('Noise »', '<b>Noise &raquo;</b>');
						newText= newText +'<br>';
						box[b].innerHTML = newText;
					}
				}
				// Small Bells
				if ($("p.BOX_Text_w_Bells").length){
					var box = $("p.BOX_Text_w_Bells");
					var boxLength = $("p.BOX_Text_w_Bells").length;
					for (b=0;b<boxLength;b=b+1){
						var boxText= box[b].innerHTML;
						newText= boxText.replace(/b/g, '<img src="'+bellSmall+'" />');
						newText= newText.replace('hj', '<img src="'+halfBellSmall+'" />');
						newText= newText.replace('Food »', '<b>Food &raquo;</b>');
						newText= newText.replace('Mood »', '<b>Mood &raquo;</b>');
						newText= newText.replace('Service »', '<b>Service &raquo;</b>');
						newText= newText.replace('Noise »', '<b>Noise &raquo;</b>');
						newText= newText +'<br>';
						box[b].innerHTML = newText;
					}
				}
			})
			
			</script>
