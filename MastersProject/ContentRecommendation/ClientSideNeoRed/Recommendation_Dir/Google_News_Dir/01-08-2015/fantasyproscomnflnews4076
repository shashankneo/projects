<script type="text/javascript">
function createCookie(name,value,days) {
  if (!days) {
    days = 21;
  }
  var date = new Date();
  date.setTime(date.getTime()+(days*24*60*60*1000));
  var expires = "; expires="+date.toGMTString();

  document.cookie = name+"="+value+expires+"; path=/";
}

$(document).ready(function() {

  createCookie('cookies_enabled', 1, 1);
  cookieEnabled = (document.cookie.indexOf("cookies_enabled") != -1) ? true : false;

  if(cookieEnabled && document.cookie.indexOf("lb") == -1) {
      if($(window).width() > 767) {
         setTimeout(function() {
         $.colorbox({title: 'Mock Drafts by FantasyPros',
                     open: 'true',
                     width: '700px',
                     height: '500px',
                     html: '<a href="http://draftwizard.fantasypros.com/simulateDraft?sport=nfl&partner=lb-green" target="_blank"><img src="http://images.fantasypros.com/images/ads/DW_NFL_Lightbox_700x440_Green.jpg"></a>'
                   });
         }, 7500);
      }
  }

  if(document.cookie.indexOf("lb") == -1) {
    createCookie('lb', 1, 20);
  }

});
</script>
