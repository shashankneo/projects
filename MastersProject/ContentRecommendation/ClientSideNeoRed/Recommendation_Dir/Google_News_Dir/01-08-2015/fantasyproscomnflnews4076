<script type="text/javascript" src="http://cdn.fantasypros.com/csw/bootstrap_2_3_2/js/bootstrap.min.js"></script>


<script type="text/javascript">
$(document).ready(function() {
    $('.nav-search-link').click(function() {
        $('.nav .search .dropdown-menu').toggle();
        $('.nav .search #playername').focus();
    });
    $('body').click(function(e) {
        if (!$(e.target).is('.nav-search-link') && !$(e.target).is('.nav-search-link img') && !$(e.target).is('#playername')) {
            $('.nav .search .dropdown-menu').hide();
        }
    });
    $('.ui-autocomplete').click(function() {
        if($('.nav .search #playername').val() != '') {
            $('.nav .search form').submit();
        }
    });

    $('.mobile-nav .dropdown').click(function() {
        $(this).find('.dropdown-menu').toggle();
        $(this).find('>a').toggleClass('active');
        if($(this).find('.dropdown-menu').css('display') == 'none') {
            $(this).find('a').css('background-color', 'none');
        }
            // if($(this).find('.dropdown-menu').each(function(e) {
            //     if($(e).css('display') == 'none') {
            //         $(e).children().find('a').css('background-color', 'none');
            //     }
            // });
    });

    setTimeout(function() {
        var iframe_width = $('#leaderboard iframe').contents().width();
        if(iframe_width == 728) {
            $('#leaderboard > div').css('width', '728px');
        }
    }, 2000);

    loggedin = (document.cookie.indexOf("fp_user") != -1) ? true : false;
    if(loggedin) {
        $('.show-logout').show();
    } else {
        $('.show-login').show();
    }

});
</script>
