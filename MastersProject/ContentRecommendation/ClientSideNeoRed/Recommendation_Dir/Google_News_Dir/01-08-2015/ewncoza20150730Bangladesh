<script type="text/javascript">
    function ExecuteAJAXCallMailNewsArticle() {

        if (jQuery('#ItemIDForMail').attr('itemID') == "") {
            jQuery('#ItemIDForMail').attr('itemID', '{721C0109-FC60-416D-9EAC-4B1A9899F7CD}');
        }

        if (jQuery("#txtName").val() == "" || jQuery("#txtFrom").val() == "" || jQuery("#txtTo").val() == "" ||
           jQuery("#txtSubject").val() == "" || jQuery("#txtMessage").val() == "" || jQuery("#txtNameTo").val() == "") {
            alert("All fields need to be completed");
        }
        else {
            jQuery.ajax({
                url: "/layouts/EWNMailAFriendAjaxRequest.ashx?MailName=" + jQuery("#txtName").val() +
                                                        "&MailYours=" + jQuery("#txtFrom").val() +
                                                        "&NameTheirs=" + jQuery("#txtNameTo").val() +
                                                        "&MailTheirs=" + jQuery("#txtTo").val() +
                                                        "&MailSubject=" + jQuery("#txtSubject").val() +
                                                        "&MailMessage=" + jQuery("#txtMessage").val() +
                                                        "&ItemId=" + jQuery('#ItemIDForMail').attr('itemID'),
                dataType: 'text',
                success: Mailsent,
                error: Mailfailed
            });
        }
    };

    function Mailsent() {
        jQuery("#mailedSuccessfully").show();
        jQuery("#tellfriend").hide();
        window.location = window.location;
    }

    function Mailfailed() {
        jQuery("#mailFailed").show();
        jQuery("#tellfriend").hide();
        setTimeout("window.location = window.location", 5000);
    }
</script>

