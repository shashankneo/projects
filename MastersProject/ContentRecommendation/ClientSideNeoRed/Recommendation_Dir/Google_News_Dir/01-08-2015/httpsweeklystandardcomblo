<script language="javascript">
Drupal.behaviors.tws = function() {

  $('.ahah-progress-throbber').hide();

  $('#alert-signup-button').bind('click', function(){
		$('.ahah-progress-throbber').show();
		var subscriptions = new Array;
		if ($('#fred-barnes:checked').attr('checked')) subscriptions.push('fred-barnes');
		if ($('#bill-kristol:checked').attr('checked')) subscriptions.push('william-kristol');
		if ($('#jay-cost:checked').attr('checked')) subscriptions.push('jay-cost');
		if ($('#scrapbook:checked').attr('checked')) subscriptions.push('scrapbook');
		if ($('#news:checked').attr('checked')) subscriptions.push('newsletter');
		if ($('#optin:checked').attr('checked')) subscriptions.push('optin');
		subscriptions = subscriptions.join('+');
		data = {
				'challenge': $('#recaptcha_challenge_field').val(),
				'response': $('#recaptcha_response_field').val(),
				'email': $('#email').val(),
				'email_confirm': $('#email-confirm').val(),
				'zip': $('#zip').val(),
				'subscriptions': subscriptions,
		}
		$.post('/alerts/signup', data, signup_response);
    return false;
  });
  
  var signup_response = function(data) {
    var result = Drupal.parseJson(data);
    $('#signup-results').html(result['response']);
    $('.ahah-progress-throbber').hide();
    if (result['status'] == 1) {
    	$('#signup-form').hide();
    }
  } 
}
</script>

