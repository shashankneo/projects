				// callbacks
				var chatter_module = jQuery('.cm-lc-widget-collapsed, .cm-comments-heading');
				widget.on('commentCountUpdated', function(data){
					if (data > 0) {
						jQuery('.cm-lc-nocontent').hide();
						if (jQuery('.cm-lc-widget.cm-lc-empty').length) {
							jQuery('.cm-lc-widget.cm-lc-empty').removeClass('cm-lc-empty');
						}
						update_comment_count(data);
					}
				});
				widget.on('userLoggedIn', function() {
					if (janrain.capture.ui.hasActiveSession()) {
						chatter_module.show();
						cmgjp2PingToPull();
					}
				});
				widget.on('initialRenderComplete', function() {
					chatter_module.show();
					set_comment_count(jQuery);
					cmgjp2PingToPull();
					if (jQuery('.fyre-auth .fyre-user-loggedin').length) {
						jQuery('.cm-lc-nocontent .cm-lc-not-logged-in').hide();
					}
				});
