    <script>
    var janrain_timeout_int = 500,
        livefyre_livecomments = null,
        $comment_count_display = cmg.query('.cm-comment-count, .cm-navbar-comment-bubble'),
        $chatter_large = cmg.query('.cm-lc-widget-expanded'),
        $chatter_large_close = cmg.query('.cm-lc-widget-close'),
        toggle_expanded_chatter = function($) {
            var $ = $ || cmg.query,
                $pinned_header = $('.cm-navbar-pinned-story-container'),
                $pinned_header_comment_count = $('.cm-navbar-comments'),
                $chatter_large_content = $('.cm-lc-widget-expanded .cm-lc-widget-container'),
                $chatter_small_content = $('.cm-lc-widget-collapsed .cm-lc-widget-container'),
                $modal_close = $('.cm-close-modal'),
                height = $(window).height() - 45;
            if ($chatter_large.is(':visible')) {
                $chatter_large
                    .animate({
                        height: 0
                    }, {
                        duration: 200,
                        complete: function() {
                            $(this).fadeOut('slow');
                            $chatter_small_content.show();
                            $chatter_large_close.fadeOut();
                            if ($pinned_header.length) {
                                $pinned_header.removeClass('always-visible');
                                $pinned_header_comment_count.removeClass('active');
                                if (typeof cmg.cmgMainNav !== 'undefined' && typeof cmg.cmgMainNav.headline_check !== 'undefined') {
                                    cmg.cmgMainNav.headline_check()();
                                }
                            }
                            $modal_close
                                .fadeIn()
                                .removeClass('cm-hidden');
                        }
                    });
            } else {
                $chatter_large
                    .fadeIn('slow')
                    .animate({
                        height: height
                    }, {
                        duration: 200,
                        complete: function() {
                            if ($pinned_header.length) {
                                $pinned_header.addClass('always-visible');
                                $pinned_header_comment_count.addClass('active');
                                if (typeof cmg.cmgMainNav !== 'undefined' && typeof cmg.cmgMainNav.display_story_headline !== 'undefined') {
                                    cmg.cmgMainNav.display_story_headline();
                                }
                            }
                            var $chatter_html = $chatter_small_content.clone(true);
                            $chatter_large_close.fadeIn();
                            $modal_close
                                .fadeOut()
                                .addClass('cm-hidden');
                            $(this).css({
                                overflow: 'visible'
                            })
                        }
                    });
            }
        },
        set_comment_count = function($) {
            var comment_count = $('.fyre-stream-stats .fyre-comment-count').text().replace(/[^0-9]+/g, '');
            $comment_count_display.text(comment_count);
        },
        update_comment_count = function(count) {
            $comment_count_display.text(count);
        },
        cmg_ping_to_pull = function() {
            if (typeof janrain != 'undefined' && localStorage && localStorage.getItem('janrainCaptureProfileData')) {
                var profileData = localStorage.getItem('janrainCaptureProfileData');
                profileData = typeof profileData == "string" ? JSON.parse(profileData) : profileData;
                if (profileData.uuid) {
                    jQuery.get('/auth/ajax/ping-pull/' + profileData.uuid, function(data) {
                        if (data.indexOf('http') !== -1) {
                            jQuery.get(data, function(response) {
                                // do nothing; the request kicks off a sync on livefyre's side
                            });
                        } else {
                            //console.error(data);
                        }
                    });
                } else {
                    //console.error('Problem with uuid/profileData: ', profileData);
                }
            }
        },
        check_janrain_loggedin_status = function() {
            if (janrain.capture.ui && !janrain.capture.ui.hasActiveSession()) {
                // not signed in but livefyre thinks we are
                // delete the backplane-channel cookie and
                // remove the fyre-auth entry from localstorage
                clear_livefyre_creds();
            }
        },
        clear_livefyre_creds = function() {
            var yesterday = new Date();
            yesterday.setTime(yesterday.getTime() + (-1 * 24 * 60 * 60 * 1000));
            document.cookie = 'backplane-channel' + '=; expires=' + yesterday.toUTCString() + '; path=/';
            localStorage.removeItem('fyre-auth');
        };

    cmg.query(document).one('ready', function(e) {

        var $ = cmg.query || jQuery;

        if (location.search.indexOf('showcomments') > -1) {
            toggle_expanded_chatter($);
        }

        $chatter_large.css({
            right: function() {
                return ($(window).width() - $('.cm-modal-content').width()) / 2
            }
        });

        $chatter_large_close.css({
            right: function() {
                return ($(window).width() - $('.cm-modal-content').width()) / 2 + $chatter_large.width() - ($(this).width() / 2)
            }
        });

        $('a.cm-lc-expand, a.cm-story-comments, div.cm-lc-widget-close, .cm-navbar-comments').off('click.lc-expand').on('click.lc-expand', function(e) {
            e.preventDefault();
            toggle_expanded_chatter($);
        });

        $('a.cmLogout, a.capture_end_session').off('click').on('click', function(e) {
            var root_domain = location.hostname.split('.').slice(-2).join('.'),
                cookie_options = {
                    expires: -2,
                    path: '/',
                    domain: root_domain
                },
                old_cookie_options = {
                    expires: -2,
                    path: '/'
                };
            e.preventDefault();
            clear_livefyre_creds();
            $.cookie('ur_name', '', cookie_options);
            $.cookie('ur_metrics', '', cookie_options);
            $.cookie('ur_name', '', old_cookie_options);
            $.cookie('ur_metrics', '', old_cookie_options);
            janrain.capture.ui.endCaptureSession();
        });
    });
    </script>
