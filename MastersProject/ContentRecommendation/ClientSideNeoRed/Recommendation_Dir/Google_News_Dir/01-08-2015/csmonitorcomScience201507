		<script type="text/javascript">
		
		function qc_results( result )
		{
			$.cookie.json = true;

			var quantSegs = $.cookie( 'QuantCastAudienceSegments' );

			if( !quantSegs )
			{
				//build and store quantSegs
				quantSegs = [];
				for( var i = 0; i < result.segments.length; i++ )
				{
					quantSegs.push( result.segments[i].id ); 
				}
				$.cookie( 'QuantCastAudienceSegments', quantSegs, { expires: 4, path: '/' });
			}
		}
		</script>
		<script type="text/javascript" src="//pixel.quantserve.com/api/segments.json?a=p-44NYiIzL-i6wg&callback=qc_results&ttl=86400"></script>
		
		
