		function moveSurvey() {
			var survey = document.getElementById("contain-402");
			if (survey !== null && survey.hasAttribute("style")) {
				sb = document.createElement("style");
				sb.innerHTML = "#contain-402 {left:20px !important; right:auto !important;}";
