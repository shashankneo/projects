<script>
/*
var rediUrl = 'http://m.thestar.com.my/';
if($('input[name*=tagimaGuid]').val() !== undefined) {
 rediUrl = 'http://m.thestar.com.my/story.aspx?id=' + $('input[name*=tagimaGuid]').val();
