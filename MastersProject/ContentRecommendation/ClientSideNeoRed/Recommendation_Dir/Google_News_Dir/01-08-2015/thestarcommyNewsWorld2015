<script type="text/javascript">
    $(document).ready(function () {
        var randomLinks = ["http://clips.thestar.com.my/mostviewed/star2/entertainment.json", "http://clips.thestar.com.my/mostviewed/star2/style.json", "http://clips.thestar.com.my/mostviewed/star2/culture.json", "http://clips.thestar.com.my/mostviewed/star2/people.json", "http://clips.thestar.com.my/mostviewed/star2/food.json", "http://clips.thestar.com.my/mostviewed/star2/health.json", "http://clips.thestar.com.my/mostviewed/star2/family.json", "http://clips.thestar.com.my/mostviewed/star2/living.json", "http://clips.thestar.com.my/mostviewed/star2/travel.json"];
        jsonRandomName = randomLinks[Math.floor(Math.random() * randomLinks.length)];
        jsonRandomTitle = jsonRandomName.substring(jsonRandomName.lastIndexOf('/') + 1).replace(".json", "");
        jsonRandomTitle = jsonRandomTitle.charAt(0).toUpperCase() + jsonRandomTitle.substr(1);
        $('#moreEntertainmentNewsHeadlineTitle').append("Trending in " + jsonRandomTitle);
        $.ajax({
            type: 'GET',
            url: jsonRandomName,
            jsonpCallback: 'star2JsonCallback',
            contentType: "application/json",
            dataType: 'jsonp',
            success: function (objRandom) {
                for (var x = 0; x < 5; x++) {
                    var stringBuilderMostViewedRandom = '<li class="xs" data-vr-contentbox=""><a href="' + objRandom.items[x].domain + objRandom.items[x].link + '" class="lnews">' + objRandom.items[x].headline + '</a></li>';
                    if (objRandom.items[x].image != "") {
                        stringBuilderMostViewedRandom = '<li class="xs" data-vr-contentbox=""><div class="moreheadlineimg"><a href="' + objRandom.items[x].domain + objRandom.items[x].link + '"><img src="' + objRandom.items[x].image + '" class="relative"></a></div><a href="' + objRandom.items[x].domain + objRandom.items[x].link + '" class="lnews">' + objRandom.items[x].headline + '</a></li>';
                    }
                    $('#most_viewed_random_tab').append(stringBuilderMostViewedRandom);
                    $('#mostViewedRandomErr').hide();
                }
                if (objRandom.items.length == 0) {
                    $('#most_viewed_random_tab').after('<span class="error-msg" id="mostViewedRandomErr">Sorry, the Tranding list is currently not available.</span>');
                }
            },
            error: function () {
                $('#most_viewed_random_tab').after('<span class="error-msg" id="mostViewedRandomErr">Sorry, the Trending list is currently not available.</span>');
                console.log("An Error occur at MostViewedRandom component");
            },
            timeout: 10000
        });
    });
</script>

