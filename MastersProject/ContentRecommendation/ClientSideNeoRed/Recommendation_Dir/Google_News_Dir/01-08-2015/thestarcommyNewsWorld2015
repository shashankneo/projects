<script type="text/javascript">var jsonName = 'news-world';</script>
<script type="text/javascript">

    $.ajax({
        type: 'GET',
        url: 'http://clips.thestar.com.my/mostviewed/' + jsonName + '.json?callback=?',
        jsonpCallback: 'jsonCallback',
        contentType: "application/json",
        dataType: 'jsonp',
        success: function (obj) {
            for (var x = 0; x < obj.items.length; x++) {
                var stringBuilderMostViewed = '<li data-vr-contentbox=""><p class="xs"><a href="' + obj.items[x].link + '">' + obj.items[x].headline + '</a></p></li>';
                //console.log(stringBuilderMostViewed);
                //alert(stringBuilderMostViewed);
                $('#most_viewed_tab').append(stringBuilderMostViewed);
                $('#mostViewedErr').hide();
            }
            if (obj.items.length == 0) {
                $('#most_viewed_tab').after('<span class="error-msg" id="mostViewedErr">Sorry, the Most Viewed list is currently not available.</span>');
            }
        },
        error: function () {
            $('#most_viewed_tab').after('<span class="error-msg" id="mostViewedErr">Sorry, the Most Viewed list is currently not available.</span>');
            console.log("An Error occur at MostViewedShared component");
        },
        timeout: 10000
    });

</script>
