<script>

    var shareURLTop = $("meta[property='og:url']").attr("content");
    var titleTop = $(".headline").text().trim();
    var descriptionTop = $("meta[property='og:description']").attr("content");
    var imageURLTop = $("meta[property='og:image']").attr("content");
    var sectionsTop = $("meta[name='sections']").attr("content");
    var otherTagsTop = $("meta[name='other tags']").attr("content");
    var gigyaSocialTop = new gigya.socialize.UserAction();
    var tagsTop = "";

    if (typeof imageURLTop != 'undefined')
        gigyaSocialTop.addMediaItem({ type: "image", src: imageURLTop, href: shareURLTop });

    if (typeof otherTagsTop != 'undefined') {
        if (otherTagsTop.indexOf(',') != -1) {
            var sectionsArrayTop = sectionsTop.split(',');
            var otherTagsArrayTop = otherTagsTop.split(',');
            for (x = 0; x < otherTagsArrayTop.length; x++) {
                for (i = 0; i < sectionsArrayTop.length; i++) {
                    if (sectionsArrayTop[i].trim() == otherTagsArrayTop[x].trim()) {
                        break;
                    }
                    else {
                        tagsTop = tagsTop + ',' + otherTagsArrayTop[x].trim();
                        break;
                    }
                }
            }
        }
        tagsTop = sectionsTop.trim() + tagsTop;
    }

    gigyaSocialTop.setTitle(titleTop);
    gigyaSocialTop.setDescription(descriptionTop);
    gigyaSocialTop.setLinkBack(shareURLTop);

    var gigyaSocial_params_FB_Top =
    {
        userAction: gigyaSocialTop,
        tags: tagsTop,
        containerID: "gigya-share_top_FB",
        showCounts: 'none',
        shareButtons: "Facebook",
        buttonTemplate: '<img src="http://clips.thestar.com.my/social/btn_facebook.png"  onclick="$onClick" />'
    }
    gigya.socialize.showShareBarUI(gigyaSocial_params_FB_Top);
    var gigyaSocial_params_TW_Top =
    {
        userAction: gigyaSocialTop,
        tags: tagsTop,
        containerID: "gigya-share_top_TW",
        showCounts: 'none',
        shareButtons: "Twitter",
        buttonTemplate: '<img src="http://clips.thestar.com.my/social/btn_twitter.png"  onclick="$onClick" />'
    }
    gigya.socialize.showShareBarUI(gigyaSocial_params_TW_Top);
    var gigyaSocial_params_GO_Top =
    {
        userAction: gigyaSocialTop,
        tags: tagsTop,
        containerID: "gigya-share_top_GO",
        showCounts: 'none',
        shareButtons: "Googleplus",
        buttonTemplate: '<img src="http://clips.thestar.com.my/social/btn_googlePlus.png"  onclick="$onClick" />'
    }
    gigya.socialize.showShareBarUI(gigyaSocial_params_GO_Top);
    var gigyaSocial_params_LN_Top =
    {
        userAction: gigyaSocialTop,
        tags: tagsTop,
        containerID: "gigya-share_top_LN",
        showCounts: 'none',
        shareButtons: "LinkedIn",
        buttonTemplate: '<img src="http://clips.thestar.com.my/social/btn_linkedin.png"  onclick="$onClick" />'
    }
    gigya.socialize.showShareBarUI(gigyaSocial_params_LN_Top);
    var gigyaSocial_params_EM_Top =
    {
        userAction: gigyaSocialTop,
