<script type="text/javascript">
    var shareURLTopImg = $("meta[property='og:url']").attr("content");
    var titleTopImg = $("meta[name='headline']").attr("content") + " | The Star Online";
    var descriptionTopImg = $("meta[property='og:description']").attr("content");
    var imageURLTopImg = $("meta[property='og:image']").attr("content");
    var sectionsTopImg = $("meta[name='sections']").attr("content");
    var otherTagsTopImg = $("meta[name='other tags']").attr("content");
    var gigyaSocialTopImg = new gigya.socialize.UserAction();
    var tagsTopImg = "";

    if (typeof imageURLTopImg != 'undefined')
        gigyaSocialTopImg.addMediaItem({ type: "image", src: imageURLTopImg, href: shareURLTopImg });

    if (typeof otherTagsTopImg != 'undefined') {
        if (otherTagsTopImg.indexOf(',') != -1) {
            var sectionsArrayTopImg = sectionsTopImg.split(',');
            var otherTagsArrayTopImg = otherTagsTopImg.split(',');
            for (x = 0; x < otherTagsArrayTopImg.length; x++) {
                for (i = 0; i < sectionsArrayTopImg.length; i++) {
                    if (sectionsArrayTopImg[i].trim() == otherTagsArrayTopImg[x].trim()) {
                        break;
                    }
                    else {
                        tagsTopImg = tagsTopImg + ',' + otherTagsArrayTopImg[x].trim();
                        break;
                    }
                }
            }
        }
        tagsTopImg = sectionsTopImg.trim() + tagsTopImg;
    }

    gigyaSocialTopImg.setTitle(titleTopImg);
    gigyaSocialTopImg.setDescription(descriptionTopImg);
    gigyaSocialTopImg.setLinkBack(shareURLTopImg);

