        </div>
    </ul>
    <ul class="social-network-mobile">
        <div class="gigya-wrapper">
            <div id="gigya-share_bottom_FB_m" class="social-media-horizontal" style="float: left; cursor: pointer;"></div>
            <div id="gigya-share_bottom_TW_m" class="social-media-horizontal" style="float: left; cursor: pointer;"></div>
            <div id="gigya-share_bottom_GO_m" class="social-media-horizontal" style="float: left; cursor: pointer;"></div>
            <div id="gigya-share_bottom_LN_m" class="social-media-horizontal" style="float: left; cursor: pointer;"></div>
            <div id="gigya-share_bottom_WA_m" class="social-media-horizontal" style="float: left; cursor: pointer;"></div>
            <div id="gigya-share_bottom_EM_m" class="social-media-horizontal" style="float: left; cursor: pointer;"></div>
        </div>
