        function confirmDownload() {
            var r = confirm('We invite you to download our new iPad app. Click Ok to download or Cancel to continue');
            var newHost = 'https://itunes.apple.com/il/app/haaretz-english-edition-for/id500132377?mt=8';
             if (r == true) {
                window.location = newHost;
             }
           	return false;
        }
        var userAgent = navigator.userAgent.toLowerCase();
        var isIpad = userAgent.indexOf("ipad") > -1;
        var expires =   1000*60*60*24*14 // 2 weeks
        var locTime = localStorage.getItem('htzIpad');
        
        if (isIpad && (!locTime || (locTime && new Date().getTime() > parseInt(locTime) + parseInt(expires)))){
        	localStorage.setItem('htzIpad', new Date().getTime());
        	confirmDownload();
        }
