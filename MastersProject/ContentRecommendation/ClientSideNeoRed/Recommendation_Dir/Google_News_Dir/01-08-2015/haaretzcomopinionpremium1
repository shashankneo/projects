<script type="text/javascript">
	// -- SETTINGS -- //
	var isBetaPage = location.href.indexOf("beta")!=-1;
    var isCm = location.href.indexOf("140:")!=-1;

	var percent			= 10;			// Chance of user being redirected to beta page (integer percentage)
	//var payingUser		= true;			// Enable beta redirection for paying users (true / false)
	//var registeredUser	= false;		// Enable beta redierction for registered users (true / false)


	// -- DO NOT CHANGE CODE BELOW THIS LINE -- //
	var articleId = location.href.substring(location.href.indexOf('1.'));
	var redirect = false;
	//var pusr = (document.cookie.indexOf('hdcPusr') > -1);
	//var rusr = (document.cookie.indexOf('hdcRusr') > -1);
	// Check whether user is already selected as beta reader and accepting participation

	try{
  
	if (localStorage.hdcResp) {
    console.log("localStorage.hdcResp")
		var hdcRespObject = JSON.parse(localStorage.hdcResp);
  		if (hdcRespObject.responsiveMode) {
      	console.log("REDIRECT")
    		redirect = true;
		}
  		else {
    		redirect = false;
  		}
	}
		
		var range = (percent != 0) ? 100 / percent : 0;
		var randNum = Math.floor((Math.random() * range) + 1);
  console.log("NUMBER:" + randNum)
		if (randNum == 1 || isBetaPage) {
      console.log("SUCCESS")
			// Ladies and gentelmen, we have a winner!
			redirect = true;
			localStorage.hdcResp = JSON.stringify({"responsiveMode" : true, "dismissReminder" : null});
		}

	if (redirect && !isBetaPage && !isCm) {
		// redirect to beta page
  		window.location.replace("/beta/" + articleId);
	}
}catch(e){}
</script>

      
          	    

          
        	    		
        
                        <script type="text/javascript">
(function(){

	jQuery(document).ready(function() {
			if (!!window.sendRequestToDFP){
				createImageTag("http://www.haaretz.com/st/c/work/AB/abn.gif");
			}
			else
			{
				createImageTag("http://www.haaretz.com/st/c/work/AB/aby.gif");
			}
});

	function createImageTag(src){
		var $img = document.createElement("img");
		$img.setAttribute('src', src);
		$img.setAttribute('height', '1px');
		$img.setAttribute('width', '1px');
		
	}
})();
</script>
      
          	    

          
        	    		
        
