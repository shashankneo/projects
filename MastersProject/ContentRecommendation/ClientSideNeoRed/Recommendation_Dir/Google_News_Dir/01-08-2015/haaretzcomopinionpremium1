<script src="/js/resp/body_scripts/internal/plugins/dialogJq1.js"></script> 

<script>
	function commentThanksDialog(dialogMainId){
		self = this;	
		self.data;
		var jQdialogMain =  $( "#"+dialogMainId );
		var jQdialogForm = jQdialogMain.find("form");
		
		this.init = init;
		this.informUser = informUser;
		this.openMain = openMain;
		this.closeMain = closeMain;
		this.closeMainAndReport = closeMainAndReport;
		
		function init(){
			jQdialogMain.hideDialog();
		
			if(typeof(getUserEmail) == "function"){
				var email = getUserEmail();
				if (email){
					$('#userEmail').val(email);
				}
			}
		}
				
		function openMain(data){
			jQdialogMain.find("input[name='c']").val(data.newCommentId);
			jQdialogMain.find("input[name='h']").val(data.hash);
			
			jQdialogMain.showDialog();
		}
	
		function closeMain(){
			jQdialogMain.hideDialog();
		}

		function closeMainAndReport(){
			_gaq.push(['_trackEvent', 'Article_Comments', 'no_thanks']);
			closeMain();
		}

		/** 
		 * set client email to  when comment is OK :-)
		 */
		function informUser (userEmail){
			//var form = jQdialogMain.find("form");
			var commentId = jQdialogMain.find("input[name='c']").val();
			var commentHash = jQdialogMain.find("input[name='h']").val();
			//alert("userEmail=" +userEmail +", form=" +jQdialogForm.serialize() +", commentId=" +commentId  +", commentHash=" +commentHash)		
		
			var isValid = validateEmail();
			if(isValid){
				_gaq.push(['_trackEvent', 'Article_Comments', 'keep_me_posted']);
				var data = jQdialogForm.serialize();
				console.log(data);
				$.ajax({
					cache : false,
					url : '/comments/updateEmail',
					data: data,
					type: 'POST',
					success : function( data, textStatus, jqXHR ){
						console.log("email update success");			
						commentThanksDialog.closeMain();
					},
					error: function( jqXHR, textStatus, errorThrown ){
						console.log("email update fail");			
						commentThanksDialog.closeMain();
					},
					timeout: 10000
				});
			}else{
				$(".badEmailMsg").show();			
			}
		}
		
		function validateEmail(){
			var isValid = true;	
			//var form = $('#commentSubmitComplete');
			var comment_email = $('#userEmail');
			var emailRegEx = /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i;
			jQdialogForm.find('.form__field').removeClass('form-is-error');
			
			if( ! comment_email.val().trim() ){
				isValid = false;
				var author_error = comment_email.siblings('.form__note');
				author_error.removeClass('h-hidden');
				author_error.text('יש להזין כתובת דוא"\ל');
				author_error.parent().addClass('form-is-error');
			}else{
				if (!((comment_email.val().trim().search(emailRegEx) != -1) && (comment_email.val().trim().length < 64)))
				{
					isValid = false;
					var author_error = comment_email.siblings('.form__note');
					author_error.removeClass('h-hidden');
					
					author_error.text('יש להזין כתובת דוא"\ל תקינה');
					author_error.parent().addClass('form-is-error');
				
				}
			}		
			return isValid;
		}
	}

	$(document).ready(function(){
				commentThanksDialog = new commentThanksDialog("commentsThanksPopupDialog");
		commentThanksDialog.init();
	});

	function getUserEmail(){
		var engSso = getCookie("engsso");
		var email = "";
		if(engSso != null) {
			var usernameStartIndex = engSso.indexOf("userName") +9;
			username = engSso.substring(usernameStartIndex);
			var usernameEndIndex = username.indexOf(":");
			email = username.substring(0,usernameEndIndex);
		}
		return email;
	}
</script> 

