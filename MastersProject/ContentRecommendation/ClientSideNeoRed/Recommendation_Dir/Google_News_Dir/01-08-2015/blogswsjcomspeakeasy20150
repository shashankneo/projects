              var dropdown = dojo.byId("nav-dropdown-1");
              if ( dojo.hasClass(dropdown, "expanded") ) {
                dojo.query(".sp-ddown").removeClass("expanded").addClass("collapsed"); // close all dropdowns
              }
              else {
                dojo.query(".sp-ddown").removeClass("expanded").addClass("collapsed"); // close all dropdowns
                dojo.removeClass(dropdown, "collapsed");
                dojo.addClass(dropdown, "expanded");
              }
              event.stopPropagation(); // so it doesn't bubble up to document click listener below
            });
          

       // so that you can click anywhere and close the dropdown:
       dojo.connect(document, "onclick", function() {
         dojo.query(".sp-ddown").removeClass("expanded").addClass("collapsed");
       }, false);
     

