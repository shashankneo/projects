	'zone':"IvgBVymL1kWKh5HN831U6g",
	'mode':"meter",
	'customPreCheck': "preCheckEdmontonjournal102451",
	'debug':0
}</script><script type="text/javascript">
function preCheckEdmontonjournal102451($)
{
    var metamatch = document.getElementsByTagName("meta");
    for (var i=0; i < metamatch.length; i++) {
    if (metamatch[i].name.match(/(keywords)/i)) {
        if (metamatch[i].content.match(/(breaking|sponsored)/i)) 
          {
           return 0;
          } 	  
	else if (metamatch[i].content.match(/(premium)/i))
	  {
          return 1;
          }
       }
    }

    loc = window.location + '';

	if (loc.match(/\/(cars|jobs|homes|tag|category|author)\//i))
    {
         return 0;
    }
    else if (loc.match(/(story\.html)/i))
    {
         return 1;
    }
    
    return 0;
}
</script>
