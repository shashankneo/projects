<script type="text/javascript">
    $('.show-weather-popup').live("click",function(event){
			$('#weather-lightbox').lightbox_me({
				centered: true,
				closeSelector: ".close",
				overlayCSS: {background: '#fff',
				opacity: 0.8}, 
				closeClick: false,
				onLoad: function() { 
					$('#weather-lightbox').find('input:first').focus()
				}
			});
			event.preventDefault();
		});
</script>

      
  
