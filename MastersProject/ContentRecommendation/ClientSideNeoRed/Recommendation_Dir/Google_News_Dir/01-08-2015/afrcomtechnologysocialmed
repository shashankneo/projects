<!-- FFX analytics stuff -->
<script type="text/javascript">
$(document).on('userProfileReady', function(args) {
    var memberId;
    userProfile = args.userProfile;
    if (userProfile) {
        memberId = userProfile.id; 
    }

    var attributes = [
                        ["event", "view"],
                        ["system", "Methode"],
                        ["site", "afr"],
                        ["assetCategory", "article"],
                        ["asset", "5ebcd589-36fe-40a6-9a54-af4d732718f6"],
                        ["title", "Facebook drone internet in the sky project is a step closer "],
                        ["assetType", "Article"],
                        ["urlHash", window.location.hash],
                        ["pageType", ((typeof digitalData !== "undefined") ? digitalData.page.category.pageType : "")],
                        ["primaryCategory", ((typeof digitalData !== "undefined") ? digitalData.page.category.primaryCategory : "")]
                     ];

    window._fmaq = window._fmaq || [];
    if (memberId) {
        window._fmaq.push(["_setMemberId",memberId]);
    }
    window._fmaq.push(["_trackEvent",attributes]);

    window._fma_options = {};

    var fma_script = document.createElement('script');
    fma_script.src = 'http://resources.fairfax.com.au/js/track/1.0/track.min.js';
    fma_script.setAttribute('async', 'true');
    var s = document.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(fma_script, s);

});

</script>
<!-- Start User Type tracking -->
<!-- This may be required to be called after hagrid call for login-->
<!-- This jsp is a copy from ffx-wcm module. This may be modified for Analytics. So any tweaking of this file will need testing for AFRDESIGN-1070-->
<script type="text/javascript">
    $(document).on('userProfileReady', function(args) {


        function saveUserType(userTypeObj) {

            //Check the existence of the Adobe DTM object.
            if (typeof _satellite != 'undefined') {

                //Call the DTM direct call rule to track AFR analytics
                _satellite.track("apmAfrAnalytics");
            }

            ugcUtil.setCookie('csub', userTypeObj.userTypeString, 30, '/', null, null);
        }

        try {
            hagridWrapper.getUserTypeCallbackEnsured(function(userTypeObj) {
                //if(userTypeObj.userType > 0) {//for member or subscriber

                    var uniqueId = (FFXHagrid.getUserProfile() && FFXHagrid.getUserProfile().unique_id);
                    var hash = 'unknown';

                    if ($.md5 && uniqueId) {
                        hash = $.md5(uniqueId);
                    }

                    for (i=0; i < digitalData.user.length; i++) {
                        for (var idx = 0; idx < digitalData.user[i].profile.length; idx++) {
                            digitalData.user[i].profile[idx].membershipType = userTypeObj.userTypeString;
                            digitalData.user[i].profile[idx].profileInfo.profileID = hash;
                        }
                    }
                    digitalData.titan.adKeyValues.csub = userTypeObj.userTypeString;

                //}

                saveUserType(userTypeObj);
            });
        } catch(e) {
            console.error(e);
        }

    });

</script>


<script type="text/javascript">_satellite.pageBottom();</script>
