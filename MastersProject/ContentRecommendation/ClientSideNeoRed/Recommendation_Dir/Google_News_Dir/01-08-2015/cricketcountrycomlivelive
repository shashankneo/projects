<script type="text/javascript">

  if($("#blinking")){
	window.k=0;
	  function startBlink(){
	    window.blinker = setInterval(function(){
	    	if(window.blink){
	    	   $('.blink').css('color','white');
	    	   $('.blink').parent().css('font-size','12px');
	    	   $('.blink').parent().css('background-color','#000');
	    	   window.blink=false;
	    	 	window.k++;
	    	 	if(window.k%5==0) stopBlink();
	    	 } else{
	    		$('.blink').css('color','#000');
	    		$('.blink').parent().css('font-size','14px');
	    		$('.blink').parent().css('background-color','white');
	    		window.blink = true;
	    	}
	    },500);
	  }

	  function stopBlink(){
	    if(window.blinker) clearInterval(window.blinker);
	  }

	setInterval(function() {
	      startBlink()
	}, 75000);

}
</script>
		
		
