  	<script type="text/javascript">
  		var accordianTab = 1;
		function timedRefreshscroller(timeoutPeriod) {setInterval("scroll(accordianTab)",timeoutPeriod);}
		function scroll(){
			var url = 'http://www.cricketcountry.com/wp_match_list_scroller_content.php?ajax=1';
			$.ajax({
				url: url,
				success: function(data){
					if(data.length>0){
						$('#matchscroller').html(data);	
						//console.log("accordianTab : " + accordianTab );
						if(accordianTab > 0 ){
							telement =  $( '.ccmatching[data="' + accordianTab + '"]');
							telement.siblings('.matching-list').show();
							telement.find('a').addClass('arrow-D');
						}
						initScript();
					}
				},
				
			});
		}	
		timedRefreshscroller( '30000') ;//30000);

		function initScript(){
			//scoreboard
	  		$('.ccmatching').click(function(){

		  		accordianTab = $(this).attr('data');
				//console.log("accordianTab clicked :" + accordianTab );
				$(this).parent().siblings().find('.matching-list').slideUp();
				$(this).parent().siblings().find('.arrow-D').removeClass('arrow-D');
				$(this).siblings('.matching-list').slideToggle();
				$(this).find('a').toggleClass('arrow-D');
				var cntOpenEl =  $('.arrow-D').length;
				//console.log("Open Elements: " + cntOpenEl);
				if(cntOpenEl==0)  accordianTab = 0;
			});	
	  	  	//scoreboard end
  	  	}
  	  	initScript();

	</script>			
