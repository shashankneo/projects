<script src="/htv/static/wro/6.13.5.1/client-common-gpt.js" type="text/javascript"></script>
<script>
    ibsys.gptInit ={};
    ibsys.gptInit.initializeGPT = function(){

        if (ibsys.predicates !== undefined && ibsys.predicates.isOutOfNetwork() ) {
            ibsys.oonPlaceholders(function() {
                ibsys.ad.gpt.init({
                    positionKeyword : 'kw'
                });
            });
        } else {
            ibsys.ad.gpt.init({
                positionKeyword : 'kw'
            });
        }
    };
</script>
<script>
            ibsys.gptInit.initializeGPT();
        </script>
