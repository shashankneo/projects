  evt = e || window.event;
  if (e.target) targ = e.target; 
  else if (e.srcElement) targ = e.srcElement; 
  if (targ.nodeType == 3) 
  // Safari bug 
  targ = targ.parentNode; 
  if (targ.id == "cnbc-contents") { 
  window.open(railAdBgClickthru); 
  } 
  }); 
  jqN('#cnbc-contents').bind('mouseenter',function () { 
  jqN(this).css('cursor','default'); 
  }); 
  jqN('#cnbc-contents').bind('mouseleave',function () { 
  jqN(this).css('cursor','default'); 
  }); 
  }
  });
  // End: DART Rails Handler  
