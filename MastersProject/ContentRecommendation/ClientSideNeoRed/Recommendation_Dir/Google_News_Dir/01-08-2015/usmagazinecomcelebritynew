                /* 
                    a min-height set on the leaderboard_footer div allows Waypoints to accurately determine 
                    when the slot is in-view before firing the ad call.  If no ad is returned, GPT will apply 
                    an inline display:none.
                */
        		var waypointFooterInView = new Waypoint.Inview({
            		element: document.getElementById('leaderboard_footer'),
            		entered: function(direction) {
                        // only trigger ad load when scrolling down
                		if (direction === 'down') {
                    		gptAds.loadLeaderboardFooter();
                		}        		
            		}
        		});
