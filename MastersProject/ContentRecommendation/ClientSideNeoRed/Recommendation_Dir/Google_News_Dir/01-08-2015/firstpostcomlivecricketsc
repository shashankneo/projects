function getAll(matchid,teamshort,innings,currenttab,direction){
var buildurl = '/cricket-live-feed/specials/cricket/json/commentry/'+matchid+'_'+teamshort+'_'+innings+'_fullcommentry.json';
var displaycommentary = '';
jQuery.ajax({
url:buildurl,
dataType: 'json',
beforeSend: function() {
if(direction=='old'){
$('#loadmore_'+currenttab).hide();
$('#loader_'+currenttab).show();
}
},
success:function(data){
var limit = 50;
var start = 50;
var end = 50;
var current_li_count = $("#bbb_comme_"+currenttab).children().length;
console.log("current li count : "+current_li_count);
console.log("Total Commentry Count : "+data['commentry'].length);
if(direction=='latest'){
start = 0;
end = current_li_count;
}else{
start = current_li_count;
if(start<=data['commentry'].length-1){
end = data['commentry'].length-1;
$("#loadmore_"+currenttab).hide();
}else{
end = current_li_count-1+30;
}
}

console.log("Start Count : "+start);
console.log("End Count : "+end);

for (post_cnt = start; post_cnt <end; post_cnt++) {
displaycommentary += '<li class="clearfix">';
if(data['commentry'][post_cnt]['overs']=='' && data['commentry'][post_cnt]['text']!=''){
displaycommentary += '<p class="bbb_ritbox active">'+data['commentry'][post_cnt]['text']+'</p>';
}else{
displaycommentary += '<p class="bbb_lftbox">'+data['commentry'][post_cnt]['overs']+'</p>';
displaycommentary += '<p class="bbb_ritbox">'+data['commentry'][post_cnt]['text']+'</p>';
}
displaycommentary += '</li>';
}
if(direction=='old'){
$('#loader_'+currenttab).hide();
$('#loadmore_'+currenttab).show();
if(start==end){
$('#loadmore_'+currenttab).hide();
}
$(displaycommentary).hide().appendTo('#bbb_comme_'+currenttab).slideDown("slow");
}else{
$('#bbb_comme_'+currenttab).hide();
$('#bbb_comme_'+currenttab).html(displaycommentary);
$('#bbb_comme_'+currenttab).fadeIn('slow');
}
}
});
}
</script><script>

