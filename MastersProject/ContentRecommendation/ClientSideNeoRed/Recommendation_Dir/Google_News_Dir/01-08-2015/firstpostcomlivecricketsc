(function($) {
$('.other_tab').live('click', function(){
var show_tab = $(this).attr('tab');
if (typeof show_tab != 'undefined' && show_tab != '')
{
$('.site_nav li a').each(function( index, value ) {
$(this).removeClass('current').addClass('other_tab');
});
$(this).removeClass('other_tab').addClass('current');
$('.tab_content').css('display', 'none');
$('.tabcommentry').css('display', 'none');
$('#'+show_tab).css('display', 'block');
console.log("tab "+show_tab);
$('#tabcommentry_'+show_tab).css('display', 'block');


}
});
})(jQuery);
