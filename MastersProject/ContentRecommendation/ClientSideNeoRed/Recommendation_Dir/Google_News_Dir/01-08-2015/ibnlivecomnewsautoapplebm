				function moresectionnews(text){
var fileurl;
   if(text=="latest"){
        fileurl="/test_redis.php?t=latestbox_latest";
            }else{
        fileurl="/test_redis.php?t=latestbox_"+text;   
      }
    $.get(fileurl, function(data)
    { 							
          
            $("#latestdata").html(data).hide().fadeIn();
            
         


    }); 

}
 moresectionnews("latest");


$(document).ready(function(){
    $("a.mlink").click(function(e){
      e.preventDefault();
       e.stopPropagation();
      $this = $(this);
      $("div.ln_moreb").slideToggle();
     
    });

 
});



 $(document).click(function(e){
e.stopPropagation();
if($("div.ln_moreb").is(":visible")) {
$("div.ln_moreb").slideToggle();

}

});
