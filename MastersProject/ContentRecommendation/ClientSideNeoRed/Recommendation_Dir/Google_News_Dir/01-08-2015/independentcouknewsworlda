});
$("#pageToolsLargeFont").click(function(event) {
setFont('articleContent', '300%');
$.get("http://www.independent.co.uk/news/world/asia/duma-arson-attack-is-terrorism-says-israels-opposition-leader-10432898.html?pageToolsFontSize=300%25");
event.preventDefault();
});
});
</script>
</div>
</div>
</div>
