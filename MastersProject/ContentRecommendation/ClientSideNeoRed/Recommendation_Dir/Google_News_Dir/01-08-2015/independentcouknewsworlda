<script>
// email newsletter and competition user journey
function indes_gyJourney() {
// define vars
var indes_SourceURL = window.location.href,
id;
// create session storage items with passed variables
function indes_Storage(indes_Type) {
sessionStorage.setItem('indes_SourceURL', indes_SourceURL);
sessionStorage.setItem('indes_Type', indes_Type);
}
// clear storage
function indes_ClearStorage() {
sessionStorage.removeItem('indes_SourceURL');
sessionStorage.removeItem('indes_Type');
return false;
}
// set item to store using the ID of the link, // this then drives the conditional behaviour on // the journey pages.
$('.indes_store').click(function() {
var id = document.querySelector('.indes_store').dataset.id;
indes_Storage(id);
})
$('.competition_btn').click(function() {
var id = 'indes_competition_entry';
indes_Storage(id);
})
};
indes_gyJourney();
</script>
<!-- sponsored features ICN tag placement -->
<script>
var querystring = 'icn=INDsponFeatures';
$('.inpage-widget-6279238 a').each(function()
{
var href = $(this).attr('href');
if(href) {
href += (href.match(/\?/) ? '&' : '?') + querystring;
$(this).attr('href', href);
}
});
</script>
