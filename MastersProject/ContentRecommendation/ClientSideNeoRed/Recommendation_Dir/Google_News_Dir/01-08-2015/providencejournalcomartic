<script type='text/javascript'>
function capLink(cap) {
	var strLength = 83;
	var linkStart = cap.indexOf("<a");
	var linkEnd   = cap.indexOf("</a>");
	if (linkEnd > strLength) {
		var tempCap = cap.slice(linkStart);
		var refEnd  = tempCap.indexOf('">');
		var tempEnd = tempCap.indexOf("</a>");
		// How long is the link text
		var lText = tempEnd - (refEnd + 2);
		if((linkStart + lText) <= strLength) {
			strLength = linkEnd + 4;
		} 
	}
	return strLength;
}
$('.indimgcont_caption_container-inner').each(function(k,v){
	var $this = $(this).find('#cap-move');
	//var strlength = 83;
	var currCaptionDom = $this.prev();
	var currCaption_string = $this.prev().text();
	var strlength = capLink(currCaption_string);
	var currTruncatedString = currCaption_string.substring(0,strlength);
	if (currCaptionDom.html().length > strlength) {
		$this.data('caption', currCaption_string);
		currCaptionDom.html(currTruncatedString + '...');
		$this.text('[+]');
		$this.addClass('expanded');
		currCaptionDom.css('height', '18px');
	} else {
		$this.text('');
	}
});
$(document.body).on('click','#cap-move',function(){
	var $this = $(this);
	//var strlength = 83;
	var currCaptionDom = $this.prev();
	var currCaption_string = $this.prev().html();
	var strlength = capLink(currCaption_string);
	var currTruncatedString = currCaption_string.substring(0,strlength);
	if(!$this.hasClass("expanded")) {
		//replace full caption with truncated caption	
		currCaptionDom.animate({ height: '18px' }, 300, function(){ currCaptionDom.html(currTruncatedString + '...'); });	
		$this.addClass('expanded');
		$this.text('[+]');
	}else{ 
		//replace truncated full caption with caption
		var sSize = $this.data('caption').length;
		var cHeight = (Math.ceil(sSize/parseInt(strlength))*15) + 'px';
		currCaptionDom.html($this.data('caption'));
		currCaptionDom.animate({ height: '+=' + cHeight }, 300);
		$this.removeClass('expanded');
		$this.text('[-]');
	}
});
</script>
