itemPager({indvitem: $('.indimgcont_outter-1'),navcont: $('#rel-img-pagination-container-outter'),prevbtn: $('.rel-img-pagination-container-prev'),nextbtn: $('.rel-img-pagination-container-next'),track: true});
itemPager({indvitem: $('.indimgcont_outter-2'),navcont: $('#rel-gal-pagination-container-outter'),prevbtn: $('.rel-gal-pagination-container-prev'),nextbtn: $('.rel-gal-pagination-container-next'),active_pgr_class: 'gallery_active',track: true});
$('.rel-imgs-mast-outter-container-imgs,.rel-imgs-mast-outter-container-gal').hover(function() {$(this).find('.rel-img-pagination-container').fadeIn( 500 );}, function() {$(this).find('.rel-img-pagination-container').fadeOut( 100 );});
</script>
