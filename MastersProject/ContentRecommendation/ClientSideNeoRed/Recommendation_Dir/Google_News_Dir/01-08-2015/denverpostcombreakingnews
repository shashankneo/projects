                if (commentCountText != '') {
                    clearInterval(CClooper);
                    var commentCountTabText = (commentCountText[0].toLowerCase() == 'add') ? "No comments" : ( (commentCountText[0] == 1) ? commentCountText[0] + ' comment' : commentCountText[0] + ' comments' );
