<script type="text/javascript">
	$('.dpArticleTab').on('click', function(){
		$(this).siblings('li').removeClass('dpActiveTab');
		$(this).addClass('dpActiveTab');
	})
	function dpShowOtherTab(tabclicked,dumptoshow) {
		if(!$(tabclicked).hasClass('dpActiveTab')) {
			$('.dpActiveDump').fadeOut(100).removeClass('dpActiveDump');
			$(dumptoshow).fadeIn(100).addClass('dpActiveDump');
		}
	}
	function dpShowOtherTabTop(fullHash) {
		if(!$('#dpArticleCommentingDump').hasClass('dpActiveDump'))	{
			$('#dpArticleRelatedDump').fadeOut(100).removeClass('dpActiveDump');
			$('#dpArticleCommentingDump').fadeIn(100).addClass('dpActiveDump');
		}
		if(!$('#dpArticleCommentsTab').hasClass('dpActiveTab')) {
			$('#dpArticleCommentsTab').siblings('li').removeClass('dpActiveTab');
			$('#dpArticleCommentsTab').addClass('dpActiveTab');
		}
		if (fulllHash == '#disqus_thread') {
			$('html, body').animate({ scrollTop: $("#dpArticleBottom").offset().top }, 30);
		} else {
			$('html, body').animate({ scrollTop: $(fullHash).offset().top }, 30);
		}
	}
	$(document).ready( function() {
		var fullHash = window.location.hash;
		var partialHash = '';
		if (fullHash != '#disqus_thread') {
			partialHash = fullHash.split('-');
		}
		if (fullHash == '#disqus_thread' || partialHash[0] == '#comment') {
			dpShowOtherTabTop(fullHash);
		}
	});
</script><!-- END FREEFORM RENDER, ID 23767546 --></td></tr><tr><td valign="top"><!-- BEGIN FREEFORM RENDER, ID null --><!-- FREEFORM RENDER, ID null NOT FOUND --><!-- END FREEFORM RENDER, ID null --></td></tr></table></div></div><div class="clear"></div><div id="region5" class="region5"><!--IE6 fix--><table border="0" width="100%" cellpadding="0" cellspacing="0"><tr><td valign="top"><!-- BEGIN FREEFORM RENDER, ID null --><!-- FREEFORM RENDER, ID null NOT FOUND --><!-- END FREEFORM RENDER, ID null --></td></tr></table></div><div class="clear"></div><div id="region8and9box" class="region8and9box"><div id="region8" class="region8"><!--IE6 fix--><table border="0" width="100%" cellpadding="0" cellspacing="0"><tr><td valign="top"><!-- BEGIN FREEFORM RENDER, ID null --><!-- FREEFORM RENDER, ID null NOT FOUND --><!-- END FREEFORM RENDER, ID null --></td></tr></table></div><div id="region9" class="region9"><!--IE6 fix--><table border="0" width="100%" cellpadding="0" cellspacing="0"><tr><td valign="top"><!-- BEGIN FREEFORM RENDER, ID null --><!-- FREEFORM RENDER, ID null NOT FOUND --><!-- END FREEFORM RENDER, ID null --></td></tr></table></div></div><div class="clear"></div><div id="region10" class="region10"><!--IE6 fix--><table border="0" width="100%" cellpadding="0" cellspacing="0"><tr><td valign="top"><!-- BEGIN COMPLEX LISTING PORTLET [ID = 3945818] --><script language="JavaScript" type="text/javascript">
