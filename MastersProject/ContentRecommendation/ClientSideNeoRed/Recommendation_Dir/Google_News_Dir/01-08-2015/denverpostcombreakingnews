            var pop_section = 'sidebarmostpop';

            article_pop_dp = setInterval(function(){
                if ( typeof dfmNav.pData !== 'undefined' && typeof dfmNav.pData.pop_config !== 'undefined' ) 
                {
                    var dp_pop_id = '#sidebarmostpop';
                    var dp_pop_num = 8;
                    var dp_pop_tab = '';
                    if ( is_it_home() == true ) {
                        dp_pop_num = 6;
                    }
                    if ( is_it_article() == true ) {
                        dp_pop_num = 8;
                        if (dppopSections['section'[1]]) {
                            dp_pop_tab = dppopSections['section'[1]];
                        }
                    }
                    if ( dfm.api('data','pageType') == 'section' ) {
                        dp_pop_num = 6;
                        if (dppopSections['section'[1]]) {
                            dp_pop_tab = dppopSections['section'[1]];
                        }
                    }
                    dfmNav.pData.pop_config.id = dp_pop_id;
                    dfmNav.pData.pop_config.loadthistab = dp_pop_tab;
                    dfmNav.pData.pop_config.quantity = dp_pop_num;
                    //console.log(dfmNav.pData.pop_config);
                    window.clearInterval(article_pop_dp);
                }
            } ,1000);

            </script>

        </div>
