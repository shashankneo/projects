<script type="text/javascript">

var mlbfantasyliveshow = {
    _name : 'mlbfantasyliveshow',
    _timer : null,
    _embed : '',
    _keepModalState : false,
    init : function(config) {

        var my = this;

        // set up the panel toggle
        $('body').delegate('.'+this._name+'Link', 'click', function() {
            if (my._dialog.is(':visible')) { my.close(); }
            else { my.open(); }
        });

        // set up dialog cnt
        this._dialog = $('#'+this._name+'Cnt');

        // start config updater
        this.update(config);
    },
    getConfig : function() {
        //console.log('getConfig: video/player/'+this._name+'/defaults');
        $.ajax({
            url : 'http://www.cbssports.com/data/video/player/get-dat-file',
            context : this,
            data : {
                as : 'json',
                dat_file_path : 'video/player/'+this._name+'/defaults'
            },
            success : this.update,
            dataType : 'json'
        });

    },
    update : function(config) {
        config = config.content;
        //console.log('update: '+JSON.stringify(config));

        // disable if game abbr doesnt match
        if (config.PREVIEW_GAME_ABBR != 'MLB_20150802_TB@BOS' && config.PREVIEW_GAME_ABBR != 'all') {
            config.SHOW_ON_PREVIEW = false;
        }

        // reset updater and return if data has not changed or if game abbr doesnt match
        if (typeof JSON != 'undefined' && typeof this._config != 'undefined' && JSON.stringify(config) == JSON.stringify(this._config)) {
            //console.log('No change in data.');
            if (this._timer) { clearTimeout(this._timer); }
            this._timer = setTimeout(this._name+'.getConfig()', parseInt(config.DEFAULTS_REFRESH)*1000);
            return;
        }

        // we need to track whether enabled has changed so we dont re-open
        // panel if the enabled state hasn't changed
        var lastEnabled = this._enabled;

        // set config
        this._config = config;
        this._enabled = (this._config.SHOW_ON_PREVIEW == 'true');
        this._keepModalState = (typeof this._config.KEEP_MODAL_STATE != 'undefined') ? (this._config.KEEP_MODAL_STATE == 'true') : this._keepModalState;
        this._refresh = parseInt(this._config.DEFAULTS_REFRESH);
        this._title = this._config.SHOW_TITLE;
        this._video = {
            width : parseInt(this._config.VIDEO_WIDTH),
            height : parseInt(this._config.VIDEO_HEIGHT),
            uvpc : this._config.SHOW_UVPC,
            dynamicControls : (this._config.VIDEO_CONTROLS == 'dynamic'),
            pcid : this._config.SHOW_PCID
        };
        this._video.embed = '<div id="embedVideoContainer_'+this._name+'" class="embedVideo">'
                          + '    <iframe id="embedVideo_'+this._name+'" name="embedVideo_'+this._name+'" src="http://www.cbssports.com/video/player/embed?args=player_id%3D'+this._name+'%26pcid%3D'+this._video.pcid+'%26width%3D'+this._video.width+'%26height%3D'+this._video.height+'%26autoplay%3Dtrue%26enable_end_card%3Dfalse%26continuous_play%3Dfalse%26dynamic_controls%3D'+this._video.dynamicControls+'%26comp_ads_enabled%3Dfalse%26comp_ad_w%3D300%26comp_ad_h%3D60%26autoplay_live%3Dtrue%26resizable%3Dtrue" width="'+this._video.width+'" height="'+this._video.height+'" marginwidth="0" marginheight="0" scrolling="no" frameborder="0"></iframe>'
                          + '</div><div class="closeBtn">CLOSE</div>';
        this._videoOpenLink = $('<div class="'+this._name+'Link p">'+this._title+'</div>');
        this._videoOpenLinkSelector = this._config.P_LINK_SELECTOR;
        this._videoOpenLinkHeight = $('.'+this._name+'Link').height();

        // if video is enabled
        if (this._enabled && this._enabled != lastEnabled) {
            // nuke the embed if it's there
            this._embed = ($('#embedVideoContainer_gtpreview').length) ? $('#embedVideoContainer_gtpreview')[0].outerHTML : '';
            $('#embedVideoContainer_gtpreview').remove();

            // add video link if it's not there
            if ($('.'+this._name+'Link').length == 0) {
                $(this._videoOpenLinkSelector).before(this._videoOpenLink);
            }

            // if the modal hasn't been opened
            if (typeof this._modalIsOpen == 'undefined') {
                // if using stored modal state, open based on that
                if (this._keepModalState) {
                    if (readCookie(this._name+'ModalStateP') == 'false') {
                        this.close();
                    }
                    else {
                        this.open();
                    }
                } // otherwise open the modal
                else {
                    this.open();
                }
            }
        }
        else if (this._enabled != lastEnabled) {
            // replace the link with the stored embed code if there is some
            $('.'+this._name+'Link').remove();
            this._dialog.after(this._embed);

            this.close();

            // and reset modal state
            delete this._modalIsOpen;
        }

        // reset updater
        if (this._timer) { clearTimeout(this._timer); }
        this._timer = setTimeout(this._name+'.getConfig()', this._refresh*1000);
    },
    open : function() {
        //console.log('open: '+this._title);

        if (this._dialog.is(':visible')) { return; }

        // open panel
        this._dialog.html(this._video.embed);
        this._dialog.slideDown();

        // set up close btn
        var my = this;
        this._dialog.find('.closeBtn').click( function() {
            my.close();
        });

        // store state
        this._modalIsOpen = true;
        document.cookie = this._name+'ModalStateP=true; domain=.cbssports.com; path=/';

    },
    close : function() {
        //console.log('close: '+this._title);

        if (!this._dialog.is(':visible')) { return; }

        this._dialog.slideUp();
        this._dialog.html('');

        // store state
        this._modalIsOpen = false;
        document.cookie = this._name+'ModalStateP=false; domain=.cbssports.com; path=/';
    }
};

$(function() {
    mlbfantasyliveshow.init({"content":{"SHOW_UVPC":"","VIDEO_WIDTH":"640","SHOW_TITLE":"<span class=\"linkText\">CBS Sports MLB Channel</span>","SB_LINK_SELECTOR":"#mlbfantasyliveshowCnt","SHOW_ON_GT":"false","P_LINK_SELECTOR":"#mlbfantasyliveshowCnt","VIDEO_HEIGHT":"360","USE_STATUS_EVENTS":"false","CSS_FILE":"style.html","PREVIEW_GAME_ABBR":"","KEEP_MODAL_STATE":"true","VIDEO_CONTROLS":"dynamic","SHOW_ON_PREVIEW":"false","DEFAULTS_REFRESH":"123","SHOW_ON_SCOREBOARD":"true","GT_LINK_SELECTOR":".mlbStage","SHOW_PCID":"495597123793","TZ_OFFSET":"4","MODAL_POSITION":"center"}});
});

</script>

