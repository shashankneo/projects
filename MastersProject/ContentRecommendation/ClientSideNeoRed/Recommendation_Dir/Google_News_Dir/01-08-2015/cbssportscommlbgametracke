                $('head').append(style);
                $('#getScreenName').show();
                var screenNameContent = '<a href="http://www.cbssports.com/registration/validate/send?master_product=2580&userid=Vb2S3wq0GDAAAExcbro&email=&end=http%3A%2F%2Fwww.cbssports.com%2Fregistration%3Fmaster_product%3D2580%26end%3Dhttp%253A%252F%252Fwww.cbssports.com%252Fmlb%252Fgametracker%252Fpreview%252FMLB_20150802_TB%2540BOS%252Frays-red-sox-preview%253FprofFR%253D1">Get a Screen Name to Comment</a>';
                $('#getScreenName').html(screenNameContent);
            } else if (userObj.profile.valid_email_flag != '1') {
                $('.fyre .fyre-user-drop').hide();
                var style = "<style>.goog-inline-block.fyre-button-right-outer-box{display:none}</style>";
                $('head').append(style);
                $('#getScreenName').show();
                var screenNameContent = '<a href="http://www.cbssports.com/registration/validate/send?master_product=2580&userid=Vb2S3wq0GDAAAExcbro&email=&end=http%3A%2F%2Fwww.cbssports.com%2Fregistration%3Fmaster_product%3D2580%26end%3Dhttp%253A%252F%252Fwww.cbssports.com%252Fmlb%252Fgametracker%252Fpreview%252FMLB_20150802_TB%2540BOS%252Frays-red-sox-preview%253FprofFR%253D1">Validate your email to Comment</a>';
                $('#getScreenName').html(screenNameContent);
            }
            /*
