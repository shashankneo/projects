    <meta itemprop="thumbnailUrl image" content="http://media1.s-nbcnews.com/j/newscms/2015_31/1145401/150729-mullah-omar-0528_7cf56b71f4384dc16560411abfb5a394.nbcnews-fp-1200-800.jpg" />
