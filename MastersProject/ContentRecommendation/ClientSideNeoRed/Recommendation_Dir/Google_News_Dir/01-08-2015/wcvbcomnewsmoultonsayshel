        ibsys.userProfile.userProfileInit.sitekey = 'bos';
        ibsys.userProfile.userProfileInit.siteName = 'WCVB';
        ibsys.userProfile.userProfileInit.offersEndpoint = '/servlet/profile/sites/htv-bos/offers';
        ibsys.userProfile.userProfileInit.subscriptionsEndpoint = '/servlet/profile/user-scope/sites/bos';
        ibsys.userProfile.userProfileInit.ibOauthUrl = 'https://id.ib-prod.com/oauth-provider';
        console.debug('init userProfile');
        ibsys.vm.userProfile = new ibsys.userProfile.UserProfile();
    </script>
