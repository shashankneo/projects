        <div class="newsletterSignupBox">
            <div class="nsBox">
                <h3 class="rightrail title"><a href="/newsletters">NEWSLETTERS</a></h3><a href="/newsletters"><span style="display:block" class="icon envelope"></span></a>
                <div class="blackdivider1"></div>
                <h3 class="newsletter copy">Receive the latest local  updates in your inbox</h3>
                <form method="post" id="newsletterBox-0" class="nsForm" name="ns-subscribe-form" class="validate" target="_blank" data-nsHandlerKey = "2">
                    <fieldset>
                        <input class="required nsEmail" name="EMAIL" type="text" value="" onfocus="nsu.clearSignup(this)" />
                        <input class="nsSubscribe" name="subscribe" type="submit" value="SIGN UP" />
                    </fieldset>
                </form>
            </div>
            <div class="nsFooter"><a href="http://www.nbcnewyork.com/privacy/" name="&lpos=Newsletter Sign Up&lid=Privacy Policy">Privacy Policy</a> | <a href="http://www.nbcnewyork.com/newsletters/" name="&lpos=Newsletter Sign Up&lid=More Newsletters">More Newsletters</a></div>
        </div>
        <script type="text/javascript">
            jQuery(document).ready(function() {
                
                nbc.validateEmail = function(submittedAddr, currentArticle) {
                    if( (submittedAddr.length > 0) && (submittedAddr != "Email") ) {
                        var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
                        return emailReg.test(submittedAddr);
                    } else {
                        jQuery('#newsletterBox-'+ currentArticle + ' .nsEmail').val("Invalid email address");
                        return false;
                    }
                }

                nsu = {};
                nsu.hasOmnitureFired = false;
            
                nsu.clearSignup = function(dasEle){
                    var str=dasEle.value;
                    var clearId = jQuery(dasEle).parent().parent().attr('id');
                    if(str.toLowerCase() == "email" || str.toLowerCase() == "invalid email address" || str.toLowerCase() == "there was an error subscribing"){
                        dasEle.value="";
                        $('#' + clearId + " .nsEmail").css({"color":"#333"});
                    }
                }
            
                nsu.signUserUp = function(email, nsHandlerKey, currentArticle) {
                    var isAddressValid = nbc.validateEmail(email, currentArticle);
                    thisNewsletterID = nsHandlerKey;
                    if(isAddressValid != true) {
                        return false;
                    } else {
                        nsu.subscribeUser(email, nsHandlerKey, currentArticle);
                    }
                }
            
                
                nsu.fireOmniValues = function() {
                    nbcu.un = nbcu_account;
                    nbcu.prop7 = 'News Newsletter Sign Up';
                    nbcu.eVar7 = 'News Newsletter Sign Up';
                    nbcu.events = 'event19';
                    nbcu.linkTrackVars = 'prop7,eVar7,events';
                    nbcu.linkTrackEvents = 'event19';
                    U.log(nbcu.prop7 + ' | ' + nbcu.eVar7 + ' | ' + nbcu.events + ' | ' + nbcu.linkTrackVars);
                    try {
                    nbcu.tl(true,'o','News Newsletter Sign Up Complete');
                    }
                    
                    catch(e) {
                    U.log(e);
                    }
                    nsu.hasOmnitureFired = true;
                    return false;
                }
            
                nsu.subscribeUser = function(email, nsHandlerKey, currentArticle) {
                    var ajaxUrl = "/i/dispatcher/?h=newsletter&subscribe=yes&email="+email+"&subscribeIds=" + nsHandlerKey;
                    jQuery.ajax({
                        type: 'POST',
                        url: ajaxUrl,
                        dataType: 'json',
                        success: function(data) {
                            if(data.status == 200) {
                                nsu.fireOmniValues();
                                jQuery('#newsletterBox-'+ currentArticle + '.nsForm').html('<span class="nsConfirm">Thank you for signing up!</span>');
                            } else {
                                if(nbc.env == "dev."){console.log(data.detailResponse)};
                                jQuery('#newsletterBox-'+ currentArticle + ' .nsEmail').val("Invalid email address");
                                jQuery('#newsletterBox-'+ currentArticle + ' .nsEmail').css({"color":"red","text-transform":"none"});
                            }
                        },
                        error: function(data) {
                            if(nbc.env == "dev."){console.log(data.detailResponse)};
                            jQuery('#newsletterBox-'+ currentArticle + ' .nsEmail').val("There was an error subscribing");
                            jQuery('#newsletterBox-'+ currentArticle + ' .nsEmail').css({"color":"red","text-transform":"none"});
                        }
                    });
                }
                
                var currentArticle = 0;
                var nsHandlerKey = "";
                jQuery('#newsletterBox-'+ currentArticle + '.nsForm' ).submit(function() {
                    nsHandlerKey = jQuery(this).attr('data-nsHandlerKey');
                    capturedEmail = jQuery('#newsletterBox-'+ currentArticle + ' .nsEmail').val();
                    nsu.signUserUp(capturedEmail, nsHandlerKey, currentArticle);
                    return false;
                });
            });    
                
        </script>
            
