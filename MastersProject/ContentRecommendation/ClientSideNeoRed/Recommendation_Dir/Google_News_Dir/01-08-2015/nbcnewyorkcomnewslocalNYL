            <article id="article_0_elements">
            <div id="article_0_headline_byline" class="headline_region">
                                
                
                
                                
                <h1 class="headline" itemprop="headline">Rip Tide Pulls Swimmer out to Sea; His Condition Is Unknown Following Rescue</h1>
                <h2 class="subtitle"></h2>
                                <!--endclickprintinclude-->
