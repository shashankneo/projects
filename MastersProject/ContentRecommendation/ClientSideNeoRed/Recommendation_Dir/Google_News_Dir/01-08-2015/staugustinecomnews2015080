  <script type="text/javascript">
$(document).ready(function () {
	   

	var ht = $(document).height();	
	$(document).scroll(function () {
		
		var y = $(this).scrollTop();
		if (y > 200 && y < (ht-1000))
		{
			
			$(' .key-nav, #arrow-left-shadow, #arrow-right-shadow ').fadeIn();
			$('.dpSocialBar_container').hide();
		} 
		else
		{
			
			$(' .key-nav, #arrow-left-shadow, #arrow-right-shadow ').fadeOut();
			$('.dpSocialBar_container').show();
		}
	}); 
 
    $(document).keydown(function(e) {
        var url = false;
        if (e.which == 37) {  // Left arrow key code


            url = $('#page-navigation-pre a').attr('href');
      
        }
        else if (e.which == 39) {  // Right arrow key code


            url = $('#page-navigation-next a').attr('href');
          
        }
        if (url) {
           window.location = url;
        }      
               
    });
    
    $('.key-nav').click(function(){
		  
		  window.location = $(this).find('a').attr('href');
		  
		  });
		  
   
 
});
</script>
  
  
  
  
  
  
  
  
  
  
  
  



