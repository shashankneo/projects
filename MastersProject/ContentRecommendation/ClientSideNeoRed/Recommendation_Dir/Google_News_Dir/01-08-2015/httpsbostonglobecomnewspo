<script type="text/javascript">
var aam_optimizely = function(data) {
    var json = JSON.stringify(data, undefined, 2),
        obj = JSON && JSON.parse(json) || window.jQuery.parseJSON(json);

    for (var i=0; i < obj.stuff.length; i++) {
        var current = obj.stuff[i];

        if(current.cn == "aam_optimizely") {
            var audiences = current.cv;
            if(audiences) {
                window['optimizely'] = window['optimizely'] || [];
                var segArray = audiences.split('aam=')[1].
                    split(',');

                for(var j=0; j < segArray.length; j++) {
                    window['optimizely'].push(['addToSegment', segArray[j]]);
                    window['optimizely'].push(['trackEvent', window.location.href]);
                }
            }
        }
    }
}
var getAudiences = function() {
    var url = 'http://bostonglobe.demdex.net/event?d_stuff=1&d_dst=1&d_rtbd=json&d_cb=aam_optimizely';
    var tempScriptTag = document.createElement('script');

    tempScriptTag.type = 'text/javascript';
    tempScriptTag.src = url; document.head.appendChild(tempScriptTag);
}
