                CNBC_Utils.execute('chartbeat most popular', function(){
			var refreshInterval = 300;
			function renderModule() { 
				$.ajax( { 
					url: 'http://www.cnbc.com/id/102535788/services/chartbeat', 
					success: function(data) { 
						var result = $.trim($(data).text());
						if(!result) { 
							return false;
						} ;
						if( $.trim($('#chartbeatWidget_102535788').text()) != result ) { 
							$('#chartbeatWidget_102535788').html(data);
						} ;						
					}  
				} );
			} ;
			renderModule();
			if(refreshInterval) {
				window.setInterval(renderModule, refreshInterval*1000 );
			};			
		}, 'chartbeatWidget_102535788');
