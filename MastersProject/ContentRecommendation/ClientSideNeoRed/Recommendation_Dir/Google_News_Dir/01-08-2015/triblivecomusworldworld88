					$( document ).ready(function() {
                        
                       $( "#tags" ).autocomplete({
                         source: function( request, response ) {
                            $.ajax({
                              type: "POST",       
                              url: "http://search.tribweb.com/suggest?site=default_collection&client=triblive&access=p&format=rich&",
                              dataType: "jsonp",
                              data: {
                                       q: request.term
                               },
                               success: function( data ) {
                                       response($.map(data.results, function(v){
                                                                                                   return {label: v.name};
                                          }));
                                    }
                               });
                             },
                             minLength: 1,
                                                                   
