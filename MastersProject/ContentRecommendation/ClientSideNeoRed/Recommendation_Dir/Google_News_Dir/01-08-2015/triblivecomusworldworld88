<script type="text/javascript">
function resizeText(multiplier) {
	parent_elem = document.getElementById("storyText");
	child_elements=parent_elem.getElementsByTagName("p");
	var currentSize = parent_elem.style.fontSize || 1;
	
	for(i=0;i<child_elements.length;i++)
	{
		parent_elem.style.fontSize = ( parseFloat(currentSize) + (multiplier * 0.2)) + "em";
		child_elements[i].style.fontSize=( parseFloat(currentSize) + (multiplier * 0.2)) + "em";
	}
}
</script>

	<a id="largetext" onclick="resizeText(1)">
		<img alt="Larger text" src="/csp/mediapool/sites/TribLIVE/assets/img/icons/style_add.png"> Larger text
	</a> 
	<a id="smalltext" onclick="resizeText(-1)">
		<img alt="Smaller text" src="/csp/mediapool/sites/TribLIVE/assets/img/icons/style_delete.png"> Smaller text
