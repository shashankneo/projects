                $j(document).ready(function() {
                                $j(".tab_content_eTribTabs").hide();
                                
                                $j("ul.eTribTabs li").click(function() {
                                                $j("ul.eTribTabs li").removeClass("active_eTribTabs");
                                                $j(this).addClass("active_eTribTabs");
                                                $j(".tab_content_eTribTabs").hide();
                                                $j('#eTribBottomIndexWrapper').css('height', '325px');
                                                var activeTab = $j(this).find("a").attr("href");
                                                if ($j.browser.msie) {$j(activeTab).show();}
                                                else {$j(activeTab).show();}
                                                return false;
                                });
                                
