dfmNav.initParams('pageTitle|' + self.SectionName,'mode|' + dfm.theMode,'site|' + tmp_domain);
//dfm.$('#dfmPageTitle').html(self.SectionName);
dfm.$(document).ready(function(){
	dfm.$('div#dfmFooterPlaceholder').after(dfm.$('div#dfmFooter').show()).remove();
});
</script>

