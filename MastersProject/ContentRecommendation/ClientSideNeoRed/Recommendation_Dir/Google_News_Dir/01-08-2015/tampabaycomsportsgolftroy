			var gigyaStreamId = gigyaArticleId + ":article:tampabay.com";
			var commentThreadID = gigyaStreamId;
			//So old Escenic articles work with Gigya.
			if (articleSource == 'Escenic') {
				var articleURL = "http://www.tampabay.com/"+"sports/golf/"+"troy-merritt-shoots-61-shares-quicken-loans-national-lead-as-tiger-fades/"+gigyaArticleId;
			} else {
				var articleURL = "http://www.tampabay.com"+"/sports/golf/troy-merritt-shoots-61-shares-quicken-loans-national-lead-as-tiger-fades/2239663";
			}

			loadGigyaStreamInfo(gigyaStreamId, gigyaArticleId);

			var myReactions=[
				{
					text: 'Important',
					ID: 'important',
					iconImgUp:'/iwov-resources/images/social/Important.png',
					iconImgOver:'/iwov-resources/images/social/Important-over.png',
					tooltip:'This item is important',
					feedMessage: 'This is important',
					headerText:'You think this post is important,'
				}
				,{
					text: 'Inspiring',
					ID: 'inspiring',
					iconImgUp:'/iwov-resources/images/social/Inspiring.png',
					iconImgOver:'/iwov-resources/images/social/Inspiring-over.png',
					tooltip:'This item is inspiring',
					feedMessage: 'Inspiring!',
					headerText:'You think this post is inspiring,'
				}
				,{
					text: 'Sad',
					ID: 'sad',
					iconImgUp:'/iwov-resources/images/social/Sad.png',
					iconImgOver:'/iwov-resources/images/social/Sad-over.png',
					tooltip:'This item is sad',
					feedMessage: 'This is sad',
					headerText:'You think this post is sad,'
				}
				,{
					text: 'Angry',
					ID: 'angry',
					iconImgUp:'/iwov-resources/images/social/Angry.png',
					iconImgOver:'/iwov-resources/images/social/Angry-over.png',
					tooltip:'This item makes me angry',
					feedMessage: 'This makes my angry!',
					headerText:'You think this post is maddening,'
				}
				,{
					text: 'Happy',
					ID: 'lol',
					iconImgUp:'/iwov-resources/images/social/LOL.png',
					iconImgOver:'/iwov-resources/images/social/LOL-over.png',
					tooltip:'This item makes me happy',
					feedMessage: 'This makes me happy!',
					headerText:'This post made you Happy,'
				}
			]

			var socialUserAction = new gigya.socialize.UserAction();
			socialUserAction.setLinkBack(articleURL);
			socialUserAction.setTitle(articleHeadline);

            //-----Deal with change in Gigya API that requires photo be explicitly set-----:
            var image = {
                type: 'image',
                src: articleImage,
                href: articleURL
	        }

            socialUserAction.addMediaItem(image);
            //----------

			var showReactionsBarUI_params= {
				barID: gigyaBarId,
				showCounts: 'top',
				containerID: 'gigyaReactionsBarButtons',
				reactions: myReactions,
				userAction: socialUserAction,
				showSuccessMessage: true,
				noButtonBorders: false
			}

            function updateShareBarUi_params(factor){

			var showShareBarUI_params = {
				userAction:socialUserAction,
                iconsOnly: 'true',
				shareButtons:
					[
						{
							provider:'facebook-like',
							tooltip:'Recommend this on Facebook',
							action:'Like'
						},
						{
							provider:'facebook'
						},
						{
							provider:'twitter-tweet',
							related:'PolitiFact,PoliticsTBTimes',
							via:'TB_Times',
							defaultText: articleHeadline
						},
						{
							provider:'email'
						},
						{
							provider:'share'
						}
					],
				moreEnabledProviders:'facebookshare, twitter, stumbleupon, tumblr, pinterest, linkedin, digg, fark, reddit, delicious, gmail, messenger, googlebookmarks, livebookmarks, myaol, myspace, hyves, baidu, orkut, skyrock, qq, sina, kaixin, renren, vznet, friendfeed, vkontakte, formspring, box.net, plaxo, technorati, slashdot, faves, newsvine, bit.ly, misterwong, amazon, netlog, evernote, aolmail, propeller, currenttv, simpy, yardbarker, blinklist, diigo, sphinn, backflip, dropjack, segnalo, linkagogo, kaboodle, skimbit',
                containerID: 'gigyaShareBarButtons'+factor,
				cid: gigyaBarId,
                useEmailCaptcha: true,
                facebookDialogType: 'share'
			};

            return showShareBarUI_params;

            }


			gigya.socialize.showShareBarUI({},updateShareBarUi_params(''));
			gigya.socialize.showShareBarUI({},updateShareBarUi_params('2'));
			gigya.socialize.showReactionsBarUI({},showReactionsBarUI_params);

			var commentParams ={
				categoryID: "tpc_comments",
				streamID: commentThreadID,
				containerID: "commentsDiv",
				useSiteLogin: true,
				onSiteLoginClicked: tpcc_onSiteLoginHandler,
				onLoad: tpcc_OnGigyaCommentLoad,
				onCommentSubmitted: tpcc_OnGigyaCommentSubmitted,
				cid:"",
				version: 2
				}
		</script>
<!--ls:end[component-1409038442555]--><!--ls:begin[component-1372836881350]--><div>
