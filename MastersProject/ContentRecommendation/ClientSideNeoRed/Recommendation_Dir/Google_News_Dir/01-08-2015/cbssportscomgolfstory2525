    var lf_config = [{"collectionMeta":"eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ0aXRsZSI6IlBlcnJ5IHNob290cyA2MSwgbGVhZHMgM00gQ2hhbXBpb25zaGlwIiwidXJsIjoiaHR0cDovL3d3dy5jYnNzcG9ydHMuY29tL2dvbGYvc3RvcnkvMjUyNTY5MjEvcGVycnktc2hvb3RzLTYxLWxlYWRzLTNtLWNoYW1waW9uc2hpcCIsInRhZ3MiOiIiLCJjaGVja3N1bSI6ImZhZWFmYmRkZTU5ZmI4OWE1ZjMyNjkxNjc3OTJiZmU1IiwiYXJ0aWNsZUlkIjoiYzoyNTI1NjkyMSJ9.jMLUSWMBbHGycglK4dq20bUimejZtJtYC9IyWKI2p2s",
