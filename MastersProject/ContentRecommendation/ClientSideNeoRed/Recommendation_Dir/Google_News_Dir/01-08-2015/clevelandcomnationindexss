                  <script type="text/javascript">
    Advance.Gigya.settings.page = {
            'title':'Northern California wildfire claims firefighter\'s life',
            'link':'http://www.cleveland.com/nation/index.ssf/2015/08/northern_california_wildfire_c.html',
